{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n\t\t", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "S. 2933"}, "official-title": {"#tail": "\n\t", "#text": "To prohibit the unauthorized use of electronic tracking devices."}, "session": {"#tail": "\n\t\t", "#text": "2d Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n\t\t\t", "@date": "20141117", "#text": "November 17, 2014"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "sponsor": {"#tail": " introduced the following bill; which was read twice and referred to the ", "#text": "Mr. Schumer", "@name-id": "S270"}, "committee-name": {"#tail": "\n      ", "@committee-id": "SSJU00", "#text": "Committee on the Judiciary"}}, "#text": "\n\t\t\t"}, "legis-type": {"#tail": "\n\t\t", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@dms-id": "A1", "@public-private": "public", "legis-body": {"#tail": "\n", "section": {"@section-type": "section-one", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Prohibition on unauthorized use of electronic tracking devices"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", is amended by inserting after ", "@entity-type": "uscode", "#text": "Part 1 of title 18, United States Code", "@value": "usc/18"}, {"#tail": " the following:", "@entity-type": "uscode", "#text": "chapter 119", "@value": "usc-chapter/18/119"}], "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "quoted-block": {"chapter": {"@style": "OLC", "#tail": "\n          ", "section": {"#tail": "\n          ", "enum": {"#tail": "\n              ", "#text": "2601."}, "header": {"#tail": "\n              ", "#text": "Prohibition on unauthorized electronic tracking"}, "subsection": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": " means a device attached to a motor vehicle or other vehicle that reveals the location or movement\n\t\t\t of the device through the transmission of electronic\n\t\t\t signals.", "#text": "electronic tracking device"}, "#text": "In this section, the term "}, "enum": {"#tail": "\n                ", "#text": "(a)"}, "header": {"#tail": "\n                ", "#text": "Definition"}, "#text": "\n                ", "@id": "id3AF5631826BA4E60A53B52C4A25C0B35"}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "Except as provided in subsection (c), it shall be unlawful to knowingly use an electronic tracking\n\t\t\t device\u2014"}, "enum": {"#tail": "\n                ", "#text": "(b)"}, "header": {"#tail": "\n                ", "#text": "Prohibition"}, "paragraph": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "to determine the location or movement of an individual; and"}, "enum": {"#tail": "\n                  ", "#text": "(1)"}, "@id": "id65FF35B333B74828915EC03D785C365E", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "with the intent to place the individual under surveillance without the consent of the individual."}, "enum": {"#tail": "\n                  ", "#text": "(2)"}, "@id": "id87B5FF4557E342E888D7EA91F62F69ED", "#text": "\n                  "}], "#text": "\n                ", "@id": "idD0A20CDB96144ADA9B2EB410E41FD405"}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "The prohibition under subsection (b) shall not apply to the use of an electronic tracking device\u2014"}, "enum": {"#tail": "\n                ", "#text": "(c)"}, "header": {"#tail": "\n                ", "#text": "Exceptions"}, "paragraph": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "on a motor vehicle or other vehicle if the registered owner, lessor, or lessee of the vehicle has\n\t\t\t consented to\n\t\t\t the use of the electronic tracking device on the vehicle;"}, "enum": {"#tail": "\n                  ", "#text": "(1)"}, "@id": "id097FBA92DB284459A68648C209F1DE42", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "by a law enforcement agency if the use is not otherwise prohibited by law;"}, "enum": {"#tail": "\n                  ", "#text": "(2)"}, "@id": "id3B827BACC5F84AD6AA27BD5D3984A826", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "by an individual to track a dependent of the individual; or"}, "enum": {"#tail": "\n                  ", "#text": "(3)"}, "@id": "id6627FF45684C40A5947B5AF05CF58BB9", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "to reasonably protect the health or safety of the individual being tracked."}, "enum": {"#tail": "\n                  ", "#text": "(4)"}, "@id": "id5CF580830F1D4C70A10DA033A3F5C767", "#text": "\n                  "}], "#text": "\n                ", "@id": "idD846E90F02AF47D0AB508D9F7669CA31"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "Any person who violates subsection (b) shall be fined under this title, imprisoned not more than 5\n\t\t\t years, or both."}, "enum": {"#tail": "\n                ", "#text": "(d)"}, "header": {"#tail": "\n                ", "#text": "Penalty"}, "#text": "\n                ", "@id": "id05AD85AA8C9D4DBEA141CCCE14A050A7"}], "#text": "\n              ", "@id": "id0EE64DB0261941BAA78960FF4AD763C1"}, "enum": {"#tail": "\n            ", "#text": "120"}, "header": {"#tail": "\n            ", "#text": "Electronic tracking"}, "#text": "\n            ", "@id": "id129CCB2239254A2481A1B55516B71D72"}, "@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "idCA7F60C551204B789297D9AB6F7876B2"}, "#text": "\n        ", "@id": "id30A941450C2345CC8B56185ED6BF09C8"}, {"#tail": "\n    ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", is amended by inserting after the\n\t\t\t item relating to ", "@entity-type": "uscode", "#text": "part 1 of title 18, United States Code", "@value": "usc/18"}, {"#tail": " the following:", "@entity-type": "uscode", "#text": "chapter 119", "@value": "usc-chapter/18/119"}], "#text": "The table of chapters for "}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Technical and conforming amendment"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "@display-inline": "no-display-inline", "toc": {"#tail": "\n          ", "multi-column-toc-entry": {"toc-enum": {"#tail": "\n              ", "#text": "120. "}, "target": {"#tail": "\n            ", "#text": "2601"}, "#tail": "\n          ", "@bold": "off", "level-header": {"#tail": "\n              ", "#text": "Electronic tracking", "@level": "chapter"}, "@level": "chapter", "#text": "\n              "}, "#text": "\n            "}, "#text": "\n          ", "@id": "id6BE1C6EF6A9E46A286DB4D351DC634D9"}, "#text": "\n        ", "@id": "id55ED96147EDE400B969BE9C944E31DEE"}], "#text": "\n      ", "@id": "S1"}, "#text": "\n\t\t"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-11-17"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 S2933 IS: To prohibit the unauthorized use of electronic tracking devices."}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. Senate"}, "#text": "\n"}, "#text": "\n"}}}