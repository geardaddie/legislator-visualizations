tail": "\n\t\t\t", "#text": "Use of ignition\n\t\t\t interlock devices to prevent repeat intoxicated driving"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", is amended by inserting after ", "@entity-type": "uscode", "#text": "Chapter 1 of title\n\t\t\t 23, United States Code", "@value": "usc-chapter/23/1"}, {"#tail": " the\n\t\t\t following:", "@entity-type": "uscode", "#text": "section 159", "@value": "usc/23/159"}], "#text": "\n          ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In\n\t\t\t general"}, "quoted-block": {"@style": "USC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "section": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "160."}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Use of ignition\n\t\t\t\tinterlock devices to prevent repeat intoxicated driving"}, "subsection": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "In this section:", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(a)"}, "header": {"#tail": "\n              ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": " means\n\t\t\t\tgrams of alcohol per 100 milliliters of blood or grams of alcohol per 210\n\t\t\t\tliters of breath.", "#text": "alcohol concentration"}, "#text": "The term "}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "Alcohol\n\t\t\t\tconcentration"}, "#text": "\n                ", "@id": "H210A9C986F3041D0871C3A96539D10D0"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": [{"#tail": " and ", "#text": "driving\n\t\t\t\twhile intoxicated"}, {"#tail": " mean\n\t\t\t\tdriving or being in actual physical control of a motor vehicle in a State while\n\t\t\t\thaving a blood alcohol concentration of 0.08 percent or greater.", "#text": "driving under the influence"}], "#text": "The terms "}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Driving while\n\t\t\t\tintoxicated; driving under the influence"}, "#text": "\n                ", "@id": "HAAD35B45B57241E6A1D29E6F8C77BE88"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": "\n\t\t\t\tmeans an in-vehicle device that requires a driver to provide a breath sample\n\t\t\t\tprior to the motor vehicle starting, and that prevents a motor vehicle from\n\t\t\t\tstarting if the alcohol concentration of the driver is above the legal\n\t\t\t\tlimit.", "#text": "ignition interlock device"}, "#text": "The term "}, "enum": {"#tail": "\n                ", "#text": "(3)"}, "header": {"#tail": "\n                ", "#text": "Ignition\n\t\t\t\tinterlock device"}, "#text": "\n                ", "@id": "H43067F8A7AA94FDD90941C68421996E7"}, {"#tail": "\n            ", "subparagraph": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "term": {"#tail": " means a vehicle\n\t\t\t\tdriven or drawn by mechanical power and manufactured primarily for use on\n\t\t\t\tpublic highways.", "#text": "motor vehicle"}, "#text": "The term "}, "enum": {"#tail": "\n                  ", "#text": "(A)"}, "header": {"#tail": "\n                  ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n                  ", "@id": "H1E594B4855FD49E2A347E1F9CEA7BBA2"}, {"#tail": "\n              ", "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t\t", "#text": "a\n\t\t\t\tvehicle operated solely on a rail line; or"}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "@id": "H5195ED0380F24C71A0E409873B02C6D5", "#text": "\n                    "}, {"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t\t", "#text": "a\n\t\t\t\tcommercial vehicle."}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "HF75BAA4FC3584BD48E729A5D663C62EF", "#text": "\n                    "}], "enum": {"#tail": "\n                  ", "#text": "(B)"}, "header": {"#tail": "\n                  ", "#text": "Exclusions"}, "text": {"#tail": "\n\t\t\t\t\t\t\t\t\t", "term": {"#tail": " does not include\u2014", "#text": "motor vehicle"}, "#text": "The\n\t\t\t\tterm "}, "#text": "\n                  ", "@id": "HF9EA298BF0E04412B82711A33958C322"}], "enum": {"#tail": "\n                ", "#text": "(4)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "Motor\n\t\t\t\tvehicle"}, "#text": "\n                ", "@id": "H42CF6BAFF6744137A85E3710074D004F"}], "#text": "\n              ", "@id": "H49203373F18A43AA8032C388D1F6C7BD"}, {"#tail": "\n            ", "enum": {"#tail": "\n              ", "#text": "(b)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Laws requiring\n\t\t\t\tignition interlock devices"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", a State meets the requirements\n\t\t\t\tof this subsection if the State has enacted and is enforcing a law that\n\t\t\t\trequires throughout the State the installation of an ignition interlock device\n\t\t\t\tfor a minimum of 180 days on each motor vehicle operated by an individual who\n\t\t\t\tis convicted of driving while intoxicated or driving under the\n\t\t\t\tinfluence.", "@entity-type": "uscode", "#text": "paragraph (2)", "@value": "usc/23/160/b/2", "@proposed": "true"}, "#text": "Subject to "}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n                ", "@id": "HBCEDF0F0D9AA481CA5FD6746DD5B92B0"}, {"#tail": "\n            ", "@id": "H5006C0BB14F9435EA309CEB13DB8C36F", "enum": {"#tail": "\n                ", "#