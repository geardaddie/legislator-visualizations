{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n\t\t", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "S. 3020"}, "official-title": {"#tail": "\n\t", "#text": "To establish the composition known as America the Beautiful as the national anthem."}, "session": {"#tail": "\n\t\t", "#text": "2d Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n\t\t\t", "@date": "20141216", "#text": "December 16, 2014"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "sponsor": {"#tail": " introduced the following bill; which was read twice and referred to the ", "#text": "Mr. Harkin", "@name-id": "S172"}, "committee-name": {"#tail": "\n      ", "@committee-id": "SSJU00", "#text": "Committee on the Judiciary"}}, "#text": "\n\t\t\t"}, "legis-type": {"#tail": "\n\t\t", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@dms-id": "A1", "@public-private": "public", "legis-body": {"#tail": "\n", "section": {"@section-type": "section-one", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "National anthem"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " finds the following:", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}, "#text": "\n          ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "Findings"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "America the Beautiful is an anthem that far better represents the scope and majesty of the\n\t\t\t geography of the United States of America as well as the principles of\n\t\t\t freedom, liberty, fraternity, and progress that are the unifying beliefs\n\t\t\t of our democracy."}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "id05a635178a0c41309c0606bc5de0b1e4", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "America the Beautiful enshrines the sacrifice of those who have given their lives to create and\n\t\t\t preserve our democracy."}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@id": "id1c8a14d4e0d946ddaa9540b92c2f8df9", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "America the Beautiful should replace the Star-Spangled Banner as the national anthem."}, "enum": {"#tail": "\n          ", "#text": "(3)"}, "@id": "idb30576af1a344a7081392d45ab6b4280", "#text": "\n          "}], "#text": "\n        ", "@id": "id609EA1B2405F45488BA09960EBB3EE21"}, {"#tail": "\n    ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended to read as follows:", "@entity-type": "uscode", "#text": "\n            ", "external-xref": {"#tail": " of title 36, United States Code", "@parsable-cite": "usc/36/301", "#text": "Section 301(a)", "@legal-doc": "usc"}, "@value": "usc/36/301/a"}, "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Amendment"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "subsection": {"#tail": "\n          ", "text": {"#tail": "\n          ", "#text": "The composition consisting of the words and music known as America the Beautiful is the national\n\t\t\t anthem."}, "enum": {"#tail": "\n            ", "#text": "(a)"}, "header": {"#tail": "\n            ", "#text": "Designation"}, "#text": "\n            ", "@id": "id93d0416726c7484085dc1272ea549307"}, "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "id6A431B20C9524D669377D3CBE0FE8093"}, "#text": "\n        ", "@id": "id7CFD018AB14149229595158DB4E45C81"}], "#text": "\n      ", "@id": "id0F992141CA0D45D0BA6967FBCFB576EC"}, "#text": "\n    "}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-12-16"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 S3020 IS: To establish the composition known as America the Beautiful as the national anthem."}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. Senate"}, "#text": "\n"}, "#text": "\n"}}}