  ", "@id": "idE68D35A1DE7A4D6B9D0BE97A00102DFC"}, {"@section-type": "subsequent-section", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Taxation of income of\n\t\t\t controlled foreign corporations attributable to imported property"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "is amended by striking the period at the end of", "@entity-type": "act", "#text": "Subsection (a) of section 954 of the Internal Revenue Code\n\t\t\t of 1986", "@value": "Internal Revenue Code of 1986/s:954/ss:a"}, {"#tail": "and\n\t\t\t inserting", "@entity-type": "act", "#text": "paragraph (5)", "@value": "Internal Revenue Code of 1986/s:954/ss:a/p:5"}, {"#tail": "as", "@entity-type": "act", "#text": "paragraph (5)", "@value": "Internal Revenue Code of 1986/s:954/ss:a/p:5"}, {"#tail": ", and by adding at the end the following new paragraph:", "@entity-type": "act", "#text": "paragraph\n\t\t\t (4)", "@value": "Internal Revenue Code of 1986/s:954/ss:a/p:4", "@proposed": "true"}], "#text": "\n          ", "quote": {"#tail": ", by redesignating", "#text": ", and"}}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "General\n\t\t\t Rule"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "paragraph": {"#tail": "\n          ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "and reduced as\n\t\t\t\tprovided in", "@entity-type": "act", "#text": "subsection (j)", "@value": "Internal Revenue Code of 1986/s:954/ss:j", "@proposed": "true"}, {"#tail": ").", "@entity-type": "act", "#text": "subsection\n\t\t\t\t(b)(5)", "@value": "Internal Revenue Code of 1986/s:954/ss:b/p:5"}], "#text": "imported property\n\t\t\t\tincome for the taxable year (determined under"}, "enum": {"#tail": "\n            ", "#text": "(5)"}, "@id": "ID102D28439F6C4B668A2FA9F90CB91F00", "#text": "\n            "}, "#text": "\n          ", "@id": "ID5BDAC310149246F183AA06327F385B18"}, "#text": "\n        ", "@id": "ID1CB82BB9E232404C860338A93DB4D1CE"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "is amended by adding at the end the following new subsection:", "@entity-type": "act", "#text": "Section 954 of the Internal Revenue Code\n\t\t\t of 1986", "@value": "Internal Revenue Code of 1986/s:954"}, "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Definition of\n\t\t\t Imported Property Income"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "subsection": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "(j)"}, "header": {"#tail": "\n            ", "#text": "Imported\n\t\t\t\tProperty Income"}, "paragraph": [{"continuation-text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ") or any foreign oil related income (within the meaning of", "@entity-type": "act", "#text": "section 907(c)", "@value": "Internal Revenue Code of 1986/s:907/ss:c"}, {"#tail": ").", "@entity-type": "act", "#text": "section 907(c)", "@value": "Internal Revenue Code of 1986/s:907/ss:c"}], "@continuation-text-level": "paragraph", "#text": "Such term\n\t\t\t\tshall not include any foreign oil and gas extraction income (within the meaning\n\t\t\t\tof"}, "#tail": "\n            ", "@id": "ID68FE72D3F7744BD1B878130DF02CFA77", "enum": {"#tail": "\n              ", "#text": "(1)"}, "header": {"#tail": "\n              ", "#text": "In\n\t\t\t\tgeneral"}, "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", the term", "@entity-type": "act", "#text": "subsection (a)(5)", "@value": "Internal Revenue Code of 1986/s:954/ss:a/p:5"}, "term": {"#tail": "means income (whether in the form of\n\t\t\t\tprofits, commissions, fees, or otherwise) derived in connection with\u2014", "#text": "imported property income"}, "#text": "For purposes of"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "manufacturing,\n\t\t\t\tproducing, growing, or extracting imported property;"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "IDF027510C6ADB4194A2DE4D829714A368", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "the sale,\n\t\t\t\texchange, or other disposition of imported property; or"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "ID2EA2B95B13724FAF87C8DC6AC8162E62", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "the lease,\n\t\t\t\trental, or licensing of imported property."}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@id": "ID90F2D5C306674400AE6DAEF041E5B5F0", "#text": "\n                "}]}, {"#tail": "\n            ", "@id": "IDE7BD18DB53AA49D6BD67E72BD5E3FD34", "enum": {"#tail": "\n              ", "#text": "(2)"}, "header": {"#tail": "\n              ", "#text": "Imported\n\t\t\t\tproperty"}, "text": {"#tail": "\n              ", "#text": "For purposes of this subsection\u2014"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": "means property which is imported into the United\n\t\t\t\tStates by the controlled foreign corporation or a related person.", "#text": "imported property"}, "#text": "Except as otherwise provided in this paragraph, the term"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n                ", "@id": "ID0961E19468874DE69F348433FA9AEC0C"}, {"#tail": "\n              ", "clause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "such property\n\t\t\t\twould be imported into the United States; or"}, "enum": {"#tail": "\n                  ", "#text": "(i)"}, "@id": "ID9CD588352746492E9787F9E9F8650B52", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "such property\n\t\t\t\twould be used as a component in other property which would be imported into the\n\t\t\t\tUnited States."}, "enum": {"#tail": "\n                  ", "#text": "(ii)"}, "@id": "IDAC5E75EA54A14A6193AF29834F2FAA2B", "#text": "\n                  "}], "enum": {"#tail": "\n                ", "#text": "(B)"}, "header": {"#tail": "\n                ", "#text": "Imported\n\t\t\t\tproperty includes certain property imported by unrelated\n\t\t\t\tpersons"}, "text": {"#tail": "\n                ", "term": {"#tail": "includes any\n\t\t\t\tproperty imported into the United States by an unrelated person if, when such\n\t\t\t\tproperty was sold to the unrelated person by the controlled foreign corporation\n\t\t\t\t(or a related person), it was reasonable to expect that\u2014", "#text": "imported property"}, "#text": "The term"}, "#text": "\n                ", "@id": "ID0C77D59EEBFC47F9B55AC318F260B237"}, {"#tail": "\n              ", "clause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "before\n\t\t\t\tsubstantial use in the United States, is sold, leased, or rented by the\n\t\t\t\tcontrolled foreign corporation or a related person for direct use, consumption,\n\t\t\t\tor disposition outside the United States; or"}, "enum": {"#tail": "\n                  ", "#text": "(i)"}, "@id": "IDDD55F7D29E234688A7249A87E524D106", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "is used by the\n\t\t\t\tcontrolled foreign corporation or a related person as a component in other\n\t\t\t\tproperty which is so sold, leased, or rented."}, "enum": {"#tail": "\n                  ", "#text": "(ii)"}, "@id": "ID5B519C0CA1884A96B767FF3BC29DC78D", "#text": "\n                  "}], "enum": {"#tail": "\n                ", "#text": "(C)"}, "header": {"#tail": "\n                ", "#text": "Exception for\n\t\t\t\tproperty subsequently exported"}, "text": {"#tail": "\n                ", "term": {"#tail": "does not include any property which is imported into the United\n\t\t\t\tStates and which\u2014", "#text": "imported\n\t\t\t\tproperty"}, "#text": "The term"}, "#text": "\n                ", "@id": "ID4857227D80264023B537A082A6B06DB2"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "term": {"#tail": "does not include any agricultural commodity which is not grown\n\t\t\t\tin the United States in commercially marketable quantities.", "#text": "imported\n\t\t\t\tproperty"}, "#text": "The term"}, "enum": {"#tail": "\n                ", "#text": "(D)"}, "header": {"#tail": "\n                ", "#text": "Exception for\n\t\t\t\tcertain agricultural commodities"}, "#text": "\n                ", "@id": "idEEC634691CCE41E1ACCC3800C192CADE"}]}, {"#tail": "\n          ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ") in\n\t\t\t\tthe United States.", "@entity-type": "act", "#text": "section 936(h)(3)(B)", "@value": "Internal Revenue Code of 1986/s:936/ss:h/p:3/sp:B"}, "term": {"#tail": "means entering, or\n\t\t\t\twithdrawal from warehouse, for consumption or use. Such term includes any grant\n\t\t\t\tof the right to use intangible property (as defined in", "#text": "import"}, "#text": "For\n\t\t\t\tpurposes of this subsection, the term"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "header": {"#tail": "\n                ", "#text": "Import"}, "#text": "\n                ", "@id": "ID9408F98654E84BC2A9520E2AD6FB3410"}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": "includes the Commonwealth of Puerto Rico, the Virgin Islands of\n\t\t\t\tthe United States, Guam, American Samoa, and the Commonwealth of the Northern\n\t\t\t\tMariana Islands.", "#text": "United\n\t\t\t\tStates"}, "#text": "For purposes of this subsection, the term"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "header": {"#tail": "\n                ", "#text": "United\n\t\t\t\tstates"}, "#text": "\n                ", "@id": "ID5A6848EFD4474709A50D1827BC629781"}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": "means any person who is not a related person with respect to the\n\t\t\t\tcontrolled foreign corporation.", "#text": "unrelated\n\t\t\t\tperson"}, "#text": "For purposes of this subsection, the term"}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "header": {"#tail": "\n                ", "#text": "Unrelated\n\t\t\t\tperson"}, "#text": "\n                ", "@id": "ID95B783D9BA6040629C809825094F90B0"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "term": {"#tail": "shall not\n\t\t\t\tinclude any imported property\n\t\t\t\tincome.", "#text": "foreign base company sales income"}, "#text": "For purposes of this\n\t\t\t\tsection, the term"}, "enum": {"#tail": "\n                ", "#text": "(D)"}, "header": {"#tail": "\n                ", "#text": "Coordination\n\t\t\t\twith foreign base company sales income"}, "#text": "\n                ", "@id": "ID8811ABC96FE94B89B469CDA9C84D2C7B"}], "enum": {"#tail": "\n              ", "#text": "(3)"}, "header": {"#tail": "\n              ", "#text": "Definitions and\n\t\t\t\tspecial rules"}, "#text": "\n              ", "@id": "ID0CDAB1DE8FEA42B08C6273870AD41889"}], "#text": "\n            ", "@id": "ID5ABFA047007849DCAB0D82A7FEA9E504"}, "#text": "\n          ", "@id": "IDC82ED20BC007428CBF4831408CB6E6B1"}, "#text": "\n        ", "@id": "ID6D1D93DC7C654257B0512E11D41A3148"}, {"#tail": "\n      ", "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "Separate\n\t\t\t Application of Limitations on Foreign Tax Credit for Imported Property\n\t\t\t Income"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "is amended by striking", "@entity-type": "act", "#text": "Paragraph (1) of section 904(d) of the Internal Revenue\n\t\t\t Code of 1986", "@value": "Internal Revenue Code of 1986/s:904/ss:d/p:1"}, {"#tail": ", by redesignating", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Internal Revenue Code of 1986/s:904/ss:d/p:1/sp:A"}, {"#tail": "as", "@entity-type": "act", "#text": "subparagraph (B)", "@value": "Internal Revenue Code of 1986/s:904/ss:d/p:1/sp:B"}, {"#tail": ", and by\n\t\t\t inserting after", "@entity-type": "act", "#text": "subparagraph (C)", "@value": "Internal Revenue Code of 1986/s:904/ss:d/p:1/sp:C", "@proposed": "true"}, {"#tail": "the following new subparagraph:", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Internal Revenue Code of 1986/s:904/ss:d/p:1/sp:A"}], "#text": "\n            ", "quote": {"#tail": "at the end of", "#text": "and"}}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "In\n\t\t\t general"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "#tail": "\n        ", "subparagraph": {"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "imported property\n\t\t\t\tincome,\n\t\t\t\tand"}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "@id": "IDDB059A6AB54B447B9DF7C323734BF616", "#text": "\n              "}, "#text": "\n            ", "@id": "ID71CC174F5C8646B4B93FE1FBC2B138E5"}, "#text": "\n          ", "@id": "ID08DB284A81A44E7AAB43C92E19773731"}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "{http://namespace