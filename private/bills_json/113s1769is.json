{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "S. 1769"}, "official-title": {"#tail": "\n  ", "#text": "To limit the establishment of certain standards of care or duties of care owed by health care providers to patients in any medical malpractice or medical product liability action or claim."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20131121", "#text": "November 21, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "committee-name": {"#tail": "\n      ", "@committee-id": "SSJU00", "#text": "Committee on the Judiciary"}, "sponsor": {"#tail": " (for himself and ", "#text": "Mr. Toomey", "@name-id": "S351"}, "cosponsor": {"#tail": ") introduced the following bill; which was read twice and referred to the ", "#text": "Mr. Carper", "@name-id": "S277"}}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@dms-id": "A1", "@public-private": "public", "legis-body": {"#tail": "\n", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "\n            Standard of Care Protection Act\n          "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the\n\t\t  ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "S1"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Rule of construction regarding health care provider standards of care"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "#text": "The development, recognition, or implementation of any guideline or other standard under any Federal health care provision shall not be construed to establish the standard of care or duty of care owed by a health care provider to a patient in any medical malpractice or medical product liability action or claim."}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "#text": "\n        ", "@id": "id9f96e6e7d50b498a93684244ffdd9954"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "For purposes of this section:"}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n        ", "text": {"{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "act", "#text": "titles XVIII", "@value": "Social Security Act/t:XVIII"}, {"#tail": " (", "@entity-type": "act", "#text": "XIX of the Social Security Act", "@value": "Social Security Act/t:XIX"}, {"#tail": ").", "@entity-type": "uscode", "#text": "1396 et seq.", "@value": "usc/42/1396/etseq"}], "term": {"#tail": " means any provision of the ", "#text": "Federal health care provision"}, "#tail": "\n        ", "external-xref": {"#tail": " and ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "uscode", "#text": "42 U.S.C. 1395 et seq", "@value": "usc/42/1395/etseq"}, "@parsable-cite": "usc/42/1395", "#text": "\n              ", "@legal-doc": "usc"}, "{http://namespaces.cato.org/catoxml}entity": [{"#tail": ", ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Patient Protection and Affordable Care Act", "@value": "Patient Protection and Affordable Care Act"}, "@entity-type": "law-citation", "#text": "\n              ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n            ", "@entity-type": "public-law", "#text": "Public Law 111\u2013148", "@value": "public-law/111/148"}, "@parsable-cite": "pl/111/148", "#text": "\n              ", "@legal-doc": "public-law"}}, {"#tail": ", and \n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "act", "#text": "title I", "@value": "Health Care and Education Reconciliation Act of 2010/t:I"}, {"#tail": " (", "@entity-type": "act", "#text": "subtitle B of title III of the Health Care and Education Reconciliation Act of 2010", "@value": "Health Care and Education Reconciliation Act of 2010/t:III/st:B"}], "@entity-type": "law-citation", "#text": "\n              ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n            ", "@entity-type": "public-law", "#text": "Public Law 111\u2013152", "@value": "public-law/111/152"}, "@parsable-cite": "pl/111/152", "#text": "\n              ", "@legal-doc": "public-law"}}], "#text": "The term "}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "Federal health care provision"}, "#text": "\n          ", "@id": "id0b60793974824262a02bfe13ba51dc29"}, {"#tail": "\n        ", "@id": "idfa30e9f298c843b4931507e7eed6efc6", "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Health care provider"}, "text": {"#tail": "\n          ", "term": {"#tail": " means any individual or entity\u2014", "#text": "health care provider"}, "#text": "The term "}, "#text": "\n          ", "subparagraph": [{"#tail": "\n          ", "text": {"#tail": "\n          ", "#text": "licensed, registered, or certified under Federal or State laws or regulations to provide health care services; or"}, "enum": {"#tail": "\n            ", "#text": "(A)"}, "@id": "id81aed1b2989a477f92d4a7efc90f78f0", "#text": "\n            "}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "#text": "required to be so licensed, registered, or certified but that is exempted by other law or regulation."}, "enum": {"#tail": "\n            ", "#text": "(B)"}, "@id": "id7f1b9c5eb20545ea93d4b0a3d3930676", "#text": "\n            "}]}, {"#tail": "\n        ", "text": {"#tail": "\n        ", "term": {"#tail": " means a medical malpractice action or claim (as defined in ", "#text": "medical malpractice or medical product liability action or claim"}, "#text": "The term ", "{http://namespaces.cato.org/catoxml}entity": [{"#tail": ") and includes a liability action or claim relating to a health care provider's prescription or provision of a drug, device, or biological product (as such terms are defined in ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "section 431(7) of the Health Care Quality Improvement Act of 1986", "@value": "Health Care Quality Improvement Act of 1986/s:431/ss:7"}, "@entity-type": "law-citation", "#text": "\n              ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n            ", "@entity-type": "uscode", "#text": "42 U.S.C. 11151(7)", "@value": "usc/42/11151//7"}, "@parsable-cite": "usc/42/11151", "#text": "\n              ", "@legal-doc": "usc"}}, {"#tail": " or ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "section 201 of the Federal Food, Drug, and Cosmetic Act", "@value": "Federal Food, Drug, and Cosmetic Act/s:201"}, "@entity-type": "law-citation", "#text": "\n              ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n            ", "@entity-type": "uscode", "#text": "21 U.S.C. 321", "@value": "usc/21/321"}, "@parsable-cite": "usc/21/321", "#text": "\n              ", "@legal-doc": "usc"}}, {"#tail": ").", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "section 351 of the Public Health Service Act", "@value": "Public Health Service Act/s:351"}, "@entity-type": "law-citation", "#text": "\n              ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n            ", "@entity-type": "uscode", "#text": "42 U.S.C. 262", "@value": "usc/42/262"}, "@parsable-cite": "usc/42/262", "#text": "\n              ", "@legal-doc": "usc"}}]}, "enum": {"#tail": "\n          ", "#text": "(3)"}, "header": {"#tail": "\n          ", "#text": "Medical malpractice or medical product liability action or claim"}, "#text": "\n          ", "@id": "idb15080c5c19545509f1c182a57651eb7"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "term": {"#tail": " includes the 50 States, the District of Columbia, Puerto Rico, and any other commonwealth, possession, or territory of the United States.", "#text": "State"}, "#text": "The term "}, "enum": {"#tail": "\n          ", "#text": "(4)"}, "header": {"#tail": "\n          ", "#text": "State"}, "#text": "\n          ", "@id": "id91cf5af87757418691569944930e3fc3"}], "#text": "\n        ", "@id": "idff84a59480fc4800918c01fb75989417"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " or ", "@entity-type": "act", "#text": "title XVIII", "@value": "Social Security Act/t:XVIII"}, {"#tail": " (", "@entity-type": "act", "#text": "XIX of the Social Security Act", "@value": "Social Security Act/t:XIX"}, {"#tail": ") shall be construed to preempt any State or common law governing medical malpractice or medical product liability actions or claims.", "@entity-type": "uscode", "#text": "1396 et seq.", "@value": "usc/42/1396/etseq"}], "#text": "No provision of the ", "external-xref": {"#tail": " and ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "uscode", "#text": "42 U.S.C. 1395 et seq", "@value": "usc/42/1395/etseq"}, "@parsable-cite": "usc/42/1395", "#text": "\n            ", "@legal-doc": "usc"}, "{http://namespaces.cato.org/catoxml}entity": [{"#tail": ", ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Patient Protection and Affordable Care Act", "@value": "Patient Protection and Affordable Care Act"}, "@entity-type": "law-citation", "#text": "\n            ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "public-law", "#text": "Public Law 111\u2013148", "@value": "public-law/111/148"}, "@parsable-cite": "pl/111/148", "#text": "\n            ", "@legal-doc": "public-law"}}, {"#tail": ", or \n            ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " or ", "@entity-type": "act", "#text": "title I", "@value": "Health Care and Education Reconciliation Act of 2010/t:I"}, {"#tail": " (", "@entity-type": "act", "#text": "subtitle B of title III of the Health Care and Education Reconciliation Act of 2010", "@value": "Health Care and Education Reconciliation Act of 2010/t:III/st:B"}], "@entity-type": "law-citation", "#text": "\n            ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "public-law", "#text": "Public Law 111\u2013152", "@value": "public-law/111/152"}, "@parsable-cite": "pl/111/152", "#text": "\n            ", "@legal-doc": "public-law"}}]}, "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "No preemption"}, "#text": "\n        ", "@id": "idd37623b7cd2048d6949f44b539d1f132"}], "#text": "\n      ", "@id": "id2ff65607ebf24941bdc73055b8b74502"}], "#text": "\n    "}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-11-21"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://pur