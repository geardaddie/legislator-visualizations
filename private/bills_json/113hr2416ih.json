ction": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "#text": "United States Civil Rights Trail\n\t\t\t Special Resource Study Act of 2013"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the\n\t\t\t ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H7DC1667D66EF442B9C7D32FC3341C9B6"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n\t\t\t", "#text": "Special resource\n\t\t\t study regarding proposed United States Civil Rights Trail"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall conduct a special\n\t\t\t resource study for the purpose of evaluating a range of alternatives for\n\t\t\t protecting and interpreting sites associated with the struggle for civil rights\n\t\t\t in the United States, including alternatives for potential addition of some or\n\t\t\t all of the sites to the National Trails System.", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "#text": "The "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "Study\n\t\t\t Required"}, "#text": "\n        ", "@id": "HE1155189F4D64B5D88580040BFE1C34E"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall conduct the special resource study in consultation with\n\t\t\t appropriate Federal, State, county, and local governmental entities.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, "#text": "The\n\t\t\t "}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Consultation"}, "#text": "\n        ", "@id": "H28FE36A403364C7AA25782D6839E530B"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall conduct the study required under\n\t\t\t ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, {"#tail": " in accordance with ", "@entity-type": "act", "#text": "subsection (a)", "@value": "United States Civil Rights Trail Special Resource Study Act of 2013/s:2/ss:a", "@proposed": "true"}], "#text": "The ", "{http://namespaces.cato.org/catoxml}entity": [{"#tail": " and ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " (", "@entity-type": "public-law", "#text": "section 8(c) of ", "external-xref": {"#tail": "\n            ", "@parsable-cite": "pl/91/383", "#text": "Public Law 91\u2013383", "@legal-doc": "public-law"}, "@value": "public-law/91/383/s:8/ss:c"}, {"#tail": ")", "@entity-type": "uscode", "#text": "16 U.S.C.\n\t\t\t 1a\u20135(c)", "@value": "usc/16/1a\u20135/c"}], "@entity-type": "law-citation", "#text": "\n            "}, {"#tail": ", as appropriate.", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " (", "@entity-type": "act", "#text": "section 5(b) of the National Trails System Act", "@value": "National Trails System Act/s:5/ss:b"}, {"#tail": ")", "@entity-type": "uscode", "#text": "16 U.S.C.\n\t\t\t 1244(b)", "@value": "usc/16/1244/b"}], "@entity-type": "law-citation", "#text": "\n            "}]}, "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "Study\n\t\t\t requirements"}, "#text": "\n        ", "@id": "H6491B9B78E06448EBB1136001EC3DD10"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall evaluate alternatives for achieving the following\n\t\t\t objectives:", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, "#text": "In conducting the special resource study, the\n\t\t\t "}, "enum": {"#tail": "\n        ", "#text": "(d)"}, "header": {"#tail": "\n        ", "#text": "Study\n\t\t\t Objectives"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "#text": "Identifying the\n\t\t\t resources and historic themes associated with the movement to secure racial\n\t\t\t equality in the United States for African Americans that, focusing on the\n\t\t\t period from 1954 through 1968, challenged the practice of racial segregation in\n\t\t\t the Nation and achieved equal rights for all American citizens."}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "HCEFDE46647484E399D2FBB358F189CFE", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "#text": "Making a review of\n\t\t\t existing studies and reports, such as the Civil Rights Framework Study, to\n\t\t\t complement and not duplicate other studies of the historical importance of the\n\t\t\t civil rights movements that may be underway or undertaken."}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@id": "H0268FC7211034252827816BA031ABD52", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "#text": "Establishing\n\t\t\t connections with agencies, organizations, and partnerships already engaged in\n\t\t\t the preservation and interpretation of various trails and sites dealing with\n\t\t\t the civil rights movement."}, "enum": {"#tail": "\n          ", "#text": "(3)"}, "@id": "H2764DC6FA98342458D2887241F6EC5E5", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "#text": "Protecting\n\t\t\t historically significant landscapes, districts, sites, and structures."}, "enum": {"#tail": "\n          ", "#text": "(4)"}, "@id": "H7AEBEFE00473414E888C1C113E9E4EC3", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", other Federal, State, or local governmental entities, or private\n\t\t\t and nonprofit organizations, including the potential inclusion of some or all\n\t\t\t of the sites in a National Civil Rights Trail.", "@entity-type": "federal-body", "#text": "National\n\t\t\t Park Service", "@entity-id": "1443"}, "#text": "Identifying\n\t\t\t alternatives for preservation and interpretation of the sites by the "}, "enum": {"#tail": "\n          ", "#text": "(5)"}, "@id": "H2C2C6F262D8F474D8EEBFCC28CF1B160", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "#text": "Identifying cost\n\t\t\t estimates for any necessary acquisition, development, interpretation,\n\t\t\t operation, and maintenance associated with the alternatives developed under the\n\t\t\t special resource study."}, "enum": {"#tail": "\n          ", "#text": "(6)"}, "@id": "HF51F296E601F4449A6DCDD9E59F9BDF2", "#text": "\n          "}], "#text": "\n        ", "@id": "H4935E564882044C2A8E660DB00021FC2"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall submit to the ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, {"#tail": " and the ", "@entity-type": "committee", "#text": "Committee on Natural\n\t\t\t Resources of the House of Representatives", "@entity-id": "HII00"}, {"#tail": " a report containing the results of the study\n\t\t\t conducted under ", "@entity-type": "committee", "#text": "Committee on Energy and\n\t\t\t Natural Resources of the Senate", "@entity-id": "SSEG00"}, {"#tail": " and any recommendations of the ", "@entity-type": "act", "#text": "subsection (c)", "@value": "United States Civil Rights Trail Special Resource Study Act of 2013/s:2/ss:c", "@proposed": "true"}, {"#tail": " with\n\t\t\t respect to the route.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}], "#text": "Not\n\t\t\t later than 3 years after the date on which funds are made available to carry\n\t\t\t out this section, the "}, "enum": {"#tail": "\n        ", "#text": "(e)"}, "header": {"#tail": "\n        ", "#text": "Report"}, "#text": "\n        ", "@id": "H649CEC727F61454080C79A4E02614515"}], "#text": "\n      ", "@id": "HC168337DCD724FAB8072A9AA03B3B837"}], "#text": "\n\t\t", "@id": "HBD21845FDFEE4F2F87359D1D361F3932"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-06-18"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 2416 IH: United States Civil Rights Trail Special Resource Study Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}