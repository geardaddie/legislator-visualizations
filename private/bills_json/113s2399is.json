: "idFFCFD16F4BB542B7977C75C43CEC26CD"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended\u2014", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 8 of the Voting Rights Act of 1965", "@value": "Voting Rights Act of 1965/s:8"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "uscode", "#text": "42 U.S.C. 1973f", "@value": "usc/42/1973f"}, "@parsable-cite": "usc/42/1973f", "#text": "\n          ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "4."}, "header": {"#tail": "\n      ", "#text": "Federal election oversight on Indian reservations"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " as ", "@entity-type": "act", "#text": "subsections (b) through (e)", "@value": "Voting Rights Act of 1965/s:8/ss:b..e"}, {"#tail": ", respectively;", "@entity-type": "act", "#text": "subsections (c) through (f)", "@value": "Voting Rights Act of 1965/s:8/ss:c..f", "@proposed": "true"}], "#text": "by redesignating "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@id": "id0B2D2911C96646A3BA741980B8C50D20", "#text": "\n        "}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", as redesignated by ", "@entity-type": "act", "#text": "subsection (c)", "@value": "Voting Rights Act of 1965/s:8/ss:c", "@proposed": "true"}, {"#tail": " of this section, by striking ", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Native Voting Rights Act of 2014/s:4/p:1", "@proposed": "true"}], "#text": "in ", "quote": [{"#tail": " and inserting ", "#text": "subsection (c), such observers"}, {"#tail": "; and", "{