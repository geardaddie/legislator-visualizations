"{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "; and", "@entity-type": "uscode", "#text": "section\n\t\t\t\t924(c)(2)", "@value": "usc/18/924/c/2"}, "term": {"#tail": " has the meaning given that term in ", "#text": "drug trafficking crime"}, "#text": "the term\n\t\t\t\t"}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "@id": "id8C9A57715D8840CBBF04C9C44E749585", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "uscode", "#text": "section 2332b(g)", "@value": "usc/18/2332b/g"}, "term": {"#tail": " has the meaning given that term in\n\t\t\t\t", "#text": "Federal crime of terrorism"}, "#text": "the term\n\t\t\t\t"}, "enum": {"#tail": "\n                ", "#text": "(3)"}, "@id": "id26CC91B847C14BF09F5CE577DBDCB0BD", "#text": "\n                "}], "#text": "\n              ", "@id": "idA5EFEB2898C3478D8AC32B21D6F88C3D"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "It\n\t\t\t\tshall be unlawful for any person to\u2014"}, "enum": {"#tail": "\n              ", "#text": "(b)"}, "header": {"#tail": "\n              ", "#text": "Offense"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "purchase or\n\t\t\t\totherwise obtain a firearm, which has been shipped, transported, or received in\n\t\t\t\tinterstate or foreign commerce, for or on behalf of any other person who the\n\t\t\t\tperson purchasing or otherwise obtaining the firearm knows\u2014"}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "#text": "\n                ", "@id": "id85680AA1DEBD48399AF81208A6B370EE", "subparagraph": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " or ", "@entity-type": "uscode", "#text": "subsection (g)", "@value": "usc/18/922/g"}, {"#tail": ";", "@entity-type": "uscode", "#text": "(n) of section\n\t\t\t\t922", "@value": "usc/18/922/n"}], "#text": "is prohibited\n\t\t\t\tfrom possessing or receiving a firearm under "}, "enum": {"#tail": "\n                  ", "#text": "(A)"}, "@id": "id5302FB4356BD4F70BB05969D5CF20FEA", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "intends to use,\n\t\t\t\tcarry, possess, or sell or otherwise dispose of the firearm in furtherance of a\n\t\t\t\tcrime of violence, a drug trafficking crime, or a Federal crime of\n\t\t\t\tterrorism;"}, "enum": {"#tail": "\n                  ", "#text": "(B)"}, "@id": "id9E3FAC050E5E44F19ABCCBD7BEC70C45", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "intends to engage\n\t\t\t\tin conduct that would constitute a crime of violence, a drug trafficking crime,\n\t\t\t\tor a Federal crime of terrorism if the conduct had occurred within the United\n\t\t\t\tStates; or"}, "enum": {"#tail": "\n                  ", "#text": "(C)"}, "@id": "idB63AD9B5D6264AFC85BCBFABC9422E09", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "is not a resident\n\t\t\t\tof any State and is not a citizen or lawful permanent resident of the United\n\t\t\t\tStates; or"}, "enum": {"#tail": "\n                  ", "#text": "(D)"}, "@id": "id57135BECDE0C4C338A8050FF7BE1C259", "#text": "\n                  "}]}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "uscode", "#text": "paragraph (1)", "@value": "usc/18/932/b/1", "@proposed": "true"}, "#text": "willfully procure\n\t\t\t\tanother to engage in conduct described in "}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "@id": "id40C407C846EB47558E3EE6763C85306A", "#text": "\n                "}], "#text": "\n              ", "@id": "id9B889D4493B2445B896C47BB48B01C27"}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall be fined under this title, imprisoned\n\t\t\t\tnot more than 15 years, or both.", "@entity-type": "uscode", "#text": "subsection (b)", "@value": "usc/18/932/b", "@proposed": "true"}, "#text": "Any\n\t\t\t\tperson who violates "}, "enum": {"#tail": "\n              ", "#text": "(c)"}, "header": {"#tail": "\n              ", "#text": "Penalty"}, "#text": "\n              ", "@id": "id63FB1792CDC44BE28F67214DE81B1BEA"}], "#text": "\n            ", "@id": "idA01B0401F0AC43BCAFC7938CD3E21D6E"}, {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "933."}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Trafficking in\n\t\t\t\tfirearms"}, "subsection": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "For\n\t\t\t\tpurposes of this section\u2014"}, "enum": {"#tail": "\n              ", "#text": "(a)"}, "header": {"#tail": "\n              ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ";", "@entity-type": "uscode", "#text": "section\n\t\t\t\t924(c)(3)", "@value": "usc/18/924/c/3"}, "term": {"#tail": " has the meaning given that term in ", "#text": "crime of violence"}, "#text": "the term\n\t\t\t\t"}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "@id": "id13FFE85311C84B5587EF2DE84C85D772", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "; and", "@entity-type": "uscode", "#text": "section\n\t\t\t\t924(c)(2)", "@value": "usc/18/924/c/2"}, "term": {"#tail": " has the meaning given that term in ", "#text": "drug trafficking crime"}, "#text": "the term\n\t\t\t\t"}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "@id": "idCE8C3