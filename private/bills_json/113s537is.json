{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n    ", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "S. 537"}, "official-title": {"#tail": "\n  ", "#text": "To require the Small Business Administration to make\n\t\t  information relating to lenders making covered loans publicly available, and\n\t\t  for other purposes."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20130312", "#text": "March 12, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "sponsor": {"#tail": "introduced\n\t\t\t the following bill; which was read twice and referred to the", "#text": "Ms. Landrieu", "@name-id": "S258"}, "committee-name": {"#tail": "\n      ", "@committee-id": "SSSB00", "#text": "Committee on Small Business and\n\t\t\t Entrepreneurship"}}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@dms-id": "A1", "@public-private": "public", "legis-body": {"#tail": "\n", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": [{"short-title": {"#tail": "\n        ", "#text": "Communicating Lender Activity\n\t\t\t Reports from the Small Business Administration Act"}, "#tail": "or the", "#text": "\n          "}, {"short-title": {"#tail": "\n        ", "#text": "CLEAR SBA\n\t\t\t Act"}, "#tail": ".", "#text": "\n          "}], "#text": "This Act may be cited as\n\t\t\t the", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short\n\t\t\t title"}, "#text": "\n      ", "@id": "S1"}, {"#tail": "\n  ", "text": {"#tail": "\n      ", "#text": "\n        ", "{http://namespac