 "HB71902E091F34CF49AB9896B7DB93216"}, {"#tail": "\n      ", "@id": "HB2ECEE332A6E4747A090D6A1D1A97C18", "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Eligible entity"}, "text": {"#tail": "\n\t\t\t\t\t", "term": {"#tail": " means\u2014", "#text": "eligible entity"}, "#text": "The term "}, "#text": "\n          ", "subparagraph": [{"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "#text": "a State;", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(A)"}, "@id": "H314364CB15A14F8AB08E6F15382C9C3C", "#text": "\n            "}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "#text": "a general unit of local government;"}, "enum": {"#tail": "\n            ", "#text": "(B)"}, "@id": "H1E7E0BF0CE0848CD9E2E9B2F74A21646", "#text": "\n            "}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "#text": "an Indian tribe; or"}, "enum": {"#tail": "\n            ", "#text": "(C)"}, "@id": "H09682330CE2D4DC7A19FB1D43516A61A", "#text": "\n            "}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "any other organization that has a documented history of effective work concerning domestic\n\t\t\t violence, dating violence, sexual assault, or stalking (as determined by\n\t\t\t the Secretary), including\u2014", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(D)"}, "clause": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "a domestic violence and sexual assault victim service provider;"}, "enum": {"#tail": "\n              ", "#text": "(i)"}, "@id": "H204389BE880C49E48442D449A29BA9A2", "#text": "\n              "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "a domestic violence and sexual assault coalition;"}, "enum": {"#tail": "\n              ", "#text": "(ii)"}, "@id": "H1840C5E7F6F44D6988FA90FAFD7A6EC0", "#text": "\n              "}, 