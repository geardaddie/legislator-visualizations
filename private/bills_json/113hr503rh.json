il": "\n\t", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "National Desert Storm and Desert Shield War Memorial Act", "@value": "National Desert Storm and Desert Shield War Memorial Act", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H3B65D035D7A34BA99CCBC16D0C379537"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "#text": "For the purposes of this Act:", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and exempt from\n\t\t\t taxation under ", "@entity-type": "act", "#text": "section 501(c)(3)", "@value": "Internal Revenue Code of 1986/s:501/ss:c/p:3"}, {"#tail": ".", "@entity-type": "act", "#text": "\n            ", "external-xref": {"#tail": " of the Internal Revenue Code of 1986", "@parsable-cite": "usc/26/501", "#text": "section 501(a)", "@legal-doc": "usc"}, "@value": "Internal Revenue Code of 1986/s:501/ss:a"}], "term": {"#tail": " means the National Desert Storm Memorial Association, a corporation organized under the laws of\n\t\t\t the State of Arkansas and described in ", "#text": "Association"}, "#text": "The term ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "header": {"#tail": "\n        ", "#text": "Association"}, "#text": "\n        ", "@id": "HA0F73D1C251F48859B4BDD89B084CACF"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "section 3", "@value": "National Desert Storm and Desert Shield War Memorial Act/s:3", "@proposed": "true"}, "term": {"#tail": " means the National Desert Storm and Desert Shield Memorial authorized to be established under\n\t\t\t ", "#text": "memorial"}, "#text": "The term ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "header": {"#tail": "\n        ", "#text": "Memorial"}, "#text": "\n        ", "@id": "HE18A828EF3AB4EABB10E9A71755E6951"}], "#text": "\n      ", "@id": "H527EB45B710948B5A6BD47876F0B9F77"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n\t\t\t", "#text": "Memorial to commemorate"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "#text": "The Association may establish the National Desert Storm and Desert Shield Memorial as a\n\t\t\t commemorative work, on Federal land in the District of Columbia to\n\t\t\t commemorate and honor those who, as a member of the Armed Forces, served\n\t\t\t on active duty in support of Operation Desert Storm or Operation Desert\n\t\t\t Shield.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "Authorization To establish commemorative work"}, "#text": "\n        ", "@id": "HCB8EB837818C4698A095F0F67ED06794"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "#text": "The establishment of the commemorative work shall be in accordance with ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": ".", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (commonly known as the ", "@entity-type": "uscode", "#text": "\n              ", "external-xref": {"#tail": " of title 40,\n\t\t\t United States Code", "@parsable-cite": "usc-chapter/40/89", "#text": "chapter 89", "@legal-doc": "usc-chapter"}, "@value": "usc-chapter/40/89"},