#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " determines that the proposed plan submitted under this\n\t\t\t\tparagraph, does not meet the requirements of this paragraph, the Secretary\n\t\t\t\tshall\u2014", "@entity-type": "federal-body", "#text": "Secretary\n\t\t\t\tof the Interior", "@entity-id": "1400"}, "#text": "if the "}, "enum": {"#tail": "\n              ", "#text": "(G)"}, "clause": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "advise the Vietnam\n\t\t\t\tVeterans Memorial Fund, Inc. not later than 30 days after receipt of the\n\t\t\t\tproposed plan of the reasons that such plan does not meet the requirements;\n\t\t\t\tand"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "H1F777F7CE0E64882BBAA9C5A759D09CB", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "allow the Vietnam\n\t\t\t\tVeterans Memorial Fund, Inc. to submit a revised donor recognition\n\t\t\t\tplan."}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "@id": "H358B42F115FA4B26A65096CB41D225DA", "#text": "\n                "}], "#text": "\n              ", "@id": "HB31541B2C41040CE914C5A30DB4C7198"}]}, "#text": "\n\t\t\t\t\t", "@id": "H6EB67AB5622344248D6DDB1255D13741"}, "#text": "\n        ", "@id": "H94607F0125E8498A824A26CABC5FA55A"}], "#text": "\n      ", "@id": "HF82642D81A344BD1BAC75152CADC9383"}], "#text": "\n\t\t", "@id": "H61E20147996F4950A105C734F0D1A9CE"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-02-06"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 588 RH: Vietnam Veterans Donor Acknowledgment Act of 2013"}, "{http://purl.org/dc/elements/1.1/}right