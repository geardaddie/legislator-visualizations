{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n    ", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "S. 530"}, "official-title": {"#tail": "\n  ", "#text": "To make participation in the American Community Survey\n\t\t  voluntary, except with respect to certain basic questions, and for other\n\t\t  purposes."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20130312", "#text": "March 12, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "committee-name": {"#tail": "\n      ", "@committee-id": "SSGA00", "#text": "Committee on Homeland\n\t\t\t Security and Governmental Affairs"}, "sponsor": {"#tail": "(for himself,", "#text": "Mr. Paul", "@name-id": "S348"}, "cosponsor": [{"#tail": ",", "#text": "Mr. McConnell", "@name-id": "S174"}, {"#tail": ", and", "#text": "Mr. Vitter", "@name-id": "S299"}, {"#tail": ") introduced the following bill; which was read twice and\n\t\t\t referred to the", "#text": "Mr.\n\t\t\t Johanns", "@name-id": "S321"}]}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": {"@section-type": "section-one", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Participation in American\n\t\t\t Community Survey"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended\n\t\t\t by adding at the end the following:", "@entity-type": "uscode", "#text": "\n            ", "external-xref": {"#tail": "of title 13, United States Code", "@parsable-