{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "H. R. 3694"}, "official-title": {"#tail": "\n  ", "#text": "To amend the Higher Education Act of 1965 to make technical improvements to the Net Price Calculator system so that prospective students may have a more accurate understanding of the true cost of college."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "I"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20131211", "#text": "December 11, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "committee-name": {"#tail": "\n      ", "@committee-id": "HED00", "#text": "Committee on Education and the Workforce"}, "sponsor": {"#tail": " (for himself, ", "#text": "Mr. Cummings", "@name-id": "C000984"}, "cosponsor": [{"#tail": ", and ", "#text": "Mr. Issa", "@name-id": "I000056"}, {"#tail": ") introduced the following bill; which was referred to the ", "#text": "Mr. Hinojosa", "@name-id": "H000636"}]}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-House", "@bill-type": "olc", "@dms-id": "HE6703537A6CA4B8DA26F616362F87737", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "Net Price Calculator Improvement Act"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H475AF89F391E4704B284DFB7324A1AFF"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended\u2014", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 132(h) of the Higher of Education Act of 1965", "@value": "Higher of Education Act of 1965/s:132/ss:h"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "uscode", "#text": "20 U.S.C. 1015a(h)", "@value": "usc/20/1015a/h"}, "@parsable-cite": "usc/20/1015a", "#text": "\n          ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Minimum Standards for Net Price Calculators"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " as ", "@entity-type": "act", "#text": "subsection (4)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:4"}, {"#tail": ";", "@entity-type": "act", "#text": "subsection (6)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:6", "@proposed": "true"}], "#text": "by redesignating "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@id": "H1DB716E949714DA8A1746EFB341B564C", "#text": "\n        "}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", by inserting before the period the following ", "@entity-type": "act", "#text": "paragraph (2)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:2"}, "#text": "in ", "quote": {"#tail": ";", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "paragraph (4)(B)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:4/sp:B"}, "#text": ", and, not later than 1 year after the date of enactment of the Net Price Calculator Improvement Act, shall meet the requirements of "}}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "@id": "HD1A64D7BC41F40229EDA85F50A60CE2C", "#text": "\n        "}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", by inserting after the first sentence the following: ", "@entity-type": "act", "#text": "paragraph (3)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:3"}, "#text": "in ", "quote": {"#tail": "; and", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "paragraph (4)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:4"}, "#text": "Not later than 1 year after the date of enactment of the Net Price Calculator Improvement Act, such calculator shall meet the requirements of "}}, "enum": {"#tail": "\n        ", "#text": "(3)"}, "@id": "H6237A18813FB40528609C7B3DA1D30C7", "#text": "\n        "}, {"#tail": "\n    ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " the following:", "@entity-type": "act", "#text": "paragraph (3)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:3"}, "#text": "by inserting after "}, "enum": {"#tail": "\n        ", "#text": "(4)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "paragraph": [{"#tail": "\n          ", "@id": "HBBF1CC8BFBAD4CBB8FACE757EF05AA3B", "enum": {"#tail": "\n            ", "#text": "(4)"}, "header": {"#tail": "\n            ", "#text": "Minimum requirements for net price calculators"}, "text": {"#tail": "\n            ", "#text": "Not later than 1 year after the date of enactment of the Net Price Calculator Improvement Act, a net price calculator for an institution of higher education shall, at a minimum, meet the following requirements:", "@display-inline": "yes-display-inline"}, "#text": "\n            ", "subparagraph": [{"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "The link for the calculator\u2014"}, "enum": {"#tail": "\n              ", "#text": "(A)"}, "clause": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": " and prominently, clearly, and conspicuously (in such size and contrast (such as shade) that it is readily noticeable and readable) posted in locations on the institution\u2019s website where information on costs and aid is provided (such as financial aid, prospective students, or tuition and fees web pages);", "#text": "net price calculator"}, "#text": "is clearly labeled as a "}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "H2737BE1A429D4CB69B6AF03F85731AF7", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "matches in size and font to the other prominent links on the primary menu; and"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "@id": "H9B2E1644E8FB4DB4ADAF173844292F45", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "may also be included on the institution\u2019s compliance web page, which contains information relating to compliance with Federal, State, and local laws."}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "@id": "H60AAAF00E4014222A6E6956395559093", "#text": "\n                "}], "#text": "\n              ", "@id": "H3527B13F690843D799551CB33514EEBC"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "The results screen for the calculator specifies the following information:"}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "clause": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ") for the institution, which is the most visually prominent figure on the results screen.", "@entity-type": "act", "#text": "subsection (a)(3)", "@value": "Higher of Education Act of 1965/s:132/ss:a/p:3"}, "#text": "Net price (as calculated under "}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "HFDC17B6F24DA461A9704257DD9EF848F", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "Cost of attendance, including\u2014"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "subclause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "tuition and fees;"}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "H21BE9275FBDD4A8B9791A3AB51F71823", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "average annual cost of room and board for the institution for a first-time, full-time undergraduate student enrolled in the institution;", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                  ", "#text": "(II)"}, "@id": "H598F8AEE5A1C4C98A1D681FE0D909DFD", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "average annual cost of books and supplies for a first-time, full-time undergraduate student enrolled in the institution; and", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                  ", "#text": "(III)"}, "@id": "H4F3E5CDB8C0E45E3926D12D6460C4E6F", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "estimated cost of other expenses (including personal expenses and transportation) for a first-time, full-time undergraduate student enrolled in the institution.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                  ", "#text": "(IV)"}, "@id": "H1F7A75CEBCE94F84809178A3B60C22A5", "#text": "\n                  "}], "#text": "\n                ", "@id": "HCC9593039A0845B794FD0E8609AD2315"}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "Estimated total need-based grant aid and merit-based grant aid, from Federal, State, and institutional sources, that may be available to a first-time, full-time undergraduate student.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "@id": "HCC7CF5CB570841D09E1030AD471289B5", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "clause (iii)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:4/sp:B/cl:iii", "@proposed": "true"}, "#text": "Percentage of the first-time, full-time undergraduate students enrolled in the institution that received any type of grant aid described in ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(iv)"}, "@id": "H05F153805C824251B4F93552570DE84D", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "paragraph (6)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:6"}, "#text": "The disclaimer described in "}, "enum": {"#tail": "\n                ", "#text": "(v)"}, "@id": "HE0AEF7D586E249EBBAE0FFFFC07275BE", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n                ", "#text": "In the case of a calculator that\u2014"}, "enum": {"#tail": "\n                ", "#text": "(vi)"}, "subclause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ") or educational benefits for active duty service members, such benefits are displayed on the results screen in a manner that clearly distinguishes them from the grant aid described in ", "@entity-type": "act", "#text": "section 480", "@value": "Higher of Education Act of 1965/s:480"}, {"#tail": "; or", "@entity-type": "act", "#text": "clause (iii)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:4/sp:B/cl:iii", "@proposed": "true"}], "#text": "includes questions to estimate a student\u2019s (or prospective student\u2019s) eligibility for veterans\u2019 education benefits (as defined in ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "H217783124A4A42778C5298F6D5D15959", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", the results screen indicates that certain students (or prospective students) may qualify for such benefits and includes a link to information about such benefits.", "@entity-type": "act", "#text": "subclause (I)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:4/sp:B/cl:vi/scl:I", "@proposed": "true"}, "#text": "does not include questions to estimate eligibility for the benefits described in "}, "enum": {"#tail": "\n                  ", "#text": "(II)"}, "@id": "H47561CDE10EF4F5199C13EC940FC9ACA", "#text": "\n                  "}], "#text": "\n                ", "@id": "HD27CE0B3E5904A4B8CD96DA8C3AB04B0"}], "#text": "\n              ", "@id": "HD9EF2CD2A46648E3ABD347045D365B98"}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "#text": "The institution populates the calculator with data from not later than 2 academic years prior to the most recent academic year."}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "@id": "HC53C5C22C8C3405086749F98F009650E", "#text": "\n              "}]}, {"#tail": "\n          ", "@id": "H53241B7EDA874A968EA3FC71AA3A72D4", "enum": {"#tail": "\n            ", "#text": "(5)"}, "header": {"#tail": "\n            ", "#text": "Prohibition on use of data collected by the net price calculator"}, "text": {"#tail": "\n            ", "#text": "A net price calculator for an institution of higher education shall\u2014", "@display-inline": "yes-display-inline"}, "#text": "\n            ", "subparagraph": [{"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "clearly indicate which questions are required to be completed for an estimate of the net price from the calculator;"}, "enum": {"#tail": "\n              ", "#text": "(A)"}, "@id": "H029D164C95D04B0ABF72EA5EDAA4E378", "#text": "\n              "}, {"#tail": "\n            ", "text": {"#tail": "\n            ", "term": {"#tail": "; and", "#text": "optional"}, "#text": "in the case of a calculator that requests contact information from users, clearly mark such requests as "}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "@id": "H8E3D2C4C692A46708C37D01CB52D6CC5", "#text": "\n              "}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "#text": "prohibit any personally identifiable information provided by users from being sold or made available to third parties."}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "@id": "HB48A913AB996451F9FF284A38D4D156E", "#text": "\n              "}]}], "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "HEB813B8927154C469ED638660BB9B97E"}, "#text": "\n        ", "@id": "HC9182213BEBA42599667E43DCC213455"}], "#text": "\n      ", "@id": "HBAA325450D57407780E68FE6D57BBB4A"}, {"#tail": "\n  ", "text": {"#tail": "\n      ", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is further amended by adding at the end the following:", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 132(h) of the Higher of Education Act of 1965", "@value": "Higher of Education Act of 1965/s:132/ss:h"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "uscode", "#text": "20 U.S.C. 1015a(h)", "@value": "usc/20/1015a/h"}, "@parsable-cite": "usc/20/1015a", "#text": "\n          ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "Universal Net Price Calculator"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n      ", "#text": "."}, "#tail": "\n    ", "paragraph": {"#tail": "\n        ", "@id": "H45B267C76780492089920BF66EB696A1", "enum": {"#tail": "\n          ", "#text": "(7)"}, "header": {"#tail": "\n          ", "#text": "Universal net price calculator"}, "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " may develop a universal net price calculator that\u2014", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "9100"}, "#text": "The ", "@display-inline": "yes-display-inline"}, "#text": "\n          ", "subparagraph": [{"#tail": "\n          ", "text": {"#tail": "\n          ", "#text": "enables users to answer one set of questions and receive net prices for any institution that is required to have a net price calculator under this subsection;"}, "enum": {"#tail": "\n            ", "#text": "(A)"}, "@id": "HA9869F2CDC364DC09FE53B605A8EF52B", "#text": "\n            "}, {"#tail": "\n          ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "act", "#text": "subparagraphs (B)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:7/sp:B", "@proposed": "true"}, {"#tail": " for each institution for which a net price is being sought; and", "@entity-type": "act", "#text": "(C) of paragraph (4)", "@value": "Higher of Education Act of 1965/s:132/ss:h/p:7/sp:C", "@proposed": "true"}], "#text": "provides the information required under "}, "enum": {"#tail": "\n            ", "#text": "(B)"}, "@id": "H90F192EF20A7455194464FF3DAFBA15F", "#text": "\n            "}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "#text": "is tested by students and families and evaluated by financial aid administrators and others in the field of postsecondary education before being finalized and publicly released."}, "enum": {"#tail": "\n            ", "#text": "(C)"}, "@id": "H0B7D2ADEAEBE414DAF28450311A178DF", "#text": "\n            "}]}, "@display-inline": "no-display-inline", "#text": "\n        ", "@id": "H77DAC3D2D4CB4348BDEE60BB2EACDAEA"}, "#text": "\n      ", "@id": "H55CBDDB621F84059A8CEC8EDDBD9EEC2"}], "#text": "\n    ", "@id": "HBD753C25022E4D9998DA2C1EE3744276"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-12-11"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 3694 IH: Net Price Calculator Improvement Act"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}