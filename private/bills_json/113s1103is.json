          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "quote": [{"#tail": " and inserting ", "#text": "peat), and"}, {"#tail": ".", "#text": "peat) and"}], "#text": "by striking\n\t\t\t "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@id": "HA270437E8B1E4B0AAE7B02E07208B8D8", "#text": "\n          "}], "#text": "\n        ", "@id": "H030416D5870347229888986D56689826"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "#text": "The amendments made by this section shall apply to any sale\n\t\t\t or use of liquefied natural gas after 14 days after the date of the enactment\n\t\t\t of this Act."}, "enum": {"#tail": "\n        ", "#text": "(d)"}, "header": {"#tail": "\n        ", "#text": "Effective\n\t\t\t date"}, "#text": "\n        ", "@id": "H0DC212ED8610447CA2321DFE59C95015"}], "#text": "\n      ", "@id": "H92D9B10D54B74A5388D86B5959185645"}], "#text": "\n\t\t", "@id": "H5A846FE806C0495980DAD9FDF9C86767"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-06-06"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 S1103 IS: LNG Excise Tax Equalization Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. Senate"}, "#text": "\n"}, "#text": "\n"}}}