{"resolution": {"@star-print": "no-star-print", "form": {"current-chamber": {"#tail": "\n\t\t", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "@display": "yes", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "H. CON. RES. 23"}, "official-title": {"#tail": "\n\t", "@display": "yes", "#text": "Expressing the sense of Congress regarding\n\t\t  the conditions for the United States becoming a signatory to the United Nations\n\t\t  Arms Trade Treaty, or to any similar agreement on the arms\n\t\t  trade."}, "session": {"#tail": "\n\t\t", "@display": "yes", "#text": "1st Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "IV"}, "action": {"action-date": {"#tail": "\n\t\t\t", "@date": "20130313", "#text": "March 13, 2013"}, "#tail": "\n\t\t", "@display": "yes", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "committee-name": {"#tail": "\n      ", "@committee-id": "HFA00", "#text": "Committee on Foreign\n\t\t\t Affairs"}, "sponsor": {"#tail": " (for\n\t\t\t himself, ", "#text": "Mr. Kelly", "@name-id": "K000376"}, "cosponsor": [{"#tail": ",\n\t\t\t ", "#text": "Mr. Westmoreland", "@name-id": "W000796"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Graves of Missouri", "@name-id": "G000546"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Griffith of Virginia", "@name-id": "G000568"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Duncan of South Carolina", "@name-id": "D000615"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Broun of Georgia", "@name-id": "B001262"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Marino", "@name-id": "M001179"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Carter", "@name-id": "C001051"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Huelskamp", "@name-id": "H001057"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Hultgren", "@name-id": "H001059"}, {"#tail": ",\n\t\t\t ", "#text": "Mrs. Hartzler", "@name-id": "H001053"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Michaud", "@name-id"