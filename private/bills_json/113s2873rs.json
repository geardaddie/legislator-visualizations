{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "S. 2873"}, "official-title": {"#tail": "\n  ", "#text": "To authorize the Secretary of the Interior to acknowledge contributions at units of the National\n\t\t\t Park System."}, "session": {"#tail": "\n    ", "#text": "2d Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "II"}, "action": [{"action-date": {"#tail": "\n      ", "@date": "20140918", "#text": "September 18, 2014"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "committee-name": {"@deleted-display-style": "strikethrough", "#tail": "\n      ", "@committee-id": "SSEG00", "@added-display-style": "italic", "#text": "Committee on Energy and Natural Resources"}, "sponsor": {"#tail": " (for himself, ", "#text": "Mr. Coburn", "@name-id": "S301"}, "cosponsor": [{"#tail": ", and ", "#text": "Mr. Warner", "@name-id": "S327"}, {"#tail": ") introduced the following bill; which was read twice and referred to the ", "#text": "Mr. Enzi", "@name-id": "S254"}]}, "#text": "\n      "}, {"#tail": "\n    ", "action-desc": {"#tail": "\n      ", "#text": "Reported by ", "sponsor": {"#tail": ", with an amendment", "#text": "Ms. Landrieu", "@name-id": "S258"}}, "action-instruction": {"#tail": "\n    ", "#text": "Strike out all after the enacting clause and insert the part printed in italic"}, "action-date": {"#tail": "\n      ", "#text": "December 10, 2014"}, "#text": "\n      ", "@stage": "Reported-in-Senate"}], "associated-doc": {"#tail": "\n    ", "#text": "[Report No. 113\u2013301]", "@role": "report"}, "calendar": {"#tail": "\n    ", "#text": "Calendar No. 641"}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Reported-in-Senate", "endorsement": {"action-date": {"#tail": "\n    ", "#text": "December 10, 2014"}, "#tail": "\n", "action-desc": {"#tail": "\n  ", "#text": "Reported with an amendment"}, "#text": "\n    "}, "@dms-id": "A1", "@public-private": "public", "legis-body": [{"#tail": "\n  ", "section": [{"@committee-id": "SSEG00", "@changed": "deleted", "@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "National Park System Donor Acknowledgment Act of 2014"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "@reported-display-style": "strikethrough", "#text": "\n      ", "@id": "id99a95d96a5824d15a9dbd6b96fe9d1cf"}, {"@committee-id": "SSEG00", "@changed": "deleted", "#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "In this Act:", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n      ", "subparagraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "term": {"#tail": " means a statement, logo, trademark, proper legal name, or other reasonable form of credit\n\t\t\t acknowledging a contribution by a donor.", "#text": "donor acknowledgment"}, "#text": "The term "}, "enum": {"#tail": "\n          ", "#text": "(A)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "#text": "\n          ", "@id": "id82213a3387174bffbf74cc2241938ffb"}, {"#tail": "\n      ", "clause": [{"#tail": "\n          ", "text": {"#tail": "\n          ", "#text": "a sign or other fixture that would block or obstruct a natural or historic site or view; or"}, "enum": {"#tail": "\n            ", "#text": "(i)"}, "@id": "idec7036e89e404ca6971e87303adc5e08", "#text": "\n            "}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "#text": "a statement or credit that promotes a political candidate or issue."}, "enum": {"#tail": "\n            ", "#text": "(ii)"}, "@id": "id33f26fca03084d859b54e640a4163423", "#text": "\n            "}], "enum": {"#tail": "\n          ", "#text": "(B)"}, "header": {"#tail": "\n          ", "#text": "Exclusions"}, "text": {"#tail": "\n          ", "term": {"#tail": " does not include\u2014", "#text": "donor acknowledgment"}, "#text": "The term "}, "#text": "\n          ", "@id": "idcf3083462e374ac0869de9188b6d223a"}], "enum": {"#tail": "\n        ", "#text": "(1)"}, "header": {"#tail": "\n        ", "#text": "Donor acknowledgment"}, "#text": "\n        ", "@id": "id92a61fde2c244e73a8f9728b7b14502f"}, {"#tail": "\n      ", "subparagraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "term": {"#tail": " means a structure at a unit of the National Park System.", "#text": "eligible structure"}, "#text": "The term "}, "enum": {"#tail": "\n          ", "#text": "(A)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "#text": "\n          ", "@id": "id3a9b3a3c6b2c4c2ca4e2944e5613a22a"}, {"#tail": "\n        ", "clause": [{"#tail": "\n          ", "text": {"#tail": "\n          ", "#text": "a visitor center;"}, "enum": {"#tail": "\n            ", "#text": "(i)"}, "@id": "id04431c74143a43999983ab88ffaee855", "#text": "\n            "}, {"#tail": "\n          ", "text": {"#tail": "\n          ", "#text": "an administrative structure; and"}, "enum": {"#tail": "\n            ", "#text": "(ii)"}, "@id": "id7cedfc3b3cf14974b7ddcde69ca245e3", "#text": "\n            "}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "#text": "a specific room or section of a visitor center or an administrative structure."}, "enum": {"#tail": "\n            ", "#text": "(iii)"}, "@id": "id5a28fcda78cc41d9936335bdea058984", "#text": "\n            "}], "enum": {"#tail": "\n          ", "#text": "(B)"}, "header": {"#tail": "\n          ", "#text": "Inclusions"}, "text": {"#tail": "\n          ", "term": {"#tail": " includes\u2014", "#text": "eligible structure"}, "#text": "The term "}, "#text": "\n          ", "@id": "id901a3d789ca148a986bf7bebbf258b7f"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "term": {"#tail": " does not include a commemorative work (as defined in ", "#text": "eligible structure"}, "#text": "The term ", "external-xref": {"#tail": " of title 40, United States\n\t\t\t Code).", "@parsable-cite": "usc/40/8902", "#text": "section 8902(a)", "@legal-doc": "usc"}}, "enum": {"#tail": "\n          ", "#text": "(C)"}, "header": {"#tail": "\n          ", "#text": "Exclusion"}, "#text": "\n          ", "@id": "idd041c7f5b9a0483299774fc48d9d7ce1"}], "enum": {"#tail": "\n        ", "#text": "(2)"}, "header": {"#tail": "\n        ", "#text": "Eligible structure"}, "#text": "\n        ", "@id": "idb8b118cb561b4c5b864bdaa1e09c2aec"}, {"#tail": "\n      ", "subparagraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "term": {"#tail": " means a component that contributes to the character significance of a landscape.", "#text": "landscape feature"}, "#text": "The term "}, "enum": {"#tail": "\n          ", "#text": "(A)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "#text": "\n          ", "@id": "id6b0088fe9ab347e18585fde7caf15f7a"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "term": {"#tail": " includes land, water, vegetation, walls, pathways, stairways, and plazas.", "#text": "landscape feature"}, "#text": "The term "}, "enum": {"#tail": "\n          ", "#text": "(B)"}, "header": {"#tail": "\n          ", "#text": "Inclusions"}, "#text": "\n          ", "@id": "ida0cac5bbd3274f69b1717a72413ad2d5"}], "enum": {"#tail": "\n        ", "#text": "(3)"}, "header": {"#tail": "\n        ", "#text": "Landscape feature"}, "#text": "\n        ", "@id": "id38375eced7ce42ec854efc3a5870bb37"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "term": {"#tail": " means the Secretary of the Interior.", "#text": "Secretary"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(4)"}, "header": {"#tail": "\n        ", "#text": "Secretary"}, "#text": "\n        ", "@id": "id3d58555d9fc64bfabac77b66bc59d1fa"}], "@reported-display-style": "strikethrough", "#text": "\n      ", "@id": "idb74ae83c97b64a6faf2ebefcb2272510"}, {"@committee-id": "SSEG00", "@changed": "deleted", "#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "Donor acknowledgments at structures in units of the national park system"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "#text": "Subject to subsection (b), the Secretary may authorize donor acknowledgments at an eligible\n\t\t\t structure to recognize contributions for a museum collection or landscape\n\t\t\t feature at a unit of the National Park System."}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "#text": "\n        ", "@id": "id573356a1069a4ad89a0317718b8f93ac"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "Donor acknowledgments under subsection (a) shall be displayed\u2014"}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Requirements"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "in a manner that is approved by the Secretary, in consultation with the Superintendent at the unit\n\t\t\t of the National Park System in which the eligible structure, museum\n\t\t\t collection, or landscape feature is located, after taking into account any\n\t\t\t input from the donating entity; and"}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "id8a9ca8cb608f490baf88345c3227c067", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "for a period of time, as determined by the Secretary, in consultation with the Superintendent at\n\t\t\t the unit of the National Park System in which the eligible structure,\n\t\t\t museum collection, or landscape feature is located, that is commensurate\n\t\t\t with the amount of the contribution and the life of the eligible\n\t\t\t structure."}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@id": "idd902fe4459ba4f42b9ba2b46c87afe11", "#text": "\n          "}], "#text": "\n        ", "@id": "ide2a0370085d54bffa8cc6f889e8e1a80"}, {"#tail": "\n      ", "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "Placement"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "Donor acknowledgments under subsection (a) may be affixed to benches, furnishings, bricks,\n\t\t\t vehicles, walls, pathways, plazas, and staircases."}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "#text": "\n          ", "@id": "id506A3CE67E184AF39A0C92EDA90A9E14"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "Any donor acknowledgment under subsection (a) associated with land, water, vegetation, an item in a\n\t\t\t museum collection, or a historic structure shall be freestanding."}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Limitation"}, "#text": "\n          ", "@id": "id032a40aea7144914ac1417a7aec1c280"}], "#text": "\n        ", "@id": "id7e4d7c505cae458293b2c91c5652cb18"}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "#text": "The Secretary may authorize the use of donor acknowledgments under this section to include\n\t\t\t acknowledgments on digital and media platforms, including online\n\t\t\t applications and web-based product downloads relating to a specific unit\n\t\t\t of the National Park System."}, "enum": {"#tail": "\n        ", "#text": "(d)"}, "header": {"#tail": "\n        ", "#text": "Expansion of donor acknowledgments"}, "#text": "\n        ", "@id": "id5c32b5089c54424dae74aa65ddf083e0"}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "#text": "Not later than 180 days after the date of enactment of this Act, the Secretary shall implement this\n\t\t\t section."}, "enum": {"#tail": "\n        ", "#text": "(e)"}, "header": {"#tail": "\n        ", "#text": "Implementation"}, "#text": "\n        ", "@id": "id6d7c21d43c2e4e87bde62f04a4e4869f"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "Nothing in this section requires the Secretary to accept a donation."}, "enum": {"#tail": "\n        ", "#text": "(f)"}, "header": {"#tail": "\n        ", "#text": "Effect of section"}, "#text": "\n        ", "@id": "id54c33595f93d4471848e401d7870b64e"}], "@reported-display-style": "strikethrough", "#text": "\n      ", "@id": "id4358db1d1d7e460da7b719407f6c1793"}, {"@committee-id": "SSEG00", "@changed": "deleted", "#tail": "\n  ", "text": {"#tail": "\n      ", "#text": "\n        ", "external-xref": {"#tail": " of title 40, United States Code, is amended\u2014", "@parsable-cite": "usc/40/8905", "#text": "Section 8905", "@legal-doc": "usc"}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "4."}, "header": {"#tail": "\n      ", "#text": "Donor acknowledgments to be displayed at commemorative works and landscape features"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "#text": "in subsection (b), by striking paragraph (7); and"}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@id": "id78911a455ed448429101f95dbe67d188", "#text": "\n        "}, {"#tail": "\n    ", "text": {"#tail": "\n        ", "#text": "by adding at the end the following:"}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "quoted-block": {"@style": "OLC", "@committee-id": "SSEG00", "@changed": "deleted", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "@reported-display-style": "strikethrough", "subsection": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "(c)"}, "header": {"#tail": "\n            ", "#text": "Donor contributions"}, "paragraph": [{"#tail": "\n            ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": " means a component that contributes to the character or significance of a landscape.", "#text": "landscape feature"}, "#text": "The term "}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "header": {"#tail": "\n                ", "#text": "In general"}, "#text": "\n                ", "@id": "iddea6d40316634c89a0e467ff6cf4c5da"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "term": {"#tail": " includes walls, pathways, stairways,  plazas, and other features that are considered to be\n\t\t\t components of a landscape in accordance with applicable National Park\n\t\t\t Service regulations.", "#text": "landscape feature"}, "#text": "The term "}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "header": {"#tail": "\n                ", "#text": "Inclusions"}, "#text": "\n                ", "@id": "idf39ce9b355f04299b621eb68fbf983c2"}], "enum": {"#tail": "\n              ", "#text": "(1)"}, "header": {"#tail": "\n              ", "#text": "Definition of landscape feature"}, "#text": "\n              ", "@id": "id8dfa22b0911141e4b4154103fec23817"}, {"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "Except as otherwise provided in this subsection, the Secretary of the Interior or Administrator of\n\t\t\t General Services, as applicable, may acknowledge or permit a sponsor to\n\t\t\t acknowledge, donor contributions at the commemorative work."}, "enum": {"#tail": "\n              ", "#text": "(2)"}, "header": {"#tail": "\n              ", "#text": "Acknowledgment of donor contribution"}, "#text": "\n              ", "@id": "id50308c6344b54918ae45c0800206a00a"}, {"#tail": "\n            ", "@id": "idb7413b919b774320bb64d8dc25a50d78", "enum": {"#tail": "\n              ", "#text": "(3)"}, "header": {"#tail": "\n              ", "#text": "Requirements"}, "text": {"#tail": "\n              ", "#text": "An acknowledgment under paragraph (2) shall\u2014"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "be displayed\u2014"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "clause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "inside an ancillary structure associated with the commemorative work; or"}, "enum": {"#tail": "\n                  ", "#text": "(i)"}, "@id": "id6d8fc1d7920c48a792ccf56ced92033c", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "as part of a  landscape feature; and"}, "enum": {"#tail": "\n                  ", "#text": "(ii)"}, "@id": "id01d2b998910248779a970c3c8cba1f97", "#text": "\n                  "}], "#text": "\n                ", "@id": "idca3d84311c1c494b8f19c6de3d96fed3"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "conform to applicable National Park Service or General Services Administration guidelines for donor\n\t\t\t recognition, as applicable."}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "id37d96a2904af4ff4b36565a99cdd795f", "#text": "\n                "}]}, {"#tail": "\n            ", "@id": "id09dd254c5f80474990d70ec1fa6f3a5e", "enum": {"#tail": "\n              ", "#text": "(4)"}, "header": {"#tail": "\n              ", "#text": "Limitations"}, "text": {"#tail": "\n              ", "#text": "An acknowledgment under paragraph (2) shall\u2014"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "be limited to an appropriate statement or credit recognizing the contribution;"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "id119bb61751664d309eb73aae447e9282", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "be displayed in a form approved by\u2014"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "clause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "the National Mall and Memorial Parks Donor Recognition Plan or other applicable National Park\n\t\t\t Service guidelines for donor recognition; and"}, "enum": {"#tail": "\n                  ", "#text": "(i)"}, "@id": "idFE9E3FE5786B46F597B2ADBE2A931653", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "General Services Administration guidelines;"}, "enum": {"#tail": "\n                  ", "#text": "(ii)"}, "@id": "idE80DAE6A907A4402A3A490F4C0985FC5", "#text": "\n                  "}], "#text": "\n                ", "@id": "id6ee7652cb0974889a86076a960cdd354"}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "be displayed for a period of up to 10 years, with the display period to be commensurate with the\n\t\t\t level of the contribution, as determined in accordance with the plan and\n\t\t\t guidelines described in subparagraph (B); and"}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@id": "idfa532cd575c64acdb8bf046257598d44", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "be freestanding."}, "enum": {"#tail": "\n                ", "#text": "(D)"}, "@id": "id3c52351fc7ca4ebb92ca614f6cddb2cd", "#text": "\n                "}]}, {"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "The sponsor shall bear all expenses related to the display of donor acknowledgments under paragraph\n\t\t\t (2)."}, "enum": {"#tail": "\n              ", "#text": "(5)"}, "header": {"#tail": "\n              ", "#text": "Cost"}, "#text": "\n              ", "@id": "id3cecdd604fa24ead8a8092e5e8c4f02f"}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "#text": "This subsection shall apply to any commemorative work dedicated after January 1, 2010."}, "enum": {"#tail": "\n              ", "#text": "(6)"}, "header": {"#tail": "\n              ", "#text": "Applicability"}, "#text": "\n              ", "@id": "id28458bcf54c4456f891bbd55976b0045"}], "#text": "\n            ", "@id": "id4fe572f13f9b4b85a85989d9a3f06e27"}, "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "id4dc3c18efc7b4cd98d65d88af85df7fa"}, "#text": "\n        ", "@id": "id4cc935652444472e8e23cd544b0f557a"}], "@reported-display-style": "strikethrough", "#text": "\n      ", "@id": "idcf7843593e664bb2b39ce8949d8d482a"}], "#text": "\n    "}, {"#tail": "\n  ", "section": [{"@committee-id": "SSEG00", "@changed": "added", "@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "National Park System Donor Acknowledgment Act", "@value": "National Park System Donor Acknowledgment Act", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "@reported-display-style": "italic", "#text": "\n      ", "@id": "id119b84ccd6294cc09848a5498c4cac43"}, {"@committee-id": "SSEG00", "@changed": "added", "#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "In this Act:", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "term": {"#tail": " means an appropriate statement or credit acknowledging a donation.", "#text": "donor acknowledgment"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "header": {"#tail": "\n        ", "#text": "Donor acknowledgment"}, "#text": "\n        ", "@id": "idb747ed864860463cbb8255b2a62eda60"}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-parent-id": "1443", "@entity-type": "federal-body", "#text": "National Park System", "@value": "National Park System"}, "term": {"#tail": " includes each program and individual unit of the ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-parent-id": "1443", "@entity-type": "federal-body", "#text": "National Park System", "@value": "National Park System"}, "#text": "\n            "}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "header": {"#tail": "\n        ", "#text": "National Park System"}, "#text": "\n        ", "@id": "id766be0e5aee84897aa9c3107c0bd72fb"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "term": {"#tail": " means the ", "#text": "Secretary"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(3)"}, "header": {"#tail": "\n        ", "#text": "Secretary"}, "#text": "\n        ", "@id": "idb6848b627c8b44ff8c528fc65baaaae7"}], "@reported-display-style": "italic", "#text": "\n      ", "@id": "id99c0de5eda0d414aa8040f3be75ab804"}, {"@committee-id": "SSEG00", "@changed": "added", "#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "Donor acknowledgments in units of National Park System"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " may authorize a donor acknowledgment to recognize a donation to\u2014", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, "#text": "The "}, "enum": {"#tail": "\n        ", "#