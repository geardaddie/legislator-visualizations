"text": {"#tail": "\n              ", "#text": "An acknowledgment under paragraph (1) shall\u2014"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "be limited to an appropriate statement or credit recognizing the\n\t\t\t contribution;"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "id9a5eca6beb764d74a4a657e48302f278", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "federal-body", "#text": "National Park Service", "@entity-id": "1443"}, {"#tail": " guidelines;", "@entity-type": "federal-body", "#text": "General Services   \n\t\t\t\t     Administration", "@entity-id": "4700"}], "#text": "be displayed in a form in accordance with "}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "id204434afbb8241a180240511966b8c1d", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "be displayed for a period of up to 10 years, with the display period to be commensurate with the\n\t\t\t level of the contribution, as\tdetermined\n\t\t\t in accordance with the plan and guidelines described in subparagraph (B);"}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@id": "idb3860ab3eb87420fae8d7adbd324d7b2", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "be freestanding; and"}, "enum": {"#tail": "\n                ", "#text": "(D)"}, "@id": "idc18d1f43f7674fc3aa30a3f9f205cbfa", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n                ", "#text": "not be affixed to\u2014"}, "enum": {"#tail": "\n                ", "#text": "(E)"}, "clause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "any landscape feature at the commemorative work; or"}, "