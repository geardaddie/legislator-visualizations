 [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "\n\t\t\t (relating to deduction for trade or business expenses) is amended by inserting\n\t\t\t after ", "@entity-type": "act", "#text": "Section 162 of the Internal Revenue Code of 1986", "@value": "Internal Revenue Code of 1986/s:162"}, {"#tail": " the following new subsection:", "@entity-type": "act", "#text": "subsection (h)", "@value": "Internal Revenue Code of 1986/s:162/ss:h"}], "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In\n\t\t\t general"}, "quoted-block": {"#tail": "\n\t\t\t", "#text": "\n\t\t\t\t\t", "@id": "H9F0DC9C341964645B6BF810597C65BB2", "subsection": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "(i)"}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Excessive\n\t\t\t\tcompensation"}, "paragraph": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "No deduction shall be allowed under this chapter for any\n\t\t\t\texcessive compensation with respect to any full-time employee."}, "enum": {"#tail": "\n              ", "#text": "(1)"}, "header": {"#tail": "\n              ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n              ", "@id": "HC1131F0000E449D1BFFF936519AB8EF4"}, {"#tail": "\n            ", "@id": "H5D6280A041954F528D23D85E182BFA9A", "enum": {"#tail": "\n              ", "#text": "(2)"}, "header": {"#tail": "\n              ", "#text": "Excessive\n\t\t\t\tcompensation"}, "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": " means, with respect to any employee, the\n\t\t\t\tamount by which\u2014", "#text": "excessive compensation"}, "#text": "For purposes of this subsection, the term\n\t\t\t\t"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "the compensation\n\t\t\t\tfor services performed by such employee during the taxable year, exceeds"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "H2D586211D435429EB8ABF93DDC6F01E2", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "the greater\n\t\t\t\tof\u2014"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "clause": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "an\n\t\t\t\tamount equal to 25 times the lowest compensation for services performed by any\n\t\t\t\tother full-time employee during such taxable year, or"}, "enum": {"#tail": "\n                  ", "#text": "(i)"}, "@id": "HBEC0EA19725B4542B1C5917331A1A030", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "$500,000."}, "enum": {"#tail": "\n                  ", "#text": "(ii)"}, "@id": "H87B571D40AFD42F8B88AC3D5CA5B942B", "#text": "\n                  "}], "#text": "\n                ", "@id": "HC4E6E7F63CDC4F9FBD941CA23DDBF277"}]}, {"#tail": "\n            ", "@id": "H327CC5E11A8D47C29CA7601AD0F56B5C", "enum": {"#tail": "\n              ", "#text": "(3)"}, "header": {"#tail": "\n              ", "#text": "Definitions and\n\t\t\t\tspecial rules"}, "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "For purposes of this subsection\u2014"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "clause": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " determines is\n\t\t\t\tappropriate.", "@entity-type": "federal-body", "#text": "Secretary of the Treasury", "@entity-id": "2000"}, "term": {"#tail": " includes wages, salary, deferred compensation,\n\t\t\t\tretirement contributions, options, bonuses, property, and any other form of\n\t\t\t\tcompensation or bonus that the ", "#text": "compensation"}, "#text": "The term\n\t\t\t\t", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n   