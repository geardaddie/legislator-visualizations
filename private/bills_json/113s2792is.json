": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", the term\n\t\t\t\t", "@entity-type": "act", "#text": "paragraph (1)(A)", "@value": "Internal Revenue Code of 1986/s:45S/ss:c/p:1/sp:A", "@proposed": "true"}, {"#tail": ", after consultation\n\t\t\t with the\n\t\t\t\t", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "2000"}, {"#tail": ", may prescribe) that satisfies the criteria for\n\t\t\t an apprenticeable occupation under ", "@entity-type": "federal-body", "#text": "Secretary of Labor", "@entity-id": "1600"}], "term": {"#tail": " means a skilled trade occupation in a\n\t\t\t\thigh-demand mechanical, technical, healthcare, or technology field\n\t\t\t (or such other\n\t\t\t occupational field as the ", "#text": "qualified occupation"}, "#text": "For purposes of ", "external-xref": {"#tail": " of title 29 of the Code of\n\t\t\t Federal Regulations.", "@parsable-cite": "usc/29/29", "#text": "section 29.4", "@legal-doc": "usc"}}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Qualified\n\t\t\t\toccupation"}, "#text": "\n                ", "@id": "idF9191FE26F8242E4B90EC039E1CF9B07"}], "#text": "\n              ", "@id": "id77D08BF7E440431CAD7CC85D4913102B"}, {"#tail": "\n            ", "enum": {"#tail": "\n              ", "#text": "(d)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Apprenticeship\n\t\t\t\tagreement"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": " means an agreement between a qualified\n\t\t\t\tindividual and an employer that satisfies the criteria under\n\t\t\t ", "#text": "apprenticeship agreement"}, "#text": "For purposes of this section, the term\n\t\t\t\t", "external-xref": {"#tail": " of title 29 of the Code of Federal Regulations.", "@parsable-cite": "usc/29/29", "#text": "section 29.7", "@legal-doc": "usc"}}, "enum": {"#tail": "\n