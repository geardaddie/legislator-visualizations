{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n    ", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "S. 555"}, "official-title": {"#tail": "\n  ", "#text": "To amend the Americans with Disabilities Act of 1990 to\n\t\t  require captioning and video description at certain movie\n\t\t  theaters."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20130313", "#text": "March 13, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "sponsor": {"#tail": "introduced\n\t\t\t the following bill; which was read twice and referred to the", "#text": "Mr. Harkin", "@name-id": "S172"}, "committee-name": {"#tail": "\n      ", "@committee-id": "SSHR00", "#text": "Committee on Health, Education, Labor,\n\t\t\t and Pensions"}}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@dms-id": "A1", "@public-private": "public", "legis-body": {"#tail": "\n", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": [{"short-title": {"#tail": "\n        ", "#text": "Captioning and Image Narration to\n\t\t\t Enhance Movie Accessibility Act"}, "#tail": "or the", "#text": "\n          "}, {"short-title": {"#tail": "\n        ", "#text": "CINEMA\n\t\t\t Act"}, "#tail": ".", "#text": "\n          "}], "#text": "This Act may be cited as\n\t\t\t the", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short\n\t\t\t title"}, "#text": "\n      ", "@id": "S1"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": "is\n\t\t\t amended\u2014", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "(", "@entity-type": "act", "#text": "Section 302(b)\n\t\t\t of the Americans with Disabilities Act of 1990", "@value": "Americans with Disabilities Act of 1990/s:302/ss:b"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "uscode", "#text": "42 U.S.C. 12182(b)", "@value": "usc/42/12182/b"}, "@parsable-cite": "usc/42/12182", "#text": "\n            ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Movie Theater\n\t\t\t Accessibility"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "as", "@entity-type": "act", "#text": "paragraph (3)", "@value": "Americans with Disabilities Act of 1990/s:302/ss:b/p:3"}, {"#tail": "; and", "@entity-type": "act", "#text": "paragraph\n\t\t\t (4)", "@value": "Americans with Disabilities Act of 1990/s:302/ss:b/p:4", "@proposed": "true"}], "#text": "by redesignating", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@id": "id23E3A660BA534028A4DF0D90106B0F33", "#text": "\n        "}, {"#tail": "\n    ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "the following:", "@entity-type": "act", "#text": "paragraph (2)", "@value": "Americans with Disabilities Act of 1990/s:302/ss:b/p:2"}, "#text": "by inserting\n\t\t\t after"}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "paragraph": {"#tail": "\n          ", "subparagraph": [{"#tail": "\n            ", "clause": [{"#tail": "\n              ", "subclause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "allows an\n\t\t\t\tindividual who is deaf or hard of hearing to have access to the content of a\n\t\t\t\tmotion picture; and"}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "idC3451D7B2CCE48179A066D0E01CA4517", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "allows that\n\t\t\t\taccess by displaying, through an individual device or individually used\n\t\t\t\ttechnology, all of the audio portion of the motion picture (including\n\t\t\t\tdisplaying the dialogue and any narration, as well as descriptions of on- and\n\t\t\t\toff-screen sounds such as sound effects, music, or lyrics for music, and\n\t\t\t\tinformation identifying the character who is speaking) as text that can be\n\t\t\t\teffectively viewed and controlled by that individual while the individual\n\t\t\t\tsimultaneously watches the motion picture."}, "enum": {"#tail": "\n                  ", "#text": "(II)"}, "@id": "idC6E90F1890FE48E0B604A3208F133896", "#text": "\n                  "}], "enum": {"#tail": "\n                ", "#text": "(i)"}, "header": {"#tail": "\n                ", "#text": "Closed\n\t\t\t\tcaptioning"}, "text": {"#tail": "\n                ", "term": {"#tail": "means a\n\t\t\t\tmethod, process, or mechanism, which may include a device, that\u2014", "#text": "closed captioning"}, "#text": "The term"}, "#text": "\n                ", "@id": "id413ab24ba97747699d593c1f42bcf763"}, {"#tail": "\n              ", "subclause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "that operates a\n\t\t\t\tcomplex of 2 or more movie theaters, screening rooms, or similar venues, at a\n\t\t\t\tsingle location, that are used for the exhibition of copyrighted motion\n\t\t\t\tpictures, if such exhibition is open to the public; and"}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "id7C9537885EB242A58731349E41D3475C", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "whose operations\n\t\t\t\taffect commerce."}, "enum": {"#tail": "\n                  ", "#text": "(II)"}, "@id": "idC327846A3C8F415AB4A0EBA20F6B4FB2", "#text": "\n                  "}], "enum": {"#tail": "\n                ", "#text": "(ii)"}, "header": {"#tail": "\n                ", "#text": "Covered\n\t\t\t\tentity"}, "text": {"#tail": "\n                ", "term": {"#tail": "means an\n\t\t\t\tentity\u2014", "#text": "covered entity"}, "#text": "The term"}, "#text": "\n                ", "@id": "id681c42d9794c44428e639c00e23e244d"}, {"#tail": "\n              ", "subclause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "allows an\n\t\t\t\tindividual who is deaf or hard of hearing to have access to the content of a\n\t\t\t\tmotion picture; and"}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "idBF2EFC56FE1B4EB59BAB5B7B728DB880", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "allows that\n\t\t\t\taccess by openly displaying on the movie screen involved all of the audio\n\t\t\t\tportion of the motion picture (including displaying the dialogue and any\n\t\t\t\tnarration, as well as descriptions of on- and off-screen sounds such as sound\n\t\t\t\teffects, music, or lyrics for music, and information identifying the character\n\t\t\t\twho is speaking) as text that can be effectively viewed by that individual and\n\t\t\t\tother members of the audience while the individual and members simultaneously\n\t\t\t\twatch the motion picture."}, "enum": {"#tail": "\n                  ", "#text": "(II)"}, "@id": "id932A290E411F479896928F6B5E2D22B4", "#text": "\n                  "}], "enum": {"#tail": "\n                ", "#text": "(iii)"}, "header": {"#tail": "\n                ", "#text": "Open\n\t\t\t\tcaptioning"}, "text": {"#tail": "\n                ", "term": {"#tail": "means a method,\n\t\t\t\tprocess, or mechanism that\u2014", "#text": "open captioning"}, "#text": "The term"}, "#text": "\n                ", "@id": "id444b31834f6c41d8a28433e31c68d853"}, {"#tail": "\n            ", "subclause": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "allows an\n\t\t\t\tindividual who is blind or visually impaired to have access to the key visual\n\t\t\t\telements of a motion picture (such as actions, settings, facial expressions,\n\t\t\t\tcostumes, and scene changes); and"}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "idE3BEDBD53B174EA68B99267D99E74976", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "allows that\n\t\t\t\taccess through the provision of contemporaneous audio narrated descriptions of\n\t\t\t\tthose elements during the natural pauses in the audio portion of the motion\n\t\t\t\tpicture, or during the audio portion if necessary."}, "enum": {"#tail": "\n                  ", "#text": "(II)"}, "@id": "id5F8286A1AFC8433EBDE482B17C702862", "#text": "\n                  "}], "enum": {"#tail": "\n                ", "#text": "(iv)"}, "header": {"#tail": "\n                ", "#text": "Video\n\t\t\t\tdescription"}, "text": {"#tail": "\n                ", "term": {"#tail": "means a\n\t\t\t\tmethod, process, or mechanism, including a device, that\u2014", "#text": "video description"}, "#text": "The term"}, "#text": "\n                ", "@id": "id7d14923120e64f8b9f28708317860d06"}], "enum": {"#tail": "\n              ", "#text": "(A)"}, "header": {"#tail": "\n              ", "#text": "Definitions"}, "text": {"#tail": "\n              ", "#text": "In\n\t\t\t\tthis paragraph:"}, "#text": "\n              ", "@id": "id857FBD877E7642F990829DB43A39495B"}, {"#tail": "\n            ", "clause": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "providing, or\n\t\t\t\tmaking available, open captioning for individuals with disabilities, including\n\t\t\t\tindividuals who are deaf or hard of hearing;"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "ida9dbd41d5c5a4355af2d3c9d5013bb58", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "providing, or\n\t\t\t\tmaking available, closed captioning for individuals with disabilities,\n\t\t\t\tincluding individuals who are deaf or hard of hearing; and"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "@id": "id8b9d489b53ff4765a035e0b4f613db82", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "providing, or\n\t\t\t\tmaking available, video description for individuals with disabilities,\n\t\t\t\tincluding individuals who are blind or visually impaired."}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "@id": "id89aca0572d0e44ecb79b476c9db0c131", "#text": "\n                "}], "enum": {"#tail": "\n              ", "#text": "(B)"}, "header": {"#tail": "\n              ", "#text": "Accessibility"}, "text": {"#tail": "\n              ", "#text": "It\n\t\t\t\tshall be discriminatory for any person who owns, leases (or leases to), or\n\t\t\t\toperates a covered entity to fail to ensure that all motion pictures shown at\n\t\t\t\tthe complex involved are accessible to individuals with disabilities,\n\t\t\t\tincluding\u2014"}, "#text": "\n              ", "@id": "id0717d6eb25904a749d2c71762d04d386"}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "subparagraph (B)", "@value": "Americans with Disabilities Act of 1990/s:302/ss:b/p:3/sp:B", "@proposed": "true"}, "#text": "Nothing in this Act shall be construed to limit or\n\t\t\t\tprohibit an individual with a disability from utilizing technology in\n\t\t\t\tconnection with a personal device in a manner that may provide the individual\n\t\t\t\twith access to closed captioning, open captioning, or video description that is\n\t\t\t\tequivalent to or greater than the corresponding access required under"}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "header": {"#tail": "\n              ", "#text": "Rule of\n\t\t\t\tconstruction"}, "#text": "\n              ", "@id": "idA78140A9F0AE43788CF1746B19A91152"}], "enum": {"#tail": "\n            ", "#text": "(3)"}, "header": {"#tail": "\n            ", "#text": "Movie Theater\n\t\t\t\tAccessibility"}, "#text": "\n            ", "@id": "id6BB2386DE9AA4CDBA8719BAA3F31651B"}, "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "id15D8A56DAB1B4CD5B037FADF3723A48C"}, "#text": "\n        ", "@id": "id9A2B964B1C7B4CEB868192140BF4DFE7"}], "#text": "\n      ", "@id": "id05647FD21D3D4399ADB8A5DBA5408740"}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "quote": [{"#tail": "and inserting", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "act", "#text": "section 303(a)", "@value": "Americans with Disabilities Act of 1990/s:303/ss:a"}, "#text": "and"}, {"#tail": ".", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ",\n\t\t\t and", "@entity-type": "act", "#text": "302(b)(3)", "@value": "Americans with Disabilities Act of 1990/s:302/ss:b/p:3", "@proposed": "true"}, {"#tail": "\n        ", "@entity-type": "act", "#text": "303(a)", "@value": "Americans with Disabilities Act of 1990/s:303/ss:a"}], "#text": ","}], "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": "is amended\n\t\t\t by striking", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "(", "@entity-type": "act", "#text": "Section 308(a)(2) of\n\t\t\t the Americans with Disabilities Act of 1990", "@value": "Americans with Disabilities Act of 1990/s:308/ss:a/p:2"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "uscode", "#text": "42 U.S.C. 12188(a)(2)", "@value": "usc/42/12188/a/2"}, "@parsable-cite": "usc/42/12188", "#text": "\n            ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "Conforming\n\t\t\t amendment"}, "#text": "\n      ", "@id": "idEACFC78301F34681865E0DED441C8F89"}, {"#tail": "\n  ", "text": {"#tail": "\n    ", "#text": "This Act takes effect 1\n\t\t\t year after the date of enactment of this Act.", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "4."}, "header": {"#tail": "\n      ", "#text": "Effective\n\t\t\t date"}, "#text": "\n      ", "@id": "id4a122b97feb042d1949a5b7d906748d1"}], "#text": "\n    "}, "metadata": {"#tail": "\n  ", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n      ", "#text": "2013-03-13"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n      ", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n      ", "#text": "EN"}, "#tail": "\n  ", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n      ", "#text": "113 S555 IS: Captioning and Image Narration to Enhance Movie Accessibility Act"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n    ", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n      ", "#text": "U.S. Senate"}, "#text": "\n      "}, "#text": "\n    "}}}