#tail": "\n        ", "clause": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "is\n\t\t\t an inhabitant; or"}, "enum": {"#tail": "\n              ", "#text": "(i)"}, "@id": "H0DA7049924B6467AACA6955649E782D2", "#text": "\n              "}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "may\n\t\t\t be found."}, "enum": {"#tail": "\n              ", "#text": "(ii)"}, "@id": "H4EBE836AF6EF400EB0D70C569F31752A", "#text": "\n              "}], "enum": {"#tail": "\n            ", "#text": "(B)"}, "header": {"#tail": "\n            ", "#text": "Service of\n\t\t\t process"}, "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", process may be\n\t\t\t served in any district in which the defendant\u2014", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Youth Sports Concussion Act of 2013/s:4/ss:c/p:1", "@proposed": "true"}, "#text": "In an action brought under "}, "#text": "\n            ", "@id": "H5947CCFA7B17484BAD04C026BAF35B0F"}], "enum": {"#tail": "\n          ", "#text": "(5)"}, "header": {"#tail": "\n\t\t\t\t\t", "#text": "Venue; service\n\t\t\t of process"}, "#text": "\n          ", "@id": "H6045C6143E6F4132A94889F9AC6A88D6"}, {"#tail": "\n      ", "subparagraph": [{"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", any other officer of a State who is authorized by\n\t\t\t the State to do so may bring a civil action under ", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Youth Sports Concussion Act of 2013/s:4/ss:c/p:1", "@proposed": "true"}, {"#tail": ", subject to the\n\t\t\t same requirements and limitations that apply under this subsection to civil\n\t\t\t actions brought by attorneys general.", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Youth Sports Concussion Act of 2013/s:4/ss:c/p:1", "@proposed": "true"}], "#text": "In addition to a civil actions brou