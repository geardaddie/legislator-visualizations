": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "Creating Access to Rehabilitation for Every Senior (CARES) Act of 2013"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "HB7DB9A491EDA4CDCB91AC166412AA996"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Elimination of Medicare 3-day prior hospitalization requirement for coverage of skilled nursing facility services in qualified skilled nursing facilities"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "\n          ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended to read as follows:", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Subsection (f) of section 1812 of the Social Security Act", "@value": "Social Security Act/s:1812/ss:f"}, "@entity-type": "law-citation", "#text": "\n            ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n            ", "@entity-type": "uscode", "#text": "42 U.S.C. 1395d", "@value": "usc/42/1395d"}, "@parsable-cite": "usc/42/1395d", "#text": "\n              ", "@legal-doc": "usc"}}, "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "subsection": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "(f)"}, "header": {"#tail": "\n            ", "#text": "Coverage of extended care services without a 3-Day prior hospitalization for qualified skilled nursing facility"}, "paragraph": [{"#tail": "\n            ", "text": {"short-title": {"#tail": ", coverage shall be provided under this part for an individual for such services in a qualified skilled nursing facility that are not post-hospital extended care services.", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                ", "@entity-type": "act", "#text": "Creating Access to Rehabilitation for Every Senior (CARES) Act of 2013", "@value": "Creating Access to Rehabilitation for Every Senior (CARES) Act of 2013", "@proposed": "true"}, "#text": "\n                  "}, "#tail": "\n            ", "#text": "Effective for extended care services furnished pursuant to an admission to a skilled nursing facility that occurs more than 90 days after the date of the enactment of the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(1)"}, "header": {"#tail": "\n              ", "#text": "In general"}, "#text": "\n              ", "@id": "H9FF96D39274D47B4B98580F8C86C824D"}, {"#tail": "\n            ", "@id": "H30BA78236B9A4E7F88C72758A177230F", "enum": {"#tail": "\n              ", "#text": "(2)"}, "header": {"#tail": "\n              ", "#text": "Continued application of certification and other requirements and provisions"}, "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " in the same manner as they apply to post-hospital extended care services:", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Social Security Act/s:1812/ss:f/p:1", "@proposed": "true"}, "#text": "The requirements of the following provisions shall apply to extended care services provided under ", "@display-inline": "yes-display-inline"}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "act", "#text": "Paragraphs (2)", "@value": "Social Security Act/s:1814/ss:a/p:2"}, {"#tail": ", except that the requirement of ", "@entity-type": "act", "#text": "(6) of section 1814(a)", "@value": "Social Security Act/s:1814/ss:a/p:6"}, {"#tail": " shall not apply insofar as it relates to any required prior receipt of inpatient hospital services.", "@entity-type": "act", "#text": "paragraph (2)(B) of such section", "@value": "Social Security Act/s:1814/ss:a/p:2/sp:B"}], "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "H9DFCD7798054452486CC1C26FA201C8E", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "act", "#text": "Subsections (b)(2)", "@value": "Social Security Act/s:1812/ss:b/p:2"}, {"#tail": ".", "@entity-type": "act", "#text": "(e) of this section", "@value": "Social Security Act/s:1812/ss:e"}], "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "HA3E49149195348C78C51FA28C8961E93", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", ", "@entity-type": "act", "#text": "Paragraphs (1)(G)(i)", "@value": "Social Security Act/s:1861/ss:v/p:1/sp:G/cl:i"}, {"#tail": ", and ", "@entity-type": "act", "#text": "(2)(A)", "@value": "Social Security Act/s:1861/ss:v/p:2/sp:A"}, {"#tail": ".", "@entity-type": "act", "#text": "(3) of section 1861(v)", "@value": "Social Security Act/s:1861/ss:v/p:3"}], "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@id": "HDF3B021088FC4551A9D01E623914A287", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "Section 1861(y)", "@value": "Social Security Act/s:1861/ss:y"}, "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(D)"}, "@id": "HAA2755F09FB747B2A3EE5F1A6DBE502A", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "Section 1862(a)(18)", "@value": "Social Security Act/s:1862/ss:a/p:18"}, "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(E)"}, "@id": "H1A5FC3590C8C4712A1A18A0D009AFB88", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "Section 1866(a)(1)(H)(ii)(I)", "@value": "Social Security Act/s:1866/ss:a/p:1/sp:H/cl:ii/scl:I"}, "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(F)"}, "@id": "H229A7E0CD23A4EF4AC564A2B11BBFDF2", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "act", "#text": "Subsections (d)", "@value": "Social Security Act/s:1883/ss:d"}, {"#tail": ".", "@entity-type": "act", "#text": "(f) of section 1883", "@value": "Social Security Act/s:1883/ss:f"}], "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(G)"}, "@id": "H50A800DAD08F4714BD98F74F4A8A645F", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "Section 1888(e)", "@value": "Social Security Act/s:1888/ss:e"}, "#text": "\n                  "}, "enum": {"#tail": "\n                ", "#text": "(H)"}, "@id": "H36EB78DA143E48BFAB93C6C20A1E6FBA", "#text": "\n                "}]}, {"#tail": "\n          ", "subparagraph": [{"continuation-text": {"#tai