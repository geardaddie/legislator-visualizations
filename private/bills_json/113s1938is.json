m": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "ID7bd4ec5780e842c9b7b605ed22a4a221", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ";", "@entity-type": "federal-body", "#text": "National\n\t\t  Science Foundation", "@entity-id": "4900"}, "#text": "the "}, "enum": {"#tail": "\n                  ", "#text": "(J)"}, "@id": "ID250aa1a14301441d8661cd180c21483a", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "; and", "@entity-type": "federal-body", "#text": "Environmental\n\t\t  Protection Agency", "@entity-id": "6800"}, "#text": "the "}, "enum": {"#tail": "\n                  ", "#text": "(K)"}, "@id": "IDf356b1813e9c4445a94691b0cffd666e", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "such other\n\t\t  Federal organizations, departments, and agencies that the President considers\n\t\t  to be appropriate."}, "enum": {"#tail": "\n                  ", "#text": "(L)"}, "@id": "IDc5ca4dd6872e494ab8f812dd59de100c", "#text": "\n                  "}]}], "#text": "\n              ", "@id": "IDcb83745dae7c4ac7a7b46aeec893f1f0"}, {"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "Each Quadrennial Energy Review shall be conducted to\n\t\t  provide an integrated view of important national energy objectives and Federal\n\t\t  energy policy, including the maximum practicable alignment of research\n\t\t  programs, incentives, regulations, and partnerships."}, "enum": {"#tail": "\n              ", "#text": "(c)"}, "header": {"#tail": "\n              ", "#text": "Conduct of\n\t\t  review"}, "#text": "\n              ", "@id": "idBA8615D13FB64CC7BB4871440A44A602"}, {"#tail": "\n            ", "enum": {"#tail": "\n              ", "#text": "(d)"}, "header": {"#tail": "\n              ", "#text": "Submission of\n\t\t  Quadrennial Energy Review to Congress"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " a report on the\n\t\t  Quadrennial Energy Review.", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}, "#text": "Not later than August 1, 2015, and every 4 years\n\t\t  thereafter, the President shall publish and submit to "}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t  general"}, "#text": "\n                ", "@id": "id779F90609C104E39AE14DF2CE87544F7"}, {"#tail": "\n            ", "@id": "id9CC77A395F33422FADFC30B722BE7CB9", "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Inclusions"}, "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " should include, as appropriate\u2014", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Department of Energy Organization Act/s:801/ss:d/p:1", "@proposed": "true"}, "#text": "The\n\t\t  report described in "}, "#text": "\n                ", "subparagraph": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "an integrated\n\t\t  view of short-, intermediate-, and long-term objectives for Federal energy\n\t\t  policy in the context of economic, environmental, and security\n\t\t  priorities;"}, "enum": {"#tail": "\n                  ", "#text": "(A)"}, "@id": "ID5283e39301954e86a15b017ff318cec1", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "anticipated\n\t\t  Federal actions (including programmatic, regulatory, and fiscal actions) and\n\t\t  resource requirements\u2014"}, "enum": {"#tail": "\n                  ", "#text": "(B)"}, "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n                  ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "; and", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Department of Energy Organization Act/s:801/ss:d/p:2/sp:A", "@proposed": "true"}, "#text": "to\n\t\t  achieve the objectives described in "}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "@id": "id33B2827C0C914BB3B0C21BB294452AF0", "#text": "\n                    "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "to\n\t\t  be coordinated across multiple agencies;"}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "idE40EF814AB4842B4817F07465067941D", "#text": "\n                    "}], "#text": "\n                  ", "@id": "IDad1a946310d5457e8a7e4f75bd4517bb"}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", including\u2014", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Department of Energy Organization Act/s:801/ss:d/p:2/sp:A", "@proposed": "true"}, "#text": "an analysis of\n\t\t  the prospective roles of parties (including academia, industry, consumers, the\n\t\t  public, and Federal agencies) in achieving the objectives described in\n\t\t  "}, "enum": {"#tail": "\n                  ", "#text": "(C)"}, "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n                    ", "#text": "an\n\t\t  analysis, by energy use sector, including\u2014"}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "subclause": [{"#tail": "\n                    ", "text": {"#tail": "\n                    ", "#text": "commercial and\n\t\t  residential buildings;"}, "enum": {"#tail": "\n                      ", "#text": "(I)"}, "@id": "ID5efcb40125e14aa1918794a2651d1019", "#text": "\n                      "}, {"#tail": "\n                    ", "text": {"#tail": "\n                    ", "#text": "the industrial\n\t\t  sector;"}, "enum": {"#tail": "\n                      ", "#text": "(II)"}, "@id": "ID096140ff313047a780f71d8d8af87299", "#text": "\n                      "}, {"#tail": "\n                    ", "text": {"#tail": "\n                    ", "#text": "transportation;\n\t\t  and"}, "enum": {"#tail": "\n                      ", "#text": "(III)"}, "@id": "ID8c9a9844a6e048a880ba2321de2cf950", "#text": "\n                      "}, {"#tail": "\n                  ", "text": {"#tail": "\n                    ", "#text": "electric\n\t\t  power;"}, "enum": {"#tail": "\n                      ", "#text": "(IV)"}, "@id": "ID0fcdfdc48bc84296921a929010f4b2bb", "#text": "\n                      "}], "#text": "\n                    ", "@id": "IDf1c286f9a6794cf6a35f06390cb24a45"}, {"#tail": "\n                  ", "text": {"#tail": "\n                  ", "#text": "requirements for\n\t\t  invention, adoption, development, and diffusion of energy technologies that are\n\t\t  mapped onto each of the energy use sectors; and"}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "IDd05fdffe59124d77bf8520f96a1cbaf0", "#text": "\n                    "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "other research\n\t\t  that inform strategies to incentivize desired actions;"}, "enum": {"#tail": "\n                    ", "#text": "(iii)"}, "@id": "ID101b67739f4f462f8e6ad6b60477fa52", "#text": "\n                    "}], "#text": "\n                  ", "@id": "ID03579c368893477c8c619ed854b6712e"}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "an assessment of\n\t\t  policy options to increase domestic energy supplies and energy\n\t\t  efficiency;"}, "enum": {"#tail": "\n                  ", "#text": "(D)"}, "@id": "IDeb90b90e86624df481571dafca268ba1", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "an evaluation of\n\t\t  energy storage, transmission, and distribution requirements, including\n\t\t  requirements for renewable energy;"}, "enum": {"#tail": "\n                  ", "#text": "(E)"}, "@id": "ID993ae0f5f2ca4ab7ba0e13b1ec5db118", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "an integrated\n\t\t  plan for the involvement of the Federal Laboratories in energy programs;"}, "enum": {"#tail": "\n                  ", "#text": "(F)"}, "@id": "ID9289a3c50e924df1864d52e35dd03389", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "portfolio\n\t\t  assessments that describe the optimal deployment of resources, including\n\t\t  prioritizing financial resources for energy programs;"}, "enum": {"#tail": "\n                  ", "#text": "(G)"}, "@id": "IDe24a4cb72fb7471bb635452b292bb066", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "a mapping of the\n\t\t  linkages among basic research and applied programs, demonstration programs, and\n\t\t  other innovation mechanisms across the Federal agencies;"}, "enum": {"#tail": "\n                  ", "#text": "(H)"}, "@id": "IDc211a09c2d74442e99e781cccecf99be", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "an identification\n\t\t  of, and projections for, demonstration projects, including timeframes,\n\t\t  milestones, sources of funding, and management;"}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "@id": "ID12ebecd48ec549af9f94f2e019e64c5b", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "an identification\n\t\t  of public and private funding needs for various energy technologies, systems,\n\t\t  and infrastructure, including consideration of public-private partnerships,\n\t\t  loans, and loan guarantees;"}, "enum": {"#tail": "\n                  ", "#text": "(J)"}, "@id": "ID0b9e8caf67e243e5b93cdfaf03e30fc1", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "an assessment of\n\t\t  global competitors and an identification of programs that can be enhanced with\n\t\t  international cooperation;"}, "enum": {"#tail": "\n                  ", "#text": "(K)"}, "@id": "ID9e11024129d847679d154ff790841cc7", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "an identification\n\t\t  of policy gaps that need to be filled to accelerate the adoption and diffusion\n\t\t  of energy technologies, including consideration of\u2014"}, "enum": {"#tail": "\n                  ", "#text": "(L)"}, "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n                  ", "#text": "Federal tax\n\t\t  policies; and"}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "@id": "idA5A1A4FDF2AB4780807F6E1871E3F302", "#text": "\n                    "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "the\n\t\t  role of Federal agencies as early adopters and purchasers of new energy\n\t\t  technologies;"}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "id837B8B5796D74A17A72E14CE0C76584D", "#text": "\n                    "}], "#text": "\n                  ", "@id": "ID623a65be6ee541598ada25f21d48ac1c"}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "a priority list\n\t\t  for implementation of objectives and actions taking into account estimated\n\t\t  Federal budgetary resources;"}, "enum": {"#tail": "\n                  ", "#text": "(M)"}, "@id": "idbed0e0793615470bbde2621656108c12", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "an analysis\n\t\t  of\u2014"}, "enum": {"#tail": "\n                  ", "#text": "(N)"}, "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n                  ", "#text": "points of maximum\n\t\t  leverage for policy intervention to achieve outcomes; and"}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "@id": "id3B8CC540E1CF48179318987670180785", "#text": "\n                    "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "areas of energy\n\t\t  policy that can be most effective in meeting national goals for the energy\n\t\t  sector; and"}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "id50790113432043E2A10E028BD9FA3FE0", "#text": "\n                    "}], "#text": "\n                  ", "@id": "ID2e72d6c3769a4010aa79be45a58b0999"}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "recommendations\n\t\t  for executive branch organization changes to facilitate the development and\n\t\t  implementation of Federal energy policies."}, "enum": {"#tail": "\n                  ", "#text": "(O)"}, "@id": "IDb018c6130ea64631b4a76750e8b31adf", "#text": "\n                  "}]}], "#text": "\n              ", "@id": "ID70d6c5b714e9415fbd6f0ea8e2227b20"}, {"#tail": "\n            ", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " may prepare and publish interim reports as\n\t\t  part of the Quadrennial Energy Review.", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, "#text": "The "}, "enum": {"#tail": "\n              ", "#text": "(e)"}, "header": {"#tail": "\n              ", "#text": "Interim\n\t\t  reports"}, "#text": "\n              ", "@id": "id86AC237B768E40C3BB3DD7665D94A49C"}, {"#tail": "\n          ", "enum": {"#tail": "\n              ", "#text": "(f)"}, "header": {"#tail": "\n              ", "#text": "Executive\n\t\t  Secretariat"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall provide the Quadrennial\n\t\t  Energy Review with an Executive Secretariat who shall make available the\n\t\t  necessary analytical, financial, and administrative support for the conduct of\n\t\t  each Quadrennial Energy Review required under this section.", "@entity-type": "federal-body", "#text": "Secretary of Energy", "@entity-id": "8900"}, "#text": "The "}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t  general"}, "#text": "\n                ", "@id": "ID47529bf5e9ae4cc381d3d6a6b3be6cd7"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and\n\t\t  provide such assistance, information, and resources as the ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "8900"}, {"#tail": " may\n\t\t  require to assist in carrying out this\n\t\t  section.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "8900"}], "#text": "The\n\t\t  heads of applicable Federal agencies shall cooperate with the "}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Cooperation"}, "#text": "\n                ", "@id": "ID5665e9e03ceb4b5b9413223ead2323e7"}], "#text": "\n              ", "@id": "ID6bb1eadbca5b4a4784f7be64de529497"}], "#text": "\n            ", "@id": "id184ECBEDBFB64077B9C0B6A8DBC5125C"}, "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "idBCB05D4C17474017879E2DEE90258F04"}, "#text": "\n        ", "@id": "id49BF3CABDFA34894A4F4F3827C2D3E5B"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "Nothing in this Act or an amendment\n\t\tmade by this Act supersedes, modifies, amends, or repeals any provision of\n\t\tFederal law not expressly superseded, modified, amended, or repealed by this\n\t\tAct.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Administration"}, "#text": "\n        ", "@id": "IDdf7f527a04f8480fbd1fa3c9cd3b349f"}], "#text": "\n      ", "@id": "id4A1367F6E66E4271942ADA54E86CF0F6"}], "#text": "\n    "}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-01-16"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 S1938 IS: Quadrennial Energy Review Act of 2014"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. Senate"}, "#text": "\n"}, "#text": "\n"}}}