}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "quote": [{"#tail": " and all that follows and inserting ", "#text": "educator in connection"}, {"#tail": "; and", "#text": "educator\u2014"}], "#text": "by striking "}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "H00C3EC051AEA468081EA5EE0A3F336B9", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t\t", "#text": "by inserting at the end the following:"}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "#tail": "\n\t\t\t\t", "clause": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "by reason of the participation of the educator in professional development courses related to the\n\t\t\t curriculum in which the educator provides instruction or to the students\n\t\t\t for which the educator provides instruction, and"}, "enum": {"#tail": "\n              ", "#text": "(i)"}, "@id": "HF953425CFC194A8B96DCA093A1088349", "#text": "\n              "}, {"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "in connection with books, supplies (other than nonathletic supplies for courses of instruction in\n\t\t\t health or physical education), computer equipment (including related\n\t\t\t software and services) and other equipment, and supplementary materials\n\t\t\t used by the eligible educator in the classroom."}, "enum": {"#tail": "\n              ", "#text": "(ii)"}, "@id": "HB972BDB2C7434D78B29E6F11EEBC8362", "#text": "\n              "}], "#text": "\n\t\t\t\t\t\t", "@id": "HFCFFEF88C284413E8F3BB3468D205045"}, "#text": "\n          ", "@id": "HE9AF57DBFBAD4B788B189699F91EB9D3"}], "#text": "\n        ", "@id": "HEF8B5163CD31430FB0C41231C1A6A083"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " is amended by inserting ", "@entity-type": "act", "#text": "