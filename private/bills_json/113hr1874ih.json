{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "H. R. 1874"}, "official-title": {"#tail": "\n\t", "#text": "To amend the Congressional Budget Act of 1974 to provide\n\t\t  for macroeconomic analysis of the impact of legislation."}, "session": {"#tail": "\n\t\t", "#text": "1st Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "I"}, "action": {"action-date": {"#tail": "\n\t\t\t", "@date": "20130508", "#text": "May 8, 2013"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "committee-name": [{"#tail": ", and in addition\n\t\t\t to the Committee on ", "@committee-id": "HBU00", "#text": "Committee on the Budget"}, {"#tail": ", for a period to be subsequently\n\t\t\t determined by the Speaker, in each case for consideration of such provisions as\n\t\t\t fall within the jurisdiction of the committee concerned", "@committee-id": "HRU00", "#text": "Rules"}], "sponsor": {"#tail": "\n\t\t\t (for himself, ", "#text": "Mr. Price of Georgia", "@name-id": "P000591"}, "cosponsor": [{"#tail": ",\n\t\t\t ", "#text": "Mr. Ryan of Wisconsin", "@name-id": "R000570"}, {"#tail": ",\n\t\t\t ", "#text": "Mrs. Black", "@name-id": "B001273"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Chaffetz", "@name-id": "C001076"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Collins of Georgia", "@name-id": "C001093"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Cotton", "@name-id": "C001095"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Garrett", "@name-id": "G000548"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Gosar", "@name-id": "G000565"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Graves of Georgia", "@name-id": "G000560"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Hensarling", "@name-id": "H001036"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Johnson of Ohio", "@name-id": "J