ader": {"#tail": "\n                  ", "#text": "Distributions to virgin islands"}, "text": {"#tail": "\n                  ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " may be paid from escrow to the Virgin Islands for expenditure only if\u2014", "@entity-type": "act", "#text": "subsection (d)(1)(A)", "@value": "Internal Revenue Code of 1986/s:409B/ss:d/p:1/sp:A", "@proposed": "true"}, "#text": "Funds and accrued interest described in "}, "#text": "\n                  ", "@id": "H7C854AE853E64B45BA868A5BEB3A8947"}, {"#tail": "\n                ", "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n                  ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", and", "@entity-type": "act", "#text": "subparagraph (D)", "@value": "Internal Revenue Code of 1986/s:409B/ss:d/p:1/sp:D", "@proposed": "true"}, "#text": "developed and approved by the committee described in "}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "@id": "H3652834573DD475883331F9B80500B68", "#text": "\n                    "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "approved by the Governor of the Virgin Islands."}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "HFC21DA6FA6B044FD855716BB8E090E9B", "#text": "\n                    "}], "enum": {"#tail": "\n                  ", "#text": "(C)"}, "header": {"#tail": "\n                  ", "#text": "Qualified infrastructure development plan"}, "text": {"#tail": "\n                  ", "term": {"#tail": " means a plan for improving and enhancing the infrastructure of the Virgin Islands which is\u2014", "#text": "qualified infrastructure development plan"}, "#text": "For purposes of this paragraph, the term "}, "#text": "\n                  ", "@id": "H70351A64C31D45C095F86AA31DA01242"}, {"#tail": "\n              ", "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n                    ", "#text": "comprised of 5 members, each serving a term of either three or five years\u2014"}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "subclause": [{"#tail": "\n                    ", "text": {"#tail": "\n                    ", "#text": "2 of whom are appointed by the Governor of the Virgin Islands, one for a 3-year and one for a 5-year term,"}, "enum": {"#tail": "\n                      ", "#text": "(I)"}, "@id": "H87BA70155A2241C7ABFA49E20EB3C284", "#text": "\n                      "}, {"#tail": "\n                    ", "text": {"#tail": "\n                    ", "#text": "2 of whom are appointed by the Virgin Islands legislature, one for a 3-year and one for a 5-year term, and"}, "enum": {"#tail": "\n                      ", "#text": "(II)"}, "@id": "H9B7531792CFA4C5188C81F4A61C9CBB0", "#text": "\n                      "}, {"#tail": "\n                  ", "text": {"#tail": "\n                    ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " for a 5-year term, and", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "#text": "1 of whom is appointed by the "}, "enum": {"#tail": "\n                      ", "#text": "(III)"}, "@id": "H1B6039366B1047B9A641DC95209C6B60", "#text": "\n                      "}], "#text": "\n                    ", "@id": "HC1476AA61A6E42D7A5B6D19CC05CF705"}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "with respect to which a vacancy is filled in the manner in which the original appointment was made."}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "HC77FF58BD8F8436BABB026FD1EEA51B4", "#text": "\n                    "}], "enum": {"#tail": "\n                  ", "#text": "(D)"}, "header": {"#tail": "\n                  ", "#text": "Committee"}, "text": {"#tail": "\n                  ", "#text": "\n                    \n                      The committee\n                     described in this subparagraph is a committee\u2014"}, "#text": "\n                  ", "@id": "H8DB76148307548338E1A93B94E7F334B"}], "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "Revenues to the virgin islands during first 20 years"}, "#text": "\n                ", "@id": "H633FD46BCB874A82B9BC8D69688A8FA7"}, {"#tail": "\n              ", "subparagraph": [{"#tail": "\n                ", "clause": [{"#tail": "\n                  ", "text": {"#tail": "\n                  ", "#text": "26.67 percent of the proceeds shall be distributed to the Virgin Islands for the first 10 years of management, and"}, "enum": {"#tail": "\n                    ", "#text": "(i)"}, "@id": "HCDB89936BDB24E0D877FA8F7F59B142A", "#text": "\n                    "}, {"#tail": "\n                ", "text": {"#tail": "\n                  ", "#text": "half of the proceeds shall be distributed to the Virgin Islands for the next 10 years of management."}, "enum": {"#tail": "\n                    ", "#text": "(ii)"}, "@id": "H7327F22B981D44C48C3C7D76E9D9AAAC", "#text": "\n                    "}], "enum": {"#tail": "\n                  ", "#text": "(A)"}, "header": {"#tail": "\n                  ", "#text": "During first 20 years"}, "text": {"#tail": "\n                  ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " imposed on designated assets after the first 10 years under management by the Virgin Islands Investment Program shall be collected by the United States Treasury in a manner similar to ", "@entity-type": "act", "#text": "subsection (c)(3)(B)", "@value": "Internal Revenue Code of 1986/s:409B/ss:c/p:3/sp:B", "@proposed": "true"}, {"#tail": ", upon which\u2014", "@entity-type": "act", "#text": "section 7652", "@value": "Internal Revenue Code of 1986/s:7652"}], "#text": "Revenues from the fee referred to in ", "@display-inline": "yes-display-inline"}, "#text": "\n                  ", "@id": "H423E402E743945E69B71FA2C1B9E8C18"}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "#text": "Beginning in the 21st year, the entire 1 percent tax collected shall be retained by the United States Treasury."}, "enum": {"#tail": "\n                  ", "#text": "(B)"}, "header": {"#tail": "\n                  ", "#text": "After the first 20 years"}, "#text": "\n                  ", "@id": "H5BA9C644652A490BAA6E5E62A76F1847"}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "No withdrawals may be made by an investor from the account during the minimum holding period of ten years. Should the investor choose to withdraw money from the account during the minimum holding period, the investor would forfeit the tax advantages of the Fund. Any funds so withdrawn would be included in gross income and subject to Federal income tax, minus payments of the 1 percent tax."}, "enum": {"#tail": "\n                  ", "#text": "(C)"}, "header": {"#tail": "\n                  ", "#text": "Minimum holding period"}, "#text": "\n                  ", "@id": "H5A1E4E57E70A4354BD086497913AFAF5"}], "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Revenues to the united states and the virgin islands"}, "#text": "\n                ", "@id": "HA5C03D50387B49B6AC054BFEF480F1DE"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "Should an investor withdraw the entire balance of the funds after the 10-year minimum holding period but before the end of the 30 years, his account will be liable for the entire 1 percent tax for each of the remaining years."}, "enum": {"#tail": "\n                ", "#text": "(3)"}, "header": {"#tail": "\n                ", "#text": "Early withdrawal"}, "#text": "\n                ", "@id": "H539171E2117742B9A0B688BB27761408"}], "#text": "\n              ", "@id": "H2922F1A698414E3DA4D3484D1E35043B"}, {"#tail": "\n          ", "text": {"#tail": "\n              ", "#text": "For purposes of this section\u2014"}, "enum": {"#tail": "\n              ", "#text": "(e)"}, "header": {"#tail": "\n              ", "#text": "Other definitions"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                ", "@entity-type": "act", "#text": "section 402A.", "@value": "Internal Revenue Code of 1986/s:402A"}, "term": [{"#tail": " and ", "#text": "elective deferrals"}, {"#tail": " have the respective meanings given such terms by ", "#text": "applicable retirement plan"}], "#text": "The terms "}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "Elective deferrals; applicable retirement plan"}, "#text": "\n                ", "@id": "HC28461E62A7D4A35B42B0B6838581704"}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": " means the United States Virgin Islands.", "#text": "Virgin Islands"}, "#text": "The term "}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Virgin islands"}, "#text": "\n                ", "@id": "H1C17936EBB544F869291A67C7DF031BE"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " or his designee.", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "term": {"#tail": " means the ", "#text": "Secretary of the Interior"}, "#text": "The term "}, "enum": {"#tail": "\n                ", "#text": "(3)"}, "header": {"#tail": "\n                ", "#text": "Secretary of the interior"}, "#text": "\n                ", "@id": "HF1B514DB9511488FB1FFBAA95290F22B"}], "#text": "\n              ", "@id": "H968235A0A984486F995648143B568E05"}], "#text": "\n            ", "@id": "HC0878A973919446494EC57201CADFB74"}, "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "H5677F1D6F3C542E281433DB14D83DD9B"}, "#text": "\n        ", "@id": "H2D86129FFF3F45CCA84C2A9091C56E7C"}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "The table of sections for such part I is amended by adding at the end the following new item:"}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Clerical amendment"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "@display-inline": "no-display-inline", "toc": {"#tail": "\n          ", "toc-entry": {"#tail": "\n          ", "#text": "\n              Sec. 409B. Treatment of distributions from certain retirement plan assets invested under a Virgin Islands investment program.\n            ", "@level": "section"}, "#text": "\n            ", "@regeneration": "no-regeneration"}, "#text": "\n          ", "@id": "H26C7E2330FFF4E29BD476180992BF5F9"}, "#text": "\n        ", "@id": "H4F37BB93596640FB96725C31EB3BD2BA"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "The amendments made by this section shall take effect on the date of the enactment of this Act."}, "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "Effective date"}, "#text": "\n        ", "@id": "H3EA4C8D307EF4B6EA09B0DD82F016CCA"}], "#text": "\n      ", "@id": "H43C29C5AAA45414DB2685AB7A8562B5F"}], "#text": "\n    ", "@id": "HF0430A6EE1D24CF0BCC44C9F3C0DF318"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-01-08"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 3821 IH: Derek M. Hodge Virgin Islands Improvement Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}