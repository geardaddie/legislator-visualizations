num": {"#tail": "\n              ", "#text": "(A)"}, "clause": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "; and", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "3600"}, "#text": "as a result of a natural or other disaster, as determined by the "}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "idF58679EF436F4DC99B2805F56CA896B2", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "through no fault of the eligible person; and"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "@id": "id262089F35CCA462FA6F2942C136D1C37", "#text": "\n                "}], "#text": "\n              ", "@id": "id6C43310D0E504226BC37674915964A5F"}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "#text": "the eligible person does not otherwise receive from a property insurer compensation for the loss."}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "@id": "id8A5A4D67385E4BE8B8A0939D521F7760", "#text": "\n              "}]}, {"@indent": "up1", "#tail": "\n        ", "text": {"#tail": "\n          ", "#text": "No payment shall be made under this chapter for the repair or maintenance of an automobile or other conveyance."}, "enum": {"#tail": "\n            ", "#text": "(4)"}, "#text": "\n            ", "@id": "id809D27C655A84950BA0C9BB0FE8111A2"}]}, "@display-inline": "no-display-inline", "#text": "\n        ", "@id": "id1D5DDC2283DB443FABD0892BD873665E"}, "#text": "\n      ", "@id": "id872B6AEF494E45AEBB3BD5D664DC6A8A"}, {"#tail": "\n  ", "text": {"#tail": "\n    ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended by striking ", "@entity-type": "uscode", "#text": "\n          ", "external-xref": {"#tail": " of title 38, United States Code", "@parsable-cite": "usc/38/3902", "#text": "Section 3902(a)", "@legal-doc": "usc"}, "@value": "usc/38/3902/a"}, "quo