{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "H. R. 2192"}, "official-title": {"#tail": "\n\t", "#text": "To amend the Act popularly known as the Antiquities Act\n\t\t  of 1906 to require certain procedures for designating national monuments, and\n\t\t  for other purposes."}, "session": {"#tail": "\n\t\t", "#text": "1st Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "I"}, "action": {"action-date": {"#tail": "\n\t\t\t", "@date": "20130523", "#text": "May 23, 2013"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "sponsor": {"#tail": " introduced\n\t\t\t the following bill; which was referred to the\n\t\t\t ", "#text": "Mr. Nunes", "@name-id": "N000181"}, "committee-name": {"#tail": "\n      ", "@committee-id": "HII00", "#text": "Committee on Natural\n\t\t\t Resources"}}, "#text": "\n\t\t\t"}, "legis-type": {"#tail": "\n\t\t", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-House", "@bill-type": "olc", "@dms-id": "H38278E80281B4EFC93B50EE93CF44120", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "#text": "National Monument Designation\n\t\t\t Transparency and Accountability Act"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the\n\t\t\t ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H2BFC7ED0B87E4411B952FB5E20F50817"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "quote": {"#tail": ", is\n\t\t\t amended\u2014", "#text": "Antiquities Act of 1906"}, "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": ", popularly known as the ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " (", "@official-title": "true", "@entity-type": "act", "#text": "Section 2 of the Act of June 8, 1906", "@value": "Antiquities Act of 1906/s:2"}, {"#tail": ")", "@entity-type": "uscode", "#text": "16\n\t\t\t U.S.C. 431", "@value": "usc/16/431"}], "@entity-type": "law-citation", "#text": "\n          "}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Limitation on\n\t\t\t designation of national monuments"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "quote": [{"#tail": " and inserting ", "#text": "That the President"}, {"#tail": "; and", "header-in-text": {"#tail": "Subject to the requirements of this section, the\n\t\t\t President", "@style": "OLC", "#text": "In\n\t\t\t general.\u2014", "@level": "subsection"}, "#text": "(a)\n\t\t\t "}], "#text": "by striking\n\t\t\t "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@id": "H7E91E8FB9D7E418F8AA3CDA27FCA50BC", "#text": "\n        "}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n\t\t\t (as so designated)\u2014", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a"}, "#text": "in "}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "#text": "\n        ", "@id": "HCEEAEB32D1234B25A13FE923DC79B4EA", "subparagraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "quote": [{"#tail": " and inserting ", "#text": "compatible with"}, {"#tail": "; and", "#text": "essential to\n\t\t\t ensure"}], "#text": "by striking\n\t\t\t "}, "enum": {"#tail": "\n          ", "#text": "(A)"}, "@id": "HD792AF6A9F2A4EDDB0D0F3E66958C5BA", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "quote": [{"#tail": " and inserting\n\t\t\t ", "#text": "\n              ", "italic": {"#tail": ", That when", "#text": "Provided"}}, {"#tail": ".", "#text": "When"}], "#text": "by striking\n\t\t\t "}, "enum": {"#tail": "\n          ", "#text": "(B)"}, "#text": "\n          ", "@id": "H03153C4B807F4ACA924D83F7899DDD02", "@commented": "no"}]}], "#text": "\n      ", "@id": "HCA11DDF1B21B4C4DB4D466B660A883FA"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is further amended by adding at\n\t\t\t the end the following:", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@official-title": "true", "@entity-type": "act", "#text": "Section\n\t\t\t 2 of the Act of June 8, 1906", "@value": "Antiquities Act of 1906/s:2"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "uscode", "#text": "16 U.S.C. 431", "@value": "usc/16/431"}, "@parsable-cite": "usc/16/431", "#text": "\n            ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "National monument\n\t\t\t designation procedures"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n      ", "#text": "."}, "#tail": "\n\t\t", "subsection": {"#tail": "\n        ", "enum": {"#tail": "\n          ", "#text": "(b)"}, "header": {"#tail": "\n\t\t\t\t\t", "#text": "National\n\t\t\t\tmonument designation procedures"}, "paragraph": [{"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may not issue a proclamation to\n\t\t\t\tdesignate a national monument under ", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": " before the last day of the\n\t\t\t\t30-day period beginning on the date on which the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a"}, {"#tail": " provides the\n\t\t\t\tlanguage of the proposed proclamation to ", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": " and to the Governor of each\n\t\t\t\tState, the chief elected official of each unit of local government, and the\n\t\t\t\tgoverning entity of each tribal government with jurisdiction over parcels of\n\t\t\t\tland located within the boundaries of the proposed national monument.", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}], "#text": "The "}, "enum": {"#tail": "\n            ", "#text": "(1)"}, "header": {"#tail": "\n            ", "#text": "Precondition to\n\t\t\t\tproclamation"}, "#text": "\n            ", "@id": "HADB5D0AD6F504DE5AC0493488D3886EC", "@commented": "no"}, {"#tail": "\n          ", "subparagraph": [{"#tail": "\n            ", "clause": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", not later than 90 days after the date on which the ", "@entity-type": "act", "#text": "clause\n\t\t\t\t(iv)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A/cl:iv", "@proposed": "true"}, {"#tail": " issues a\n\t\t\t\tproclamation under ", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": ", the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a", "@proposed": "true"}, {"#tail": " shall hold not\n\t\t\t\tfewer than one public hearing within a county (or comparable unit of local\n\t\t\t\tgovernment) located wholly or in part within the boundaries of the national\n\t\t\t\tmonument. The ", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, {"#tail": " shall ensure that all interested individuals are\n\t\t\t\tafforded an opportunity to participate in a hearing held under this\n\t\t\t\tsubparagraph.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}], "#text": "Subject to ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n                ", "@id": "HCDF4AEACFF584896956E63E02C02825B"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall solicit\n\t\t\t\tcomments from the public at a hearing held under ", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, {"#tail": ", and shall enter\n\t\t\t\tall comments received at or related to such hearing into the record of the\n\t\t\t\thearing.", "@entity-type": "act", "#text": "clause (i)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A/cl:i", "@proposed": "true"}], "#text": "The ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "header": {"#tail": "\n                ", "#text": "Comments"}, "#text": "\n                ", "@id": "H7D1CC9DF320A450FB5357DF6FF1BAC9F", "@commented": "no"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall promptly make the record of a hearing held under ", "@entity-type": "federal-body", "#text": "Secretary of\n\t\t\t\tthe Interior", "@entity-id": "1400"}, {"#tail": ",\n\t\t\t\tincluding a transcript of the hearing, available to the public on the Internet\n\t\t\t\tor by other electronic means. The ", "@entity-type": "act", "#text": "clause (i)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A/cl:i", "@proposed": "true"}, {"#tail": " shall ensure that any components of\n\t\t\t\tthe record that are completed before the entire record is finalized are made\n\t\t\t\tavailable upon their completion.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}], "#text": "The ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "header": {"#tail": "\n                ", "#text": "Availability\n\t\t\t\tof record"}, "#text": "\n                ", "@id": "HCDA1356F0BCF4F0D8FE957FF4985208F"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may decline\n\t\t\t\tto hold a public hearing under ", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, {"#tail": " if each unit of local and tribal\n\t\t\t\tgovernment located wholly or in part within the boundaries of the national\n\t\t\t\tmonument expressly waives the right to such hearing.", "@entity-type": "act", "#text": "clause (i)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A/cl:i", "@proposed": "true"}], "#text": "The ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(iv)"}, "header": {"#tail": "\n                ", "#text": "Waiver"}, "#text": "\n                ", "@id": "H249A337BF2E94F689454F3BEA11C89B5", "@commented": "no"}], "enum": {"#tail": "\n              ", "#text": "(A)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Public hearing\n\t\t\t\trequirement"}, "#text": "\n              ", "@id": "H6F6865183CBA41BBB74488EF860F2D44"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " issues a proclamation under ", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": ", the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a", "@proposed": "true"}, {"#tail": " shall initiate a notice and comment period to receive comments\n\t\t\t\tfrom the public regarding the proclamation.", "@entity-type": "federal-body", "#text": "Secretary\n\t\t\t\tof the Interior", "@entity-id": "1400"}], "#text": "Not later than 30 days after the date on\n\t\t\t\twhich the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "header": {"#tail": "\n              ", "#text": "Notice and\n\t\t\t\tcomment period requirement"}, "#text": "\n              ", "@id": "H990E543674E64E9DADE29F335BE1F405"}, {"#tail": "\n            ", "clause": [{"#tail": "\n              ", "subclause": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "An analysis of the\n\t\t\t\teconomic impact of the designation on the communities within the boundaries of\n\t\t\t\tthe monument, including an estimate of the tax revenues that will be lost to,\n\t\t\t\tor gained for, the Federal, State, and local governments as a result of the\n\t\t\t\tdesignation."}, "enum": {"#tail": "\n                  ", "#text": "(I)"}, "#text": "\n                  ", "@id": "H9CDBA48100F44D2EB282BF20EA5E8C59", "@commented": "no"}, {"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "An analysis of\n\t\t\t\tthe impact the designation will have on the Nation\u2019s energy security, including\n\t\t\t\tthe effects of the loss of sites to produce wind, geothermal, or solar energy,\n\t\t\t\tand the number of barrels of oil, tons of coal, or cubic feet of natural gas\n\t\t\t\tthat will become unavailable as a result of the proclamation."}, "enum": {"#tail": "\n                  ", "#text": "(II)"}, "#text": "\n                  ", "@id": "H309CDD2D1F644112BFF20CC8777ECF7C", "@commented": "no"}, {"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "The projected\n\t\t\t\timpact of the designation on interests, rights, and uses associated with the\n\t\t\t\tparcels of land within the boundaries of the monument, including water rights,\n\t\t\t\thunting, recreational shooting, grazing, timber production, vegetation\n\t\t\t\tmanipulation to maintain forest health, off-road vehicle use, hiking, horseback\n\t\t\t\triding, and mineral and energy leases, claims, and permits."}, "enum": {"#tail": "\n                  ", "#text": "(III)"}, "#text": "\n                  ", "@id": "HD76FEDD5065B47AE871510C953121912", "@commented": "no"}, {"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A", "@proposed": "true"}, "#text": "The record of any\n\t\t\t\thearings held under "}, "enum": {"#tail": "\n                  ", "#text": "(IV)"}, "#text": "\n                  ", "@id": "HCF8F21F94552475D845F5BF66ADAF09F", "@commented": "no"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "subparagraph (B)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:B", "@proposed": "true"}, "#text": "Any written\n\t\t\t\tcomments received during the notice and comment period conducted under\n\t\t\t\t"}, "enum": {"#tail": "\n                  ", "#text": "(V)"}, "#text": "\n                  ", "@id": "H60F85A1D0FA7449080A1D088B3D9A362", "@commented": "no"}], "enum": {"#tail": "\n                ", "#text": "(i)"}, "header": {"#tail": "\n                ", "#text": "Contents"}, "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a", "@proposed": "true"}, {"#tail": " shall submit to ", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": " a report\n\t\t\t\tcontaining the following:", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}], "#text": "Not\n\t\t\t\tlater than one year after issuing a proclamation to designate a national\n\t\t\t\tmonument under "}, "#text": "\n                ", "@id": "HB9C4ECDE74BA4CEDB9D30571624F9EF6", "@commented": "no"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall ensure that a report submitted to ", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": " under ", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}, {"#tail": " is\n\t\t\t\tpublished on the White House Internet Web site upon completion. The President\n\t\t\t\tshall further ensure that any components of the report that are completed\n\t\t\t\tbefore the entire report is finalized and submitted to ", "@entity-type": "act", "#text": "clause (i)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:C/cl:i", "@proposed": "true"}, {"#tail": " are published\n\t\t\t\ton the White House Internet Web site upon their completion.", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}], "#text": "The\n\t\t\t\t"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "header": {"#tail": "\n                ", "#text": "Publication"}, "#text": "\n                ", "@id": "H548C6BFC16E6455CA4F058774871EF65", "@commented": "no"}], "enum": {"#tail": "\n              ", "#text": "(C)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Report"}, "#text": "\n              ", "@id": "H9E51808D34894BCC9B5167475BAB0DA0", "@commented": "no"}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", in cooperation with the\n\t\t\t\tStates, shall develop and publish guidelines to provide for the implementation\n\t\t\t\tof this paragraph.", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "#text": "The "}, "enum": {"#tail": "\n              ", "#text": "(D)"}, "header": {"#tail": "\n              ", "#text": "Implementation\n\t\t\t\tguidelines"}, "#text": "\n              ", "@id": "HA3570F8A03C342D09125298B292ABAF2"}], "enum": {"#tail": "\n            ", "#text": "(2)"}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Public\n\t\t\t\tparticipation"}, "#text": "\n            ", "@id": "H5A0896DF7B144608815916827DF57873"}, {"#tail": "\n        ", "subparagraph": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall cease\n\t\t\t\tto be effective following the last day of the 2-year period beginning on the\n\t\t\t\tdate on which the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a", "@proposed": "true"}, {"#tail": " issued the proclamation, unless the proclamation is\n\t\t\t\tapproved by an Act of Congress on or before that last day.", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}], "#text": "A proclamation issued under "}, "enum": {"#tail": "\n              ", "#text": "(A)"}, "header": {"#tail": "\n              ", "#text": "Approval\n\t\t\t\trequired"}, "#text": "\n              ", "@id": "HBAD9CE68049D44EBAA6EB195BE4165A8"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and the\n\t\t\t\tapproval of the proclamation under ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a", "@proposed": "true"}, {"#tail": ", the President shall ensure\n\t\t\t\tthat any restriction placed on land and interests, rights, or uses associated\n\t\t\t\twith the parcels of land designated as a national monument, including water\n\t\t\t\trights, hunting, recreational shooting, grazing, timber production, vegetation\n\t\t\t\tmanipulation to maintain forest health, off-road vehicle use, hiking, horseback\n\t\t\t\triding, and mineral and energy leases, claims, and permits, is narrowly\n\t\t\t\ttailored and essential to the proper care and management of the objects to be\n\t\t\t\tprotected.", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A", "@proposed": "true"}], "#text": "During\n\t\t\t\tthe period between the issuance of a proclamation under ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "header": {"#tail": "\n              ", "#text": "Management of\n\t\t\t\tland before approval"}, "#text": "\n              ", "@id": "H740D07FA744F434692B2C1E22C7C8556"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " does\n\t\t\t\tnot approve a proclamation to designate a national monument under ", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}, {"#tail": ", any reservation of land made by such proclamation, and any restriction\n\t\t\t\timposed as a result of such proclamation on interests, rights, or uses\n\t\t\t\tassociated with the parcels of land, shall cease to be effective following the\n\t\t\t\tlast day of the 2-year period referred to in ", "@entity-type": "act", "#text": "subparagraph\n\t\t\t\t(A)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A", "@proposed": "true"}, {"#tail": ".", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A", "@proposed": "true"}], "#text": "If ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "header": {"#tail": "\n              ", "#text": "Effect of\n\t\t\t\tnonapproval"}, "#text": "\n              ", "@id": "HDEF6F8E0B59747B7BD715C7B655198E9", "@commented": "no"}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may not issue a proclamation\n\t\t\t\tthat is substantially similar to a proclamation previously issued under\n\t\t\t\t", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": " that ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Antiquities Act of 1906/s:2/ss:a", "@proposed": "true"}, {"#tail": " has not approved under ", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}, {"#tail": ".", "@entity-type": "act", "#text": "subparagraph\n\t\t\t\t(A)", "@value": "Antiquities Act of 1906/s:2/ss:b/p:2/sp:A", "@proposed": "true"}], "#text": "The "}, "enum": {"#tail": "\n              ", "#text": "(D)"}, "header": {"#tail": "\n              ", "#text": "Prohibition on\n\t\t\t\trepeat proclamations"}, "#text": "\n              ", "@id": "H7856C54EEAC7450C888988DEC0E1842A"}], "enum": {"#tail": "\n            ", "#text": "(3)"}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Congressional\n\t\t\t\tapproval of proclamation"}, "#text": "\n            ", "@id": "HAF8EFBF09B4444EAB6F8BC1BF500E8BB"}], "#text": "\n          ", "@id": "H3E9F1E808364448C9320491A0B305882"}, "@display-inline": "no-display-inline", "#text": "\n\t\t\t\t", "@id": "HED50A5DC42774B89A1C609DC45A41242"}, "#text": "\n      ", "@id": "HD12D29AF0F2141248044271CECD292E5"}, {"#tail": "\n  ", "text": {"#tail": "\n\t\t\t", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is further amended by adding at the end the\n\t\t\t following:", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 2 of the\n\t\t\t Act of June 8, 1906", "@value": "Antiquities Act of 1906/s:2"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "uscode", "#text": "16 U.S.C. 431", "@value": "usc/16/431"}, "@parsable-cite": "usc/16/431", "#text": "\n            ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "4."}, "header": {"#tail": "\n      ", "#text": "Limitation on\n\t\t\t restrictions"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n      ", "#text": "."}, "#tail": "\n\t\t", "subsection": {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n\t\t\t\tshall ensure that any restriction placed on land and interests, rights, or uses\n\t\t\t\tassociated with the parcels of land designated as a national monument by a\n\t\t\t\tproclamation issued under this section is narrowly tailored and essential to\n\t\t\t\tthe proper care and management of the objects to be\n\t\t\t\tprotected.", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, "#text": "The ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n          ", "#text": "(c)"}, "header": {"#tail": "\n          ", "#text": "Limitation on\n\t\t\t\trestrictions"}, "#text": "\n          ", "@id": "H03D84AB4661149F59F170A429B589552"}, "@display-inline": "no-display-inline", "#text": "\n\t\t\t\t", "@id": "H7202008D076347CC99357F1D2DE75E13"}, "#text": "\n      ", "@id": "HCDAF5694034945C49C06133135C219E9"}], "#text": "\n\t\t", "@id": "HDA942F20B7064D0193F15E06CA1CDC6F"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-05-23"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 2192 IH: National Monument Designation Transparency and Accountability Act"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}