"subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "term": [{"#tail": ") shall be permitted to sponsor a public event, soap box derby races (in this resolution referred\n\t\t\t to as the ", "#text": "sponsor"}, {"#tail": "), on the Capitol Grounds.", "#text": "event"}], "#text": "The Greater Washington Soap Box Derby Association (in this resolution referred to as the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In General"}, "#text": "\n        ", "@id": "H2D20F3A398554A0D95FD861F84E1C4ED"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and the ", "@entity-parent-id": "0050", "@entity-type": "federal-body", "#text": "Speaker of the House of\n\t\t\t Representatives", "@value": "Speaker of the House of Representatives"}, {"#tail": " jointly designate.", "@entity-type": "committee", "#text": "Committee on Rules and Administration of the\n\t\t\t Senate", "@entity-id": "SSRA00"}], "#text": "The event shall be held on June 14, 2014, or on such other date as the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Date of Event"}, "#text": "\n        ", "@id": "H666D2C2299474C289BA7E6D864062803"}], "#text": "\n      ", "@id": "HE09B8B5C0A5F40389395C91D2B3CBFA7"}, {"#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n\t\t\t", "#text": "Terms and conditions"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and the ", "@entity-type": "federal-body", "#text": "Architect of the Capitol", "@entity-id": "0100"}, {"#tail": ", the\n\t\t\t event shall be\u2014", "@entity-type": "federal-body", "#text": "Capitol Police Board", "@entity-id": "0099"}], "#text": "Under conditions to be prescribed by the ", "@disp