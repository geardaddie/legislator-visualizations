{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "H. R. 1685"}, "official-title": {"#tail": "\n\t", "#text": "To amend the Communications Act of 1934 to reform and\n\t\t  modernize the Universal Service Fund Lifeline Assistance\n\t\t  Program."}, "session": {"#tail": "\n\t\t", "#text": "1st Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "I"}, "action": {"action-date": {"#tail": "\n\t\t\t", "@date": "20130423", "#text": "April 23, 2013"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "committee-name": {"#tail": "\n      ", "@committee-id": "HIF00", "#text": "Committee on Energy and\n\t\t\t Commerce"}, "sponsor": {"#tail": " (for\n\t\t\t herself, ", "#text": "Ms. Matsui", "@name-id": "M001163"}, "cosponsor": [{"#tail": ",\n\t\t\t ", "#text": "Mr. Waxman", "@name-id": "W000215"}, {"#tail": ",\n\t\t\t ", "#text": "Ms. Eshoo", "@name-id": "E000215"}, {"#tail": ",\n\t\t\t ", "#text": "Ms. DeGette", "@name-id": "D000197"}, {"#tail": ",\n\t\t\t ", "#text": "Ms. Lofgren", "@name-id": "L000397"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Doyle", "@name-id": "D000482"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Ben Ray Luj\u00e1n of New Mexico", "@name-id": "L000570"}, {"#tail": ", and\n\t\t\t ", "#text": "Ms. Schakowsky", "@name-id": "S001145"}, {"#tail": ") introduced the\n\t\t\t following bill; which was referred to the ", "#text": "Mr. Butterfield", "@name-id": "B001251"}]}, "#text": "\n\t\t\t"}, "legis-type": {"#tail": "\n\t\t", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-House", "@bill-type": "olc", "@dms-id": "H354D7CDEFDA04416B8340F8A42B66093", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "#text": "Broadband Adoption Act of\n\t\t\t 2013"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the\n\t\t\t ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H9547D996900449048456EE8CF1651281"}, {"@section-type": "subsequent-section", "#tail": "\n  ", "text": {"#tail": "\n\t\t\t", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended\u2014", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section\n\t\t\t 254(j) of the Communications Act of 1934", "@value": "Communications Act of 1934/s:254/ss:j"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "uscode", "#text": "47 U.S.C. 254(j)", "@value": "usc/47/254/j"}, "@parsable-cite": "usc/47/254", "#text": "\n            ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Broadband Lifeline\n\t\t\t Assistance Program"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "quote": {"#tail": " and inserting the\n\t\t\t following:", "header-in-text": {"#tail": "Nothing in this", "@style": "OLC", "enum-in-header": {"#tail": " Lifeline\n\t\t\t Assistance.\u2014", "#text": "(j)"}, "@level": "subsection", "#text": "\n              "}, "#text": "\n            "}, "#text": "by striking\n\t\t\t "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": ";\n\t\t\t\tand"}, "#tail": "\n\t\t\t", "subsection": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "(j)"}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Lifeline\n\t\t\t\tAssistance"}, "paragraph": {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "Nothing in\n\t\t\t\tthis", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(1)"}, "header": {"#tail": "\n              ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n              ", "@id": "H75D7B84944EE438386AA531AB112EFD3"}, "#text": "\n            ", "@id": "H3D3EF1C30D2241A29FCDFE08ECE785BE"}, "@display-inline": "no-display-inline", "#text": "\n\t\t\t\t\t", "@id": "HACF84C319E064CFCB151F7807EBDE7F6"}, "#text": "\n        ", "@id": "H23BDF6F4932B44C8A95808F8DFB2B9A9"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t\t", "#text": "by adding at the\n\t\t\t end the following:"}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "paragraph": {"#tail": "\n          ", "subparagraph": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "The purpose of this paragraph is to promote\n\t\t\t\tthe adoption of broadband service by all people of the United States while\n\t\t\t\trecognizing that the price of broadband service is one of the barriers to\n\t\t\t\tadoption for low income households.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(A)"}, "header": {"#tail": "\n              ", "#text": "Purpose"}, "#text": "\n              ", "@id": "H6B45792662784510986460592F61D05E"}, {"#tail": "\n            ", "clause": [{"#tail": "\n              ", "text": {"short-title": {"#tail": ", the\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                  ", "@entity-type": "act", "#text": "Broadband Adoption Act of 2013", "@value": "Broadband Adoption Act of 2013", "@proposed": "true"}, "#text": "\n                    "}, "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall adopt a final rule establishing Lifeline program support for\n\t\t\t\tbroadband that enables qualifying low-income customers residing in urban and\n\t\t\t\trural areas to purchase broadband service at reduced charges by reimbursing\n\t\t\t\tproviders who elect to participate in the program for each such customer\n\t\t\t\tserved. The ", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}, {"#tail": " shall consider the results of the Low-Income Broadband\n\t\t\t\tPilot Program when establishing such final rule.", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}], "#tail": "\n\t\t\t\t\t\t\t", "#text": "Not later than 270\n\t\t\t\tdays after the date of enactment of the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n                ", "@id": "HDDB8368E6B404BC587FADB16BCB1C6E5"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Such\n\t\t\t\tprogram shall be similar in structure to the Lifeline program for basic\n\t\t\t\ttelephone service under subpart E of part 54 of title 47, Code of Federal\n\t\t\t\tRegulations. Qualifying individuals may elect to apply support from the\n\t\t\t\tLifeline program to basic telephone service, voice telephony service, or\n\t\t\t\tbroadband service, whether each service is purchased stand-alone or in a\n\t\t\t\tbundle."}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "header": {"#tail": "\n                ", "#text": "Model"}, "#text": "\n                ", "@id": "HB3DD469B6AFF4FCBA4FFD3C001B61BE6"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall consider providing a\n\t\t\t\tpreference to participating broadband service providers that include components\n\t\t\t\tinvolving digital literacy programs as part of their offerings.", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}, "#text": "The "}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "header": {"#tail": "\n                ", "#text": "Digital\n\t\t\t\tliteracy program"}, "#text": "\n                ", "@id": "HD469998383944D9086B40B701E54012D"}], "enum": {"#tail": "\n              ", "#text": "(B)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Establishment"}, "#text": "\n              ", "@id": "H49C67266B4E44CFEB575E93E7F61667A"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may\n\t\t\t\tdetermine, in consultation with the Federal-State Joint Board instituted under\n\t\t\t\t", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}, {"#tail": ", whether State matching funds may be provided as a condition\n\t\t\t\tof eligibility for low-income households within such State.", "@entity-type": "act", "#text": "subsection (a)(1)", "@value": "Communications Act of 1934/s:254/ss:a/p:1"}], "#text": "The ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "header": {"#tail": "\n              ", "#text": "State matching\n\t\t\t\tfunds"}, "#text": "\n              ", "@id": "H7A24939CD5424B318D93F3EDCADBFCA1", "@commented": "no"}, {"#tail": "\n            ", "clause": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", in calculating the amount of Lifeline\n\t\t\t\tsupport to be provided to each low-income household, shall routinely study the\n\t\t\t\tprevailing market price for broadband service and the prevailing speed of\n\t\t\t\tbroadband service adopted by households.", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}, "#text": "The "}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n                ", "@id": "H4B643898FAF9426E88B661BE88B1AB9E"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", the ", "@entity-type": "act", "#text": "clause (i)", "@value": "Communications Act of 1934/s:254/ss:j/p:2/sp:D/cl:i", "@proposed": "true"}, {"#tail": " shall rely on\n\t\t\t\tinformation that it routinely collects or that is publicly available.", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}], "#text": "To\n\t\t\t\tfulfill the requirement under "}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "header": {"#tail": "\n                ", "#text": "Information"}, "#text": "\n                ", "@id": "H9E7CCDE61D434B46A658A0EBF531E3CB"}], "enum": {"#tail": "\n              ", "#text": "(D)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Amount of\n\t\t\t\tsupport"}, "#text": "\n              ", "@id": "HA52B670E3C414E348B1353B17D934736"}, {"#tail": "\n            ", "clause": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall ensure that the program established\n\t\t\t\tunder ", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}, {"#tail": " is neutral as to the types of technology used to\n\t\t\t\tprovide voice telephony or broadband service under the Lifeline program to\n\t\t\t\tpromote competition from service providers to qualify under such\n\t\t\t\tprogram.", "@entity-type": "act", "#text": "subparagraph (B)(i)", "@value": "Communications Act of 1934/s:254/ss:j/p:2/sp:B/cl:i", "@proposed": "true"}], "#text": "The "}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "header": {"#tail": "\n                ", "#text": "In\n\t\t\t\tgeneral"}, "#text": "\n                ", "@id": "HC4328837D5874A50B70261322BBE8677"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " in order to participate\n\t\t\t\tin the program.", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}, "#text": "A\n\t\t\t\tparticipating broadband service provider need not be an eligible\n\t\t\t\ttelecommunications carrier to receive support under such program, but such\n\t\t\t\tprovider shall obtain authorization from the "}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "header": {"#tail": "\n                ", "#text": "Authorization"}, "#text": "\n                ", "@id": "HDE882625AD7B42C183BC009FC08EC7E3"}], "enum": {"#tail": "\n              ", "#text": "(E)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Technology\n\t\t\t\tneutral"}, "#text": "\n              ", "@id": "H9483F428ED114DBE9D840088A31E1B37"}, {"#tail": "\n          ", "clause": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", the ", "@entity-type": "act", "#text": "subparagraph (B)(i)", "@value": "Communications Act of 1934/s:254/ss:j/p:2/sp:B/cl:i", "@proposed": "true"}, {"#tail": " shall adopt regulations to prevent\n\t\t\t\tduplicative Lifeline subsidies from being awarded to an individual eligible\n\t\t\t\thousehold. Only one Lifeline program support amount per eligible household\n\t\t\t\tshall be available to qualifying individuals.", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}], "#text": "In establishing the program under\n\t\t\t\t", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "header": {"#tail": "\n                ", "#text": "Nonduplication"}, "#text": "\n                ", "@id": "HC4017C611ECC4253AE4A0FE371841434"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall consider any appropriate measures to prevent\n\t\t\t\tany waste, fraud, or abuse of this program.", "@entity-type": "federal-body", "#text": "Commission", "@entity-id": "2700"}, "#text": "In adopting rules to implement this\n\t\t\t\tparagraph, the "}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "header": {"#tail": "\n                ", "#text": "Preventing\n\t\t\t\twaste, fraud, or abuse"}, "#text": "\n                ", "@id": "HFFF7C183FD394B579583BE4C4CB10B80"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "The\n\t\t\t\tCommission, in consultation with other relevant Federal agencies, shall\n\t\t\t\testablish a national database which can be used to determine consumer\n\t\t\t\teligibility for Lifeline program subsidies."}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "header": {"#tail": "\n                ", "#text": "Eligibility"}, "#text": "\n                ", "@id": "HCD1DB4DEB95248848CBE6038F04A4267"}], "enum": {"#tail": "\n              ", "#text": "(F)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Accountability"}, "#text": "\n              ", "@id": "HD0AC6D5A21C34EC9B0CC572C179A6426"}], "enum": {"#tail": "\n            ", "#text": "(2)"}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Broadband\n\t\t\t\tLifeline Assistance Program"}, "#text": "\n            ", "@id": "H11790B14AEDB44BB82CAB7866D60BF51"}, "@display-inline": "no-display-inline", "#text": "\n\t\t\t\t\t", "@id": "H533FC92A17D447C59FBDB1F2ABD96D19"}, "#text": "\n        ", "@id": "H5F08A33F0E31485BB3E87D7B98F9CF05"}], "#text": "\n      ", "@id": "H2A06225D3DB84332BE9D54E763BFC62E"}], "#text": "\n\t\t", "@id": "HD65572E8F42E449BA1D7C3AAF59DD2B1"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-04-23"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 1685 IH: Broadband Adoption Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}