{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "@display": "yes", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "H. R. 3223"}, "official-title": {"#tail": "\n  ", "@display": "yes", "#text": "To provide for the compensation of furloughed Federal employees."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "II"}, "action": [{"action-date": {"#tail": "\n      ", "#text": "October 5, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "Received; read the first time"}, "#text": "\n      "}, {"action-date": {"#tail": "\n      ", "#text": "October 7, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "Read the second time and placed on the calendar"}, "#text": "\n      "}], "calendar": {"#tail": "\n    ", "#text": "Calendar No. 208"}, "legis-type": {"#tail": "\n    ", "#text": "AN ACT"}}, "attestation": {"#tail": "\n  ", "attestation-group": {"#tail": "\n  ", "role": {"#tail": "\n    ", "#text": "Clerk"}, "#text": "\n      ", "attestor": {"#tail": "\n      ", "@display": "yes", "#text": "Karen L. Haas,"}, "attestation-date": {"#tail": "\n      ", "@date": "20131005", "#text": "Passed the House of Representatives October 5, 2013.", "@chamber": "House"}}, "#text": "\n    "}, "@stage-count": "1", "@bill-stage": "Placed-on-Calendar-Senate", "@bill-type": "olc", "endorsement": {"action-date": {"#tail": "\n    ", "#text": "October 7, 2013"}, "#tail": "\n", "action-desc": {"#tail": "\n  ", "#text": "Read the second time and placed on the calendar"}, "#text": "\n    "}, "@dms-id": "H5335BDEDCFEA42EDA7D91ACFE1ECD123", "@public-private": "public", "legis-body": {"#tail": "\n  ", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-tit