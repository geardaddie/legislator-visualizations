-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "#text": "Freedom From Union Violence Act of\n\t\t\t 2013"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the\n\t\t\t ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "@display-inline": "no-display-inline", "#text": "\n      ", "@id": "H104E0C0DA90C41FC9B0D007EC51DBBE1"}, {"#tail": "\n  ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended to read as follows:", "@entity-type": "uscode", "#text": "\n          ", "external-xref": {"#tail": " of title 18, United States\n\t\t\t Code", "@parsable-cite": "usc/18/1951", "#text": "Section 1951", "@legal-doc": "usc"}, "@value": "usc/18/1951"}, "#text": "\n        ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Interference with\n\t\t\t commerce by threats or violence"}, "quoted-block": {"@style": "USC", "after-quoted-block": {"#tail": "\n      ", "#text": "."}, "#tail": "\n\t\t", "section": {"#tail": "\n        ", "enum": {"#tail": "\n          ", "#text": "1951."}, "header": {"#tail": "\n\t\t\t\t\t", "#text": "Interference\n\t\t\t\twith commerce by threats or violence"}, "subsection": [{"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", whoever in any way or degree obstructs, delays,\n\t\t\t\tor affects commerce or the movement of any article or commodity in commerce, by\n\t\t\t\trobbery or extortion, or attempts or conspires so to do, or commits or\n\t\t\t\tthreatens physical violence to any person or property in furtherance of a plan\n\t\t\t\tor purpose to do anything in violation of this section, shall be fined not more\n\t\t\t\tthan $100,000, imprisoned for a term of not more than 20 years, or both.", "@entity-type": "uscode", "#text": "subsection (c)", "@value": "usc/18/1951/c", "@proposed": "true"}, "#text": "Except\n\t\t\t\tas provided in "}, "enum": {"#tail": "\n            ", "#text": "(a)"}, "header": {"#tail": "\n            ", "#text": "Prohibition"}, "#text": "\n            ", "@id": "HC706E43F65AB49E98B8F8CAD0A8E6503"}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "For\n\t\t\t\tpurposes of this section\u2014"}, "enum": {"#tail": "\n            ", "#text": "(b)"}, "header": {"#tail": "\n            ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": " means any\u2014", "#text": "commerce"}, "#text": "the term\n\t\t\t\t"}, "enum": {"#tail": "\n              ", "#text": "(1)"}, "#text": "\n              ", "@id": "HE4FAEF280CA548C5BEA822A89BD59134", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "commerce within\n\t\t\t\tthe District of Columbia, or any territory or possession of the United\n\t\t\t\tStates;"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "HA9CA05B7657C4348B1EF34EFD67028B0", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "commerce between\n\t\t\t\tany point in a State, territory, possession, or the District of Columbia and\n\t\t\t\tany point outside thereof;"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "HCF3AEB8A8D1F4BD48635B4D26C7EF601", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "commerce between\n\t\t\t\tpoints within the same State through any place outside that State; and"}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@id": "HA04F2937BA734BFC8C03D31E35726645", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "other commerce\n\t\t\t\tover which the United States has jurisdiction;"}, "enum": {"#tail": "\n                ", "#text": "(D)"}, "@id": "H47A7E7433BFA4F0D8D6FE600B5C70808", "#text": "\n                "}]}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": " means the obtaining of property from any person, with\n\t\t\t\tthe consent of that person, if that consent is induced\u2014", "#text": "extortion"}, "#text": "the term\n\t\t\t\t"}, "enum": {"#tail": "\n              ", "#text": "(2)"}, "#text": "\n              ", "@id": "H8EFB34C0F29340FEA81C19423E9E9E82", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "by actual or\n\t\t\t\tthreatened use of force or violence, or fear thereof;"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "H5FA86576323C4882B0829C641B401C24", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "by wrongful use of\n\t\t\t\tfear not involving force or violence; or"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "H15F1C4F12B0F4E698041C97A45F3F616", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "under color of\n\t\t\t\tofficial right;"}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@id": "H0033B085130240068D056E3116BC416A", "#text": "\n                "}]}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "term": {"#tail": " has the same meaning as in ", "#text": "labor dispute"}, "#text": "the term\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity": {"#tail": "; and", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "section 2(9) of the\n\t\t\t\tNational Labor Relations Act", "@value": "National Labor Relations Act/s:2/p:9"}, "@entity-type": "law-citation", "#text": "\n                  ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                  ", "@entity-type": "uscode", "#text": "29 U.S.C. 152(9)", "@value": "usc/29/152//9"}, "@parsable-cite": "usc/29/152", "#text": "\n                    ", "@legal-doc": "usc"}}}, "enum": {"#tail": "\n              ", "#text": "(3)"}, "@id": "H8AF5546204F649C5B40CC8D94496D15E", "#text": "\n              "}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": " means the unlawful taking or obtaining of personal\n\t\t\t\tproperty from the person or in the presence of another, against his or her\n\t\t\t\twill, by means of actual or threatened force or violence, or fear of injury,\n\t\t\t\timmediate or future\u2014", "#text": "robbery"}, "#text": "the term\n\t\t\t\t"}, "enum": {"#tail": "\n              ", "#text": "(4)"}, "#text": "\n              ", "@id": "HA5B6150275044B92B7AE51EBEDB9D0E2", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "to his or her\n\t\t\t\tperson or property, or property in his or her custody or possession; or"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "H687DC1FECE80449C9C1D3EBC56923952", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "to the person or\n\t\t\t\tproperty of a relative or member of his or her family, or of anyone in his or\n\t\t\t\ther company at the time of the taking or obtaining."}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "H71F7F6089932468B9E34A003EFA46659", "#text": "\n                "}]}], "#text": "\n            ", "@id": "H3D0F55BACD1D42FB8674EC000371F9B6"}, {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "(c)"}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Exempted\n\t\t\t\tconduct"}, "paragraph": [{"#tail": "\n            ", "@id": "HAF089DF27221412E8EC9727DC6BA1984", "enum": {"#tail": "\n              ", "#text": "(1)"}, "header": {"#tail": "\n              ", "#text": "In\n\t\t\t\tgeneral"}, "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " does not apply to any conduct that\u2014", "@entity-type": "uscode", "#text": "Subsection (a)", "@value": "usc/18/1951/a", "@proposed": "true"}, "#text": "\n                "}, "#text": "\n              ", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "is incidental to\n\t\t\t\totherwise peaceful picketing during the course of a labor dispute;"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@id": "HE25ACB3336B04102A1E3444F8C73DAED", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "consists solely of\n\t\t\t\tminor bodily injury, or minor damage to property, or threat or fear of such\n\t\t\t\tminor injury or damage; and"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "HC428A6CCB75640DEA0A885727846D69E", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "is not part of a\n\t\t\t\tpattern of violent conduct or of coordinated violent activity."}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@id": "H0C20C5CACFA34453B8E2919C8E45BBD0", "#text": "\n                "}]}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall be subject to prosecution only by the\n\t\t\t\tappropriate State and local authorities.", "@entity-type": "uscode", "#text": "paragraph (1)", "@value": "usc/18/1951/c/1", "@proposed": "true"}, "#text": "Any violation of this section that involves any\n\t\t\t\tconduct described in "}, "enum": {"#tail": "\n              ", "#text": "(2)"}, "header": {"#tail": "\n              ", "#text": "State and local\n\t\t\t\tjurisdiction"}, "#text": "\n              ",