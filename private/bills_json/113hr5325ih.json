", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "American Manufacturing Workforce Act of 2014", "@value": "American Manufacturing Workforce Act of 2014", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H9BFB1AB3058845EEA106C31A7401EFFB"}, {"#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n\t\t\t", "#text": "Refundable Tax Credit for Unemployed Workers Obtaining Manufacturing Job Training"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " (relating to\n\t\t\t refundable credits) is amended by inserting after ", "@entity-type": "act", "#text": "Subpart C of part IV of subchapter A of ", "external-xref": {"#tail": " of the Internal Revenue Code of 1986", "@parsable-cite": "usc-chapter/26/1", "#text": "chapter 1", "@legal-doc": "usc-chapter"}, "@value": "Internal Revenue Code of 1986/ch:1/sch:A/pt:IV/spt:C"}, {"#tail": " the\n\t\t\t following new section:", "@entity-type": "act", "#text": "section 36B", "@value": "Internal Revenue Code of 1986/s:36B"}], "#text": "\n          ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "section": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "36C."}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Unemployed workers obtaining manufacturing job training"}, "subsection": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "In the case of an eligible individual, there shall be allowed as a credit against the tax imposed\n\t\t\t by this subtitle for the taxable year with respect to each eligible\n\t\t\t individual an amount equal to the eligible individual\u2019s qualified training\n\t\t\t costs paid or incurred by the taxpayer during the taxable year.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(a)"}, "header": {"#tail": "\n              ", "#text": "In general"}, "#text": "\n              ", "@id": "H3779666CD8534CC3A96139C78C00E973"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "The credit allowed under subsection (a) with respect to each eligible individual shall not exceed\n\t\t\t $1,000."}, "enum": {"#tail": "\n              ", "#text": "(b)"}, "header": {"#tail": "\n              ", "#text": "Limitation"}, "#text": "\n              ", "@id": "H68CDD14464CA464680D61164446C39DA"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "For purposes of this section\u2014"}, "enum": {"#tail": "\n              ", "#text": "(c)"}, "header": {"#tail": "\n              ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "term": {"#tail": " means expenses for tuition, fees, and course materials paid or incurred in qualified manufacturing\n\t\t\t training.", "#text": "qualified training costs"}, "#text": "The term "}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "Qualified training costs"}, "#text": "\n                ", "@id": "H4C859870244E4A8F8BC899E4862ADE06"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "federal-body", "#text": "Secretary of Labor", "@entity-id": "1600"}, "term": {"#tail": " means training to develop or better skills for a manufacturing position in the manufacturing\n\t\t\t industry, as determined by the ", "#text": "qualified manufacturing job training"}, "#text": "The term ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Qualified manufacturing training"}, "#text": "\n                ", "@id": "HACB3254FB8DC4AF0A9062EDB9205C193"}, {"#tail": "\n            ", "@id": "HFAE51521EB60434A8EF6842B4B0E8B39", "enum": {"#tail": "\n                ", "#text": "(3)"}, "header": {"#tail": "\n                ", "#text": "Eligible individual"}, "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "term": {"#tail": " means an individual who\u2014", "#text": "eligible individual"}, "#text": "For purposes of this section, the term ", "@display-inline": "yes-display-inline"}, "#text": "\n                ", "subparagraph": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "is the taxpayer or the taxpayer\u2019s spouse or dependent,"}, "enum": {"#tail": "\n                  ", "#text": "(A)"}, "@id": "H05474F63FB124EFFBFE02E9F64172F4F", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "is certified by the State employment security agency established in accordance with the Act of June\n\t\t\t 6, 1933, as amended (", "external-xref": {"#tail": "), as having been in receipt of\n\t\t\t unemployment compensation under State or Federal law on any day within the\n\t\t\t 1-year period ending on the date qualified manufacturing job training\n\t\t\t begins, and", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                    ", "@entity-type": "uscode", "#text": "\n                      29 U.S.C. 49\u201349n", "@value": "usc/29/49\u201349n"}, "@parsable-cite": "usc/29/49-49n", "#text": "\n                      "