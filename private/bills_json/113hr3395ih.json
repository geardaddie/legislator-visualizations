": [{"#tail": "\n", "text": {"#tail": "\n", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " is amended to read as follows:", "@entity-type": "act", "#text": "Subparagraph (D) of ", "external-xref": {"#tail": " of the Internal Revenue Code of 1986", "@parsable-cite": "usc/26/51", "#text": "section 51(d)(13)", "@legal-doc": "usc"}, "@value": "Internal Revenue Code of 1986/s:51/ss:d/p:13/sp:D"}, "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "quoted-block": {"@act-name": "", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "@style": "OLC", "#tail": "\n      ", "@id": "HE3E9A4F84E354518A6D4B1E471EABDEA", "#text": "\n", "subparagraph": {"#tail": "\n          ", "clause": [{"#tail": "\n", "subclause": [{"#tail": "\n", "text": {"#tail": "\n              ", "#text": "qualified veteran status documentation,"}, "enum": {"#tail": "\n                ", "#text": "(I)"}, "@id": "H49B62F96A8964A4091C333F3A949A55C", "#text": "\n                "}, {"#tail": "\n", "text": {"#tail": "\n              ", "#text": "qualified proof of unemployment compensation, and"}, "enum": {"#tail": "\n                ", "#text": "(II)"}, "@id": "HAC8FFB77CA004A7E886838BB3D71087C", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and ", "@entity-type": "act", "#text": "subclauses (I)", "@value": "Internal Revenue Code of 1986/s:51/ss:d/p:13/sp:D/cl:i/scl:I", "@proposed": "true"}, {"#tail": " is true.", "@entity-type": "act", "#text": "(II)", "@value": "Internal Revenue Code of 1986/s:51/ss:d/p:13/sp:D/cl:i/scl:II", "@proposed": "true"}], "#text": "an affidavit furnished by the individual stating, under penalty of perjury, that the information provided under "}, "enum": {"#tail": "\n                ", "#text": "(III)"}, "@id": "HA23E34C783024874BAAF1A3A57386701", "#text": "\n                "}], "enum": {"#tail": "\n              ", "#text": "(i)"}, "header": {"#tail": "\n              ", "#text": "In general"}, "text": {"#tail": "\n", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall be applied without regard to ", "@entity-type": "act", "#text": "Subparagraph (A)", "@value": "Internal Revenue Code of 1986/s:51/ss:d/p:13/sp:A"}, {"#tail": " thereof in the case of an individual seeking treatment as a qualified veteran with respect to whom the pre-screening notice contains\u2014", "@entity-type": "act", "#text": "subclause (II) of clause (ii)", "@value": "Internal Revenue Code of 1986/s:51/ss:d/p:13/sp:A/cl:ii/scl:II"}], "#text": "\n                "}, "#text": "\n              ", "@id": "HFF0CFEA5E2B0477BACE81175FAB0A8A7"}, {"#tail": "\n", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", the term ", "@entity-type": "act", "#text": "clause (i)", "@value": "Internal Revenue Code of 1986/s:51/ss:d/p:13/sp:D/cl:i", "@proposed": "true"}, {"#tail": " or the National Guard upon release or discharge from the Armed Forces which includes information sufficient to establish that such individual is a veteran.", "@entity-type": "federal-body", "#text": "Department of Defense", "@entity-id": "9700"}], "term": {"#tail": " means any documentation provided to an individual by the ", "#text": "qualified veteran status documentation"}, "#text": "For purposes of "}, "enum": {"#tail": "\n              ", "#text": "(ii)"}, "header": {"#tail": "\n              ", "#text": "Qualified veteran status documentation"}, "#text": "\n              ", "@id": "HA119CFFD79F04478A7B034539F999162"}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", the term ", "@entity-type": "act", "#text": "clause (i)", "@value": "Internal Revenue Code of 1986/s:51/ss:d/p:13/sp:D/cl:i", "@proposed": "true"}, {"#tail": "), or not less than 6 months (i