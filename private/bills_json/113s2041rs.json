-private": "public", "legis-body": {"#tail": "\n  ", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "May 31, 1918 Act Repeal Act", "@value": "May 31, 1918 Act Repeal Act", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "idD3A7739F6347412AB822D7EB36A901DA"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "In this Act:", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ").", "@entity-type": "statute-at-large", "#text": "40 Stat. 592, chapter 88", "@value": "statute-at-large/40/592"}, "term": {"#tail": " means the \n            Act of May 31, 1918 (", "#text": "1918 Act"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "header": {"#tail": "\n        ", "#text": "1918 Act"}, "#text": "\n        ", "@id": "id95A99B59F1AA4F54B6D8A8AB6D3D3010"}, {"@committee-id": "SLIA00", "@changed": "added", "#tail": "\n      ", "text": {"#tail": "\n      ", "quote": {"#tail": " on file with Bingham County, Idaho and the Tribes.", "#text": "Plat of the Townsite of Fort Hall"}, "term": {"#tail": " means the  land that was taken out of trust by being set aside or set apart under the 1918 Act on\n\t\t\t the Fort Hall Reservation, consisting of approximately 120 acres in the\n\t\t\t East Half of the Northeast Quarter in Section 35 and the West Half of the\n\t\t\t West Half of the Northwest Quarter in Section 36, Township 4 South, Range\n\t\t\t 34 East, Boise Meridian, Idaho, based upon a survey completed on May 19,\n\t\t\t 1921, and depicted on the document entitled  ", "#text": "Fort Hall Townsite"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "header": {"#tail": "\n        ", "#text": "Fort Hall Townsite"}, "@reported-display-style": "italic", "#text": "\n        ", "@id": "id7BF22BED84924DB7B59FF4E34B7F860F"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "deleted-phrase": {"#tail": " Reservation.", "@committee-id": "SLIA00", "#text": "Indian", "@reported-display-style": "strikethrough"}, "term": {"#tail": " means the Shoshone-Bannock Tribes of the Fort Hall ", "#text": "Tribes"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "deleted-phrase": {"#tail": "\n          ", "@committee-id": "SLIA00", "#text": "(2)", "@reported-display-style": "strikethrough"}, "#text": "\n          ", "italic": {"#tail": "\n        ", "#text": " (3)"}}, "header": {"#tail": "\n        ", "#text": "Tribes"}, "#text": "\n        ", "@id": "id7BF21A8B7ACA41109EA8D10C13D98FFD"}], "#text": "\n      ", "@id": "idF49EDC0A605E44C284FEE266F80968E7"}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "The 1918 Act is repealed.", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "Repeal"}, "#text": "\n      ", "@id": "idEDA882DFA7694727BA63995B410B4A78"}, {"#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "4."}, "header": {"#tail": "\n      ", "#text": "Right of first refusal"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "The Tribes shall have the exclusive right of first refusal to purchase at fair market value any\n\t\t\t land\u2014"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "deleted-phrase": {"#tail": "\n            ", "@committee-id": "SLIA00", "#text": "set aside or set apart under the 1918 Act", "@reported-display-style": "strikethrough"}, "#text": "\n            ", "added-phrase": {"#tail": "; and", "@committee-id": "SLIA00", "#text": "within the Fort Hall Townsite", "@reported-display-style": "italic"}}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "id084AB3806C854C3290451978A2E2B19E", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "offered for sale."}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@id": "id62CA6BF489AA4AF7B254961B72430BC5", "#text": "\n          "}], "#text": "\n        ", "@id": "id337B8B9C36BE4678976E1C7910FE91EC"}, {"#tail": "\n    ", "text": {"#tail": "\n        ", "#text": "The United States shall hold in\n\t\t\t trust for the benefit of the Tribes or a member of the Tribes, as\n\t\t\t applicable\u2014"}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Acquired land held in trust"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "any land ", "added-phrase": [{"#tail": " acquired by the Tribes or a member of the Tribes ", "@committee-id": "SLIA00", "#text": "owned or", "@reported-display-style": "italic"}, {"#tail": " before the date of enactment of this Act;\n\t\t\t and", "@committee-id": "SLIA00", "#text": "within the Fort Hall Townsite", "@reported-display-style": "italic"}]}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "id7426227704BE4D27BD6A33924F229DBA", "#text": "\n          "}, {"@committee-id": "SLIA00", "@changed": "deleted", "#tail": "\n        ", "text": {"#tail": "\n          ", "#text": "any land\u2014"}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@reported-display-style": "strikethrough", "#text": "\n          ", "@id": "id2DE4D5A6DC794E349FAFEFA775A72E08", "subparagraph": [{"#tail": "\n          ", "text": {"#tail": "\n          ", "#tex