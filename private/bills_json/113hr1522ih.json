ted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "quoted-block-continuation-text": {"#tail": "\n          ", "#text": "The\n\t\t\t\tpreceding sentence shall not apply to any manufacturing recovery\n\t\t\t\temployee.", "@quoted-block-continuation-text-level": "paragraph"}, "@display-inline": "no-display-inline", "#text": "\n\t\t\t\t\t", "@id": "H80C6F7F9FF9B4B50A0CCFE7DF3C2DE45"}, "#text": "\n        ", "@id": "H1C7A99D5C91741F48ED05CACD0EEFFF1"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "#text": "The amendments made by this section shall apply to\n\t\t\t individuals who first begin work for the employer after the date of the\n\t\t\t enactment of this Act."}, "enum": {"#tail": "\n        ", "#text": "(d)"}, "header": {"#tail": "\n        ", "#text": "Effective\n\t\t\t date"}, "#text": "\n        ", "@id": "H4909C81B7F49479784F9260BFA0C9A5E"}], "#text": "\n      ", "@id": "HB4EBACCD95F448268839EDA8FA66F206"}], "#text": "\n\t\t", "@id": "H9CA07EA49C374CA58936D4EE0F992FF0"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-04-12"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 1522 IH: Manufacturing Economic Recovery Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}