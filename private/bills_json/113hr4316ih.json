@bill-type": "olc", "@dms-id": "H059D430119124FDEB5EB50054B8649FE", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "Endangered Species Recovery Transparency Act", "@value": "Endangered Species Recovery Transparency Act", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H78B7E244266D498DA34997A62BFD4274"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n\t\t\t", "#text": "Disclosure of expenditures under Endangered Species Act of 1973"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "#text": "\n          ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": "; relating to conforming amendments\n\t\t\t which have executed) is amended to read as follows:", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " (", "@entity-type": "act", "#text": "Section 13 of the Endangered Species Act of 1973", "@value": "Endangered Species Act of 1973/s:13"}, {"#tail": "\n          ", "@entity-type": "statute-at-large", "#text": "87 Stat. 902", "@value": "statute-at-large/87/902"}], "@entity-type": "law-citation", "#text": "\n            "}}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "Requirement To disclose"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "section": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "13."}, "header": {"#tail": "\n\t\t\t\t\t\t", "#text": "Disclosure of expenditures"}, "subsection": [{"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", in consultation with the ", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, {"#tail": ", shall\u2014", "@entity-type": "federal-body", "#text": "Secretary of Commerce", "@entity-id": "1300"}], "#text": "The "}, "enum": {"#tail": "\n              ", "#text": "(a)"}, "header": {"#tail": "\n              ", "#text": "Requirement"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and the ", "@entity-type": "committee", "#text": "Committee on Natural\n\t\t\t Resources of the House of Representatives", "@entity-id": "HII00"}, {"#tail": " an annual report detailing Federal\n\t\t\t Government expenditures for covered suits during the preceding fiscal year\n\t\t\t (including the information described in ", "@entity-type": "committee", "#text": "Committee on Energy and\n\t\t\t Natural Resources of the Senate", "@entity-id": "SSEG00"}, {"#tail": "); and", "@entity-type": "act", "#text": "subsection (b)", "@value": "Endangered Species Act of 1973/s:13/ss:b", "@proposed": "true"}], "#text": "not later than 90 days after the end of each fiscal year, submit to the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "@id": "HEB82905804AA411B951C7A4BB9937443", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "subsection (b)", "@value": "Endangered Species Act of 1973/s:13/ss:b", "@proposed": "true"}, "#text": "make publicly available through the Internet a searchable database of the information described in\n\t\t\t "}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "@id": "H8D1BDD072930448E960D59687AD5873D", "#text": "\n                "}], "#text": "\n              ", "@id": "H94680BA9D14C47EC854D24CFBDB71184"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "The report shall include\u2014"}, "enum": {"#tail": "\n              ", "#text": "(b)"}, "header": {"#tail": "\n              ", "#text": "Included information"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "the case name and number of each covered suit, and a hyperlink to the record or decision for each\n\t\t\t covered suit (if available);"}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "@id": "H8D45E7C47DBD47F5BD8AACBE82D26FAE", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "a description of the claims in each covered suit;"}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "@id": "HA255CD0287094E43B39B567D141A7C59", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "the name of each covered agency whose actions gave rise to a claim in a covered suit;"}, "enum": {"#tail": "\n                ", "#text": "(3)"}, "@id": "H9407DF3765A14893A8A83515E4C99763", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " or to prepare for litigation of,\n\t\t\t litigate, negotiate a settlement agreement or consent decree in, or\n\t\t\t provide material, technical, or other assistance in relation to, a covered\n\t\t\t suit;", "@entity-type": "act", "#text": "section 11(g)(2)", "@value": "Endangered Species Act of 1973/s:11/ss:g/p:2"}, "#text": "funds expended by each covered agency (disaggregated by agency account) to receive and respond to\n\t\t\t notices referred to in ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(4)"}, "@id": "HFB2112FAFCDF4AF98E47894A74A9EE32", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "; and", "@entity-type": "act", "#text": "paragraph (4)", "@value": "Endangered Species Act of 1973/s:13/ss:b/p:4", "@proposed": "true"}, "#text": "the number of full-time equivalent employees that participated in the activities described in\n\t\t\t "}, "enum": {"#tail": "\n                ", "#text": "(5)"}, "@id": "HF9C9D5AE29184175A2302CB2890C14FC", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "attorneys fees and other expenses (disaggregated by agency account) awarded in covered suits,\n\t\t\t including any consent decrees or settlement agreements (regardless of\n\t\t\t whether a decree or settlement agreement is sealed or otherwise subject to\n\t\t\t nondisclosure provisions), including the bases for such awards.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(6)"}, "@id": "HE4795CB500654947A474514960553B2E", "#text": "\n                "}], "#text": "\n              ", "@id": "H63FA112CCB9D489484F4194D694968A3"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "#text": "The head of each covered agency shall provide to the Secretary in a timely manner all information\n\t\t\t requested by the Secretary to comply with the requirements of this\n\t\t\t section."}, "enum": {"#tail": "\n              ", "#text": "(c)"}, "header": {"#tail": "\n              ", "#text": "Requirement To provide information"}, "#text": "\n              ", "@id": "HEF87E7077BEE4FC393AFE7719D88957D"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "subsection (b)", "@value": "Endangered Species Act of 1973/s:13/ss:b", "@proposed": "true"}, "#text": "Notwithstanding any other provision of this section, this section shall not affect any restriction\n\t\t\t in a consent decree or settlement agreement on the disclosure of\n\t\t\t information that is not described in "}, "enum": {"#tail": "\n              ", "#text": "(d)"}, "header": {"#tail": "\n              ", "#text": "Limitation on disclosure"}, "#text": "\n              ", "@id": "H3E775246817C4E01AEFAB7CE6304D560"}, {"#tail": "\n          ", "enum": {"#tail": "\n              ", "#text": "(e)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Definitions"}, "paragraph": [{"#tail": "\n              ", "text": {"{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", the ", "@entity-type": "federal-body", "#text": "Department of the Interior", "@entity-id": "1400"}, {"#tail": ", or the ", "@entity-type": "federal-body", "#text": "Forest Service", "@entity-id": "12C2"}, {"#tail": ".", "@entity-type": "federal-body", "#text": "National Marine\n\t\t\t Fisheries Service", "@entity-id": "1448"}], "quote": {"#tail": " means any agency of the ", "#text": "covered agency"}, "#tail": "\n\t\t\t\t\t\t\t", "#text": "The term ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "header": {"#tail": "\n                ", "#text": "Covered agency"}, "#text": "\n                ", "@id": "HB01975FAE60A4F98BC29C7269F4EC060"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "quote": {"#tail": " means any civil action containing a claim against the Federal Government, in which the claim\n\t\t\t arises under this Act and is based on the action of a covered agency.", "#text": "covered suit"}, "#text": "The term ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Covered suit"}, "#text": "\n                ", "@id": "H248531D396A84BE5BB02432B5A9AAAF2"}], "#text": "\n              ", "@id": "H299AFF86C6E944BB8775ABBCCE43BC7D"}], "#text": "\n            ", "@id": "H772123622BCF42C1937BA481B993A204"}, "#text": "\n\t\t\t\t\t", "@id": "H09C6B54E3BA142DEAA10D966CA7946B4"}, "#text": "\n        ", "@id": "HDEBE159707D24B0991CD95B2281262EA"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "#text": "The table of contents in the first section of such Act is amended by striking the item relating to\n\t\t\t such section and inserting the following:"}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Clerical amendment"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n\t\t\t", "toc": {"#tail": "\n          ", "toc-entry": {"#tail": "\n          ", "#text": "Sec.\u200213.\u2002Disclosure of expenditures.", "@level": "section"}, "#text": "\n\t\t\t\t\t\t", "@regeneration": "no-regeneration"}, "#text": "\n\t\t\t\t\t", "@id": "H01F016D9896E48F19B5CF21EB53879C7"}, "#text": "\n        ", "@id": "H755C7A8B88F049F19ABF81656E5800EB"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", as in\n\t\t\t effect before the enactment of this Act.", "@entity-type": "act", "#text": "section 13 of such Act", "@value": "Endangered Species Act of 1973/s:13", "@proposed": "true"}, "#text": "This section shall not be construed to affect the amendments made by "}, "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "Prior amendments not affected"}, "#text": "\n        ", "@id": "HF30A89724B14403DBAF9A05104B3F143"}], "#text": "\n      ", "@id": "H24C663C18A07429487C0A16FD970F2DD"}], "#text": "\n\t\t", "@id": "HDF916F79F03647F587207CE3A01707DE"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-03-27"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 4316 IH: Endangered Species Recovery Transparency Act"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}