{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "@display": "yes", "#text": "IN THE HOUSE OF\n\t\t  REPRESENTATIVES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "@display": "yes", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "H. R. 807"}, "official-title": {"#tail": "\n\t", "#text": "To require that the Government prioritize\n\t\t  all obligations on the debt held by the public in the event that the debt limit\n\t\t  is reached.", "@display": "yes", "pagebreak": {"#tail": "\n    "}}, "session": {"#tail": "\n\t\t", "@display": "yes", "#text": "1st Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "IB"}, "action": [{"action-date": {"#tail": "\n\t\t\t", "@date": "20130225", "#text": "February 25, 2013"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "committee-name": {"@deleted-display-style": "strikethrough", "#tail": "\n      ", "@committee-id": "HWM00", "@added-display-style": "italic", "#text": "Committee on Ways and\n\t\t\t Means"}, "sponsor": {"#tail": " (for\n\t\t\t himself, ", "#text": "Mr. McClintock", "@name-id": "M001177"}, "cosponsor": [{"#tail": ",\n\t\t\t ", "#text": "Mr. Scalise", "@name-id": "S001176"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Jordan", "@name-id": "J000289"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Price of Georgia", "@name-id": "P000591"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Hensarling", "@name-id": "H001036"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Garrett", "@name-id": "G000548"}, {"#tail": ", ", "#text": "Mr. Cole", "@name-id": "C001053"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Young of Indiana", "@name-id": "Y000064"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Franks of Arizona", "@name-id": "F000448"}, {"#tail": ", ", "#text": "Ms. Foxx", "@name-id": "F000450"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Collins of Georgia", "@name-id": "C001093"}, {"#tail": ",\n\t\t\t ", "#text": "Mr. Mulvaney", "@name-id":