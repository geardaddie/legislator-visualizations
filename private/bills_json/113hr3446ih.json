y-ref": {"#tail": ".", "@entity-type": "act", "#text": "paragraph (2)(B)", "@value": "Higher Education Act of 1965/s:483/ss:i/p:2/sp:B", "@proposed": "true"}, "#text": "whose independent status is not subject to verification by a financial aid administrator under "}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@id": "H4FC1B023CE34436A833F274F56843B9A", "#text": "\n                "}]}], "#text": "\n            ", "@id": "H43B2CF00487B4A68BE424B79B7F92D9F"}, "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "H740CFE319D3444DBBDC73BF598783EB7"}, "#text": "\n        ", "@id": "HC707181A5A9240D6A80D5E84A2D861A7"}], "#text": "\n      ", "@id": "H5C6F359569BA4FEF831314E325122C95"}], "#text": "\n    ", "@id": "H17EF6D6C821D4B969A2D8A0A5553B513"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-11-12"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 3446 IH: FAFSA Fairness Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}