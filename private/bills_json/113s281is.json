t general", "@level": "subparagraph"}, "#text": "(A)"}], "#text": "by\n\t\t\t striking"}, "enum": {"#tail": "\n            ", "#text": "(i)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n            ", "#text": ";"}, "#tail": "\n          ", "paragraph": {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "A\n\t\t\t\tperson"}, "enum": {"#tail": "\n                ", "#text": "(5)"}, "header": {"#tail": "\n                ", "#text": "Custom farming\n\t\t\t\tservices"}, "#text": "\n                ", "@id": "ida909d81616d84c86a48f93dca1c7bf8f"}, "@display-inline": "no-display-inline", "#text": "\n              ", "@id": "id2285EA500B4640B9BED159705D967DDC"}, "#text": "\n            ", "@id": "idf5b55d448e134e14a54dbc16a66f27f8"}, {"#tail": "\n          ", "text": {"#tail": "\n          ", "quote": [{"#tail": "after", "#text": "under usual and customary terms"}, {"#tail": "; and", "#text": "services"}], "#text": "by\n\t\t\t inserting"}, "enum": {"#tail": "\n            ", "#text": "(ii)"}, "@id": "id1101d1c7a3bb477db70131b3a5eefab4", "#text": "\n            "}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "; and", "@entity-type": "act", "#text": "subparagraph (B)", "@value": "Food Security Act of 1985/s:1001A/ss:c/p:5/sp:B"}, "#text": "by striking\n\t\t\t "}, "enum": {"#tail": "\n            ", "#text": "(iii)"}, "@id": "id48c6497101d943a9b7741565bb329c9d", "#text": "\n            "}], "#text": "\n          ", "@id": "idca54de791049408eb2bcdba3f40b0c98"}, {"#tail": "\n      ", "text": {"#tail": "\n          ", "#text": "by adding at the\n\t\t\t end the following:"}, "enum": {"#tail": "\n          ", "#text": "(D)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "#tail": "\n        ", "paragraph": {"#tail": "\n            ", "@id": "idffa585940c2644d5b401dee22c0469d1", "enum": {"#tail": "\n        