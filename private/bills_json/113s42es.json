, "enum": {"#tail": "\n              ", "#text": "(1)"}, "header": {"#tail": "\n              ", "#text": "In\n\t\t\t\tgeneral", "@display-inline": "yes-display-inline"}, "@display-inline": "no-display-inline", "#text": "\n              ", "@id": "id65b5d6d1-5368-40a8-ba9d-1de64e3e3910", "@commented": "no"}, {"#tail": "\n          ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall\n\t\t\t\tinclude\u2014", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Antitrust Criminal Penalty Enhancement and Reform Act of 2004/s:216/ss:c/p:1", "@proposed": "true"}, "#text": "Relief for any action under ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(2)"}, "header": {"#tail": "\n              ", "#text": "Compensatory\n\t\t\t\tdamages", "@display-inline": "yes-display-inline"}, "@display-inline": "no-display-inline", "#text": "\n              ", "@id": "id13172e00-aa73-433f-889a-15f8bd3c6ff6", "@commented": "no", "subparagraph": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "reinstatement\n\t\t\t\twith the same seniority status that the covered individual would have had, but\n\t\t\t\tfor the discrimination;", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(A)"}, "@display-inline": "no-display-inline", "#text": "\n                ", "@id": "idbea39029-c92f-46ff-9f75-7b6bace4a94c", "@commented": "no"}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "the amount of\n\t\t\t\tback pay, with interest; and", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(B)"}, "@display-inline": "no-display-inline", "#text": "\n                ", "@id": "id6b699649-52af-4bb9-a14c-d3d7883cb97b", "@commented": "no"}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "#text": "compensation for\n\t\t\t\tany special damages sustained as a result of the discrimination including\n\t\t\t\tlitigation costs, expert witness fees, and reasonable attorney\u2019s fees.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n                ", "#text": "(C)"}, "@display-inline": "no-display-inline", "#text": "\n                ", "@id": "idc32c7ed2-8ef0-4deb-aad4-ec8a25c6bdb3", "@commented": "no"}]}], "@display-inline": "no-display-inline", "#text": "\n            ", "@id": "id7a8ebfaa-7424-40a0-8628-b27444232de8", "@commented": "no"}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "#text": "Nothing in this section shall be deemed to\n\t\t\t\tdiminish the rights, privileges, or remedies of any covered individual under\n\t\t\t\tany Federal or State law, or under any collective bargaining\n\t\t\t\tagreement.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(d)"}, "header": {"#tail": "\n            ", "#text": "Rights retained\n\t\t\t\tby whistleblowers", "@display-inline": "yes-display-inline"}, "@display-inline": "no-display-inline", "#text": "\n            ", "@id": "id73049c49-3688-4f6b-9f5d-1983a68b3d54", "@commented": "no"}], "@display-inline": "no-display-inline", "#text": "\n          ", "@id": "idf793b30b-0a2c-4738-b48b-ca8a99ff4289", "@commented": "no"}, "@display-inline": "no-display-inline", "#text": "\n        ", "@id": "id6eb9254d-5c0d-41b8-92bb-87f5ef2c14fc"}, "@display-inline": "no-display-inline", "#text": "\n      ", "@id": "id5c600dbc-cf42-42ac-8417-7ff59ba456bc", "@commented": "no"}], "#text": "\n    ", "@display-enacting-clause": "yes-display-enacting-clause"}, "@public-print": "no", "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "