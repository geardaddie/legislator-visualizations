{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n    ", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "S. 467"}, "official-title": {"#tail": "\n  ", "#text": "To allow consumers to unlock mobile wireless devices for\n\t\t  interoperability purposes."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20130305", "#text": "March 5, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "sponsor": {"#tail": "introduced the\n\t\t\t following bill; which was read twice and referred to the", "#text": "Mr. Wyden", "@name-id": "S247"}, "committee-name": {"#tail": "\n      ", "@committee-id": "SSJU00", "#text": "Committee on the\n\t\t\t Judiciary"}}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@bill-type": "olc", "@dms-id": "A1", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "Wireless Device Independence Act\n\t\t\t of 2013"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as\n\t\t\t the", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short\n\t\t\t title"}, "#text": "\n      ", "@id": "S1"}, {"#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Allowing\n\t\t\t consumers to unlock cell phones"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended by strik