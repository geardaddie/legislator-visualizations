": "\n      ", "@id": "H5CD5F619EA2D459EA497492BCE7B0C33"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "10."}, "header": {"#tail": "\n\t\t\t", "#text": "Strategic plan to detect and interdict biological and chemical weapons"}, "subsection": [{"#tail": "\n      ", "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n\t\t\t\t", "#text": "In general"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n\t\t\t shall develop a strategic plan (in this section referred to as the ", "@entity-type": "federal-body", "#text": "Secretary of Homeland Security", "@entity-id": "7000"}, "quote": {"#tail": ") to detect and interdict biological and chemical weapons entering the United States.", "#text": "Plan"}, "#text": "Not later than one year after the date of enactment of this Act, the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "Development"}, "#text": "\n          ", "@id": "H2B168F7B34FE490C806E09214B6A6600"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall complete implementation of the Plan not later than two\n\t\t\t years after the development of the Plan under ", "@entity-type": "federal-body", "#text": "Secretary of Homeland Security", "@entity-id": "7000"}, {"#tail": ".", "@entity-type": "act", "#text": "paragraph (1)", "@value": "National Border and Homeland Security Act/s:10/ss:a/p:1", "@proposed": "true"}], "#text": "The "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Implementation"}, "#text": "\n          ", "@id": "HEF4D20BB8B8A4AB99C820CA3B3983D58"}], "#text": "\n        ", "@id": "H170B3E11EB4E45E498791F2E34B62DEB"}, {"#tail": "\n    ", "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n\t\t\t\t", "#text": "Reports to Congress"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall submit to ", "@entity-type": "federal-body", "#text": "Secretary of Homeland\n\t\t\t Security", "@entity-id": "7000"}, {"#tail": " a report that describes the Plan.", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}], "#text": "Not later than one year after the date of the enactment of this Act, the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "Initial report"}, "#text": "\n          ", "@id": "H39E39F2C7B66488985B7EDB160C4D04D"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " and\n\t\t\t annually thereafter, the ", "@entity-type": "act", "#text": "paragraph (1)", "@value": "National Border and Homeland Security Act/s:10/ss:a/p:1", "@proposed": "true"}, {"#tail": " shall submit\n\t\t\t reports to ", "@entity-type": "federal-body", "#text": "Secretary of Homeland Security", "@entity-id": "7000"}, {"#tail": " on the implementation of the Plan.", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}], "#text": "Not later than two years after the submission of the initial report under "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Annual report"}, "#text": "\n          ", "@id": "H1EC6483EF6EC40BDB6905093C320B784"}], "#text": "\n        ", "@id": "H62469970CB364E47979AC26F97CD934E"}], "#text": "\n      ", "@id": "H5B9853401B5E47FE901DB2FCB8C39B68"}], "#text": "\n\t\t", "@id": "HC04029D1782C4042AFA23C852E462C20"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-06-25"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/