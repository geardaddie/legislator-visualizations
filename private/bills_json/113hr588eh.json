{"bill": {"#text": " \n", "form": {"current-chamber": {"#tail": " \n", "@display": "no", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n", "congress": {"#tail": " ", "@display": "yes", "#text": "113th CONGRESS"}, "#tail": " \n", "legis-num": {"#tail": " \n", "#text": "H. R. 588"}, "official-title": {"#tail": " \n", "@display": "yes", "#text": "To provide for donor contribution acknowledgments to be displayed at the Vietnam Veterans Memorial Visitor Center, and for other purposes."}, "session": {"#tail": " \n", "@display": "yes", "#text": "1st Session"}, "distribution-code": {"#tail": " \n", "@display": "no", "#text": "IB"}, "legis-type": {"#tail": " \n", "#text": "AN ACT"}}, "attestation": {"#tail": "\n", "attestation-group": {"#tail": "\n  ", "role": {"#tail": "\n    ", "#text": "Clerk."}, "#text": "\n      ", "attestor": {"#tail": "\n      ", "@display": "no", "#text": "Karen L. Haas,"}, "attestation-date": {"#tail": "\n      ", "@date": "20130506", "#text": "Passed the House of Representatives May 6, 2013.", "@chamber": "House"}}, "#text": "\n    "}, "@stage-count": "1", "@bill-stage": "Engrossed-in-House", "@bill-type": "olc", "endorsement": {"#tail": "\n", "@display": "yes"}, "@dms-id": "H269318032EE5465EA109E795052CB1D2", "@public-private": "public", "legis-body": {"#tail": " ", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n", "text": {"#tail": " ", "quote": {"short-title": {"#tail": "\n          \n        ", "#text": "Vietnam Veterans Donor Acknowledgment Act of 2013"}, "#tail": ".", "#text": "\n          \n            "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H12ECBC07D07148B9BD9239495C8521CF"}, {"#tail": "\n", "text": {"#tail": " \n", "#text": "\n        \n          ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended\u2014", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " (", "@entity-type": "public-law", "#text": "Section 6(b) of ", "external-xref": {"#tail": "\n          ", "@parsable-cite": "pl/96/297", "#text": "Public Law 96\u2013297", "@legal-doc": "public-law"}, "@value": "public-law/96/297/s:6/ss:b"}, {"#tail": ")", "@entity-type": "uscode", "#text": "\n            ", "external-xref": {"#tail": "\n         note", "@parsable-cite": "usc/16/431", "#text": "\n            16 U.S.C. 431\n          ", "@legal-doc": "usc"}, "@value": "usc/16/431/note"}], "@entity-type": "law-citation", "#text": "\n          "}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Donor contribution acknowledgments at the Vietnam Veterans Memorial Visitor Center"}, "paragraph": [{"#tail": "\n", "text": {"#tail": " ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n           by striking the ", "@entity-type": "public-law", "#text": "paragraph (4)", "@value": "public-law/96/297/s:6/ss:b/p:4"}, "#text": "in \n            ", "quote": {"#tail": " after the semicolon;", "#text": "and"}}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@id": "HA90BC0BD02A84FC9B3E89E6ACA98C50A", "#text": "\n        "}, {"#tail": "\n", "text": {"#tail": " ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " by striking the period and inserting ", "@entity-type": "public-law", "#text": "paragraph (5)", "@value": "public-law/96/297/s:6/ss:b/p:5"}, "#text": "in ", "quote": {"#tail": "; and", "#text": "; and"}}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "@id": "HAFAC1A496D9E4F88BEDCDFE6B30A4D0D", "#text": "\n        "}, {"#tail": "\n    ", "text": {"#tail": " \n", "#text": "by inserting at the end the following new paragraph:"}, "enum": {"#tail": "\n        ", "#text": "(3)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": " ", "paragraph": {"#tail": "\n          ", "text": {"#tail": " \n", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\u2014", "@entity-type": "uscode", "#text": "\n                ", "external-xref": {"#tail": " of title 40, United States Code", "@parsable-cite": "usc/40/8905", "#text": "section 8905(b)(7)", "@legal-doc": "usc"}, "@value": "usc/40/8905/b/7"}, "#text": "notwithstanding "}, "enum": {"#tail": "\n            ", "#text": "(6)"}, "#text": "\n            ", "@id": "H9DABBA03A8EE4A238326E29747F747EB", "subparagraph": [{"#tail": "\n", "text": {"#tail": " ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall allow the Vietnam Veterans Memorial Fund, Inc. to acknowledge donor contributions to the visitor center by displaying, inside the visitor center, an appropriate statement or credit acknowledging the contribution;", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "#text": "the "}, "enum": {"#tail": "\n              ", "#text": "(A)"}, "@id": "H6D64831E844242EBAD726A09CA10A6D6", "#text": "\n              "}, {"#tail": "\n", "text": {"#tail": " ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " and for a period of time commensurate with the level of the contribution and the life of the facility;", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "#text": "donor contribution acknowledgments shall be displayed in a form approved by the "}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "@id": "H505AF69325F7409FBDA9F316D90BF485", "#text": "\n              "}, {"#tail": "\n", "text": {"#tail": " ", "#text": "the Vietnam Veterans Memorial Fund shall bear all expenses related to the display of donor acknowledgments;"}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "@id": "H54B53531023C4859A93A0219506D3DA5", "#text": "\n              "}, {"#tail": "\n", "text": {"#tail": " ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " for approval, its plan for displaying donor acknowledgments;", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, "#text": "prior to the display of donor acknowledgments, the Vietnam Veterans Memorial Fund, Inc. shall submit to the "}, "enum": {"#tail": "\n              ", "#text": "(D)"}, "@id": "H17E62FD97FE64954A91780344C9E4544", "#text": "\n              "}, {"#tail": "\n", "text": {"#tail": " ", "#text": "such plan shall include the sample text and types of the acknowledgments or credits to be displayed and the form and location of all displays;"}, "enum": {"#tail": "\n              ", "#text": "(E)"}, "@id": "HBC817B8BAC134EC593B9005099306AC1", "#text": "\n              "}, {"#tail": "\n", "text": {"#tail": " \n", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall approve the plan, if the ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, {"#tail": " determines that the plan\u2014", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}], "#text": "the "}, "enum": {"#tail": "\n              ", "#text": "(F)"}, "clause": [{"#tail": "\n", "text": {"#tail": " ", "#text": "allows only short, discrete, and unobtrusive acknowledgments or credits;"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "HF6CBC8C306254EE2B00911EF33B11054", "#text": "\n                "}, {"#tail": "\n", "text": {"#tail": " ", "#text": "does not permit any advertising slogans or company logos; and"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "@id": "H5E84563DFD0243C6858692622E877FC4", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": " ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " guidelines for indoor donor recognition; and", "@entity-type": "federal-body", "#text": "National Park Service", "@entity-id": "1443"}, "#text": "conforms to applicable "}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "@id": "H1138A05EBC49492F868B0A44AD189094", "#text": "\n                "}], "#text": "\n              ", "@id": "HE3E78ACDFD1E43DBA6D9303DF2F86492"}, {"#tail": "\n          ", "text": {"#tail": " \n", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " determines that the proposed plan submitted under this paragraph, does not meet the requirements of this paragraph, the ", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, {"#tail": " shall\u2014", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}], "#text": "if the "}, "enum": {"#tail": "\n              ", "#text": "(G)"}, "clause": [{"#tail": "\n", "text": {"#tail": " ", "#text": "advise the Vietnam Veterans Memorial Fund, Inc. not later than 30 days after receipt of the proposed plan of the reasons that such plan does not meet the requirements; and"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "H1F777F7CE0E64882BBAA9C5A759D09CB", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": " ", "#text": "allow the Vietnam Veterans Memorial Fund, Inc. to submit a revised donor recognition plan."}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "@id": "H358B42F115FA4B26A65096CB41D225DA", "#text": "\n                "}], "#text": "\n              ", "@id": "HB31541B2C41040CE914C5A30DB4C7198"}]}, "#text": " \n", "@id": "H6EB67AB5622344248D6DDB1255D13741"}, "#text": "\n        ", "@id": "H94607F0125E8498A824A26CABC5FA55A"}], "#text": "\n      ", "@id": "HF82642D81A344BD1BAC75152CADC9383"}], "#text": " \n", "@id": "H61E20147996F4950A105C734F0D1A9CE"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 588 EH: Vietnam Veterans Donor Acknowledgment Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}