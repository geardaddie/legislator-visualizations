{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "@display": "yes", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n", "congress": {"#tail": "\n    ", "@display": "yes", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "H. R. 1900"}, "official-title": {"#tail": "\n  ", "#text": "To provide for the timely consideration of all licenses, permits, and approvals required under Federal law with respect to the siting, construction, expansion, or operation of any natural gas pipeline projects.", "@display": "yes", "pagebreak": {"#tail": "\n    "}}, "session": {"#tail": "\n    ", "@display": "yes", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "IB"}, "action": [{"action-date": {"#tail": "\n      ", "@date": "20130509", "#text": "May 9, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "committee-name": {"@deleted-display-style": "strikethrough", "#tail": "\n      ", "@committee-id": "HIF00", "@added-display-style": "italic", "#text": "Committee on Energy and Commerce"}, "sponsor": {"#tail": " (for himself, ", "#text": "Mr. Pompeo", "@name-id": "P000602"}, "cosponsor": [{"#tail": ", ", "#text": "Mr. Matheson", "@name-id": "M001142"}, {"#tail": ", ", "#text": "Mr. Olson", "@name-id": "O000168"}, {"#tail": ", and ", "#text": "Mr. Gardner", "@name-id": "G000562"}, {"#tail": ") introduced the following bill; which was referred to the ", "#text": "Mr. Johnson of Ohio", "@name-id": "J000292"}]}, "#text": "\n      "}, {"action-date": {"#tail": "\n      ", "@date": "20131118", "#text": "November 18, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "Additional sponsors: ", "cosponsor": [{"#tail": ", ", "#text": "Mr. McKinley", "@name-id": "M001180"}, {"#tail": ", ", "#text": "Mr. Radel", "@name-id": "R000596"}, {"#tail": ", ", "#text": "Mr. Latta", "@name-id": "L000566"}, {"#tail": ", ", "#text": "Mr. Marino",