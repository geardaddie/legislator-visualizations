m": {"#tail": "\n            ", "#text": "(C)"}, "header": {"#tail": "\n            ", "#text": "Foreign financial institution"}, "#text": "\n            ", "@id": "H2AE3486E290B4C22AA8DAFCF8C832BD8", "@commented": "no"}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", ", "@entity-type": "uscode", "#text": "paragraph (1)", "@value": "usc/18/1956/a/1"}, {"#tail": ", or ", "@entity-type": "uscode", "#text": "(2)", "@value": "usc/18/1956/a/2"}, {"#tail": ", with respect to which penalties may be imposed\n\t\t\t pursuant to such section.", "@entity-type": "uscode", "#text": "(3) of section 1956(a) of title\n\t\t\t 18, United States Code", "@value": "usc/18/1956/a/3"}], "term": {"#tail": " means any of the activities described in ", "#text": "money laundering"}, "#text": "The term "}, "enum": {"#tail": "\n            ", "#text": "(D)"}, "header": {"#tail": "\n            ", "#text": "Money laundering"}, "#text": "\n            ", "@id": "HCC582AE98E284BF0AA27B98420D585C7"}]}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " may further define the terms used in this section in the regulations\n\t\t\t prescribed under this section.", "@entity-type": "federal-body", "#text": "Secretary of the Treasury", "@entity-id": "2000"}, "#text": "The "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Other definitions"}, "#text": "\n          ", "@id": "H8DF03F171476466B9F61F7A0A8B7380B"}], "#text": "\n        ", "@id": "HB0170EB3D3DD4D38BCA8B2AA392206C5"}], "#text": "\n      ", "@id": "H7B74C4061EE740E2B40235E62585E12A"}, {"#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n\t\t\t", "#text": "Imposition of sanctions with respect to United States persons that engage in transactions with\n\t\t\t certain foreign persons"}, "subsection": [{"#tail": "\n      ", "tex