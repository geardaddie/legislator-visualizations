"legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "Wild Game Donation Act of 2013"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H752D38EB47AD4E619DDF09C2754E2652"}, {"@section-type": "subsequent-section", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Charitable deduction for costs associated with donations of wild game meat"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " is amended by adding at the end the following new paragraph:", "@entity-type": "act", "#text": "Subsection (e) of ", "external-xref": {"#tail": " of the Internal Revenue Code of 1986", "@parsable-cite": "usc/26/170", "#text": "section 170", "@legal-doc": "usc"}, "@value": "Internal Revenue Code of 1986/s:170/ss:e"}, "#text": "\n          ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "quoted-block": {"@act-name": "", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "@style": "OLC", "#tail": "\n      ", "paragraph": {"#tail": "\n          ", "subparagraph": [{"#tail": "\n            ", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ") shall be increased by the amount of the qualified processing fees paid with respect to such contribution.", "@entity-type": "act", "#text": "paragraph (1)(A)", "@value": "Internal Revenue Code of 1986/s:170/ss:e/p:1/sp:A"}, "#text": "In the case of a charitable contribution by an individual of qualified wild game meat, the amount of such contribution otherwise taken into account under this section (after the application of "}, "enum": {"#tail": "\n              ", "#text": "(A)"}, "header": {"#tail": "\n              ", "#text": "In general"}, "#text": "\n              ", "@id": "H8D8811773B2C43719A0A4C12C4FEB396"}, {"#tail": "\n            ", "clause": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "such animal is killed in the wild by the individual making the charitable contribution of such meat (not including animals raised on a farm for the purpose of sport hunting),"}, "enum": {"#tail": "\n                ", "#text": "(i)"}, "@id": "H276CE4E4690A4290B7A3946ECE98D74C", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "such animal is hunted or taken in accordance with all State and local laws and regulations, including season and size restrictions,"}, "enum": {"#tail": "\n                ", "#text": "(ii)"}, "@id": "HB9A3307A75D9415799DA31EF2A36734E", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "such meat is processed for human consumption by a processor which is licensed for such purpose under the appropriate Federal, State, and local laws and regulations and which is in compliance with all such laws and regulations, and"}, "enum": {"#tail": "\n                ", "#text": "(iii)"}, "@id": "H044812E1B9394AE2910772B36C6FD987", "#text": "\n                "}, {"#tail": "\n            ", "text": {"#tail": "\n              ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ").", "@entity-type": "act", "#text": "section 22(b)(2) of the Bill Emerson Good Samaritan Food Donation Act", "@value": "Bill Emerson Good Samaritan Food Donation Act/s:22/ss:b/p:2"}, "#text": "such meat is apparently wholesome (under regulations similar to the regulations under "}, "enum": {"#tail": "\n                ", "#text": "(iv)"}, "@id": "H2723EF8A8FA2451EB16A9E211584A92A", "#text": "\n                "}], "enum": {"#tail": "\n              ", "#text": "(B)"}, "header": {"#tail": "\n              ", "#text": "Qualified wild game meat"}, "text": {"#tail": "\n              ", "term": {"#tail": " means the meat of any animal which is typically used for human consumption, but only if\u2014", "#text": "qualified wild game meat"}, "#text": "For purposes of this paragraph, the term "}, "#text": "\n              ", "@id": "H735483A2FD9841419B8EBF863CAB5713"}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " for the purpose of processing wild game meat, but only to the extent that such meat is donated as a charitable contribution under this section.", "@entity-type": "act", "#text": "subparagraph (B)(iii)", "@value": "Internal Revenue Code of 1986/s:170/ss:e/p:8/sp:B/cl:iii", "@proposed": "true"}, "term": {"#tail": " means any fee or charge paid to a processor which fulfills the requirements of ", "#text": "qualified processing fee"}, "#text": "For purposes of this paragraph, the term "}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "header": {"#tail": "\n              ", "#text": "Qualified processing fee"}, "#text": "\n              ", "@id": "HD5DCCFBE94494769AAE4135F4D67D8B4"}], "enum": {"#tail": "\n            ", "#text": "(8)"}, "header": {"#tail": "\n            ", "#text": "Special rule for contributions of wild game meat"}, "#text": "\n            ", "@id": "H6712D0C5152B430889C31A55523002A7"}, "#text": "\n          ", "@id": "H6DBC458374484918AE996065D0E61236"}, "#text": "\n        ", "@id": "H6419AB3A202E4854BD7B2D84AFC2C788"}, {"#tail": "\n      ", "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Exclusion of processor's income from tax exempt organizations"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " is amended by inserting after ", "@entity-type": "act", "#text": "Part III of subchapter B of ", "external-xref": {"#tail": " of the Internal Revenue Code of 1986", "@parsable-cite": "usc-chapter/26/1", "#text": "chapter 1", "@legal-doc": "usc-chapter"}, "@value": "Internal Revenue Code of 1986/ch:1/sch:B/pt:III"}, {"#tail": " the following new section:", "@entity-type": "act", "#text": "section 139D", "@value": "Internal Revenue Code of 1986/s:139D"}], "#text": "\n            "}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "quoted-block": {"@act-name": "", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "@style": "OLC", "#tail": "\n        ", "section": {"#tail": "\n            ", "enum": {"#tail": "\n              ", "#text": "139E."}, "header": {"#tail": "\n              ", "#text": "Certain income received from charitable organizations"}, "subsection": [{"#tail": "\n              ", "text": {"#tail": "\n              ", "#text": "Gross income of a qualified meat processor shall not include any amount paid to such processor as a qualified processing fee by a charitable organization for the processing of donated wild game meat."}, "enum": {"#tail": "\n                ", "#text": "(a)"}, "header": {"#tail": "\n                ", "#text": "In general"}, "#text": "\n                ", "@id": "H7D20FC63FB984892BFFE4153AC1373B9"}, {"#tail": "\n            ", "text": {"#tail": "\n                ", "#text": "For purposes of this section\u2014"}, "enum": {"#tail": "\n                ", "#text": "(b)"}, "header": {"#tail": "\n                ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "section 170(e)(8)(B)(iii)", "@value": "Internal Revenue Code of 1986/s:170/ss:e/p:8/sp:B/cl:iii", "@proposed": "true"}, "term": {"#tail": " means a processor which fulfills the requirements of ", "#text": "qualified meat processor"}, "#text": "The term "}, "enum": {"#tail": "\n                  ", "#text": "(1)"}, "header": {"#tail": "\n                  ", "#text": "Qualified meat processor"}, "#text": "\n                  ", "@id": "H59405FCABE8E445DBFD4FE4647F29521"}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " and the charitable purpose of which is to provide free food to individuals in need of food assistance.", "@entity-type": "act", "#text": "section 170(c)", "@value": "Internal Revenue Code of 1986/s:170/ss:c"}, "term": {"#tail": " means an entity to which a charitable contribution may be made under ", "#text": "charitable organization"}, "#text": "The term "}, "enum": {"#tail": "\n                  ", "#text": "(2)"}, "header": {"#tail": "\n                  ", "#text": "Charitable organization"}, "#text": "\n                  ", "@id": "H53148043D53045FBB2672F22530BC737"}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", without regard to ", "@entity-type": "act", "#text": "section 170(e)(8)(B)", "@value": "Internal Revenue Code of 1986/s:170/ss:e/p:8/sp:B", "@proposed": "true"}, {"#tail": " thereof) which is received as a charitable contribution (as defined in ", "@entity-type": "act", "#text": "clause (iii)", "@value": "Internal Revenue Code of 1986/s:170/ss:e/p:8/sp:B/cl:iii", "@proposed": "true"}, {"#tail": ") by a charitable organization.", "@entity-type": "act", "#text": "section 170(c)", "@value": "Internal Revenue Code of 1986/s:170/ss:c"}], "term": {"#tail": " means qualified wild game meat (as defined in ", "#text": "donated wild game meat"}, "#text": "The term "}, "enum": {"#tail": "\n                  ", "#text": "(3)"}, "header": {"#tail": "\n                  ", "#text": "Donated wild game meat"}, "#text": "\n                  ", "@id": "HB1F18084B1864A8C84FC6B524BA2FB53"}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "term": {"#tail": " means any fee or charge paid to a qualified meat processor for the purpose of processing donated wild game meat.", "#text": "qualified processing fee"}, "#text": "The term "}, "enum": {"#tail": "\n                  ", "#text": "(4)"}, "header": {"#tail": "\n                  ", "#text": "Qualified processing fee"}, "#text": "\n                  ", "@id": "HBDC18A901B884A3BA5C073D5F89B2DB3"}], "#text": "\n                ", "@id": "HAACCDABB2E0641B5868049F8E834CADE"}], "#text": "\n              ", "@id": "H4C0CDC1828504D3EBE7C18A3507A8FFF"}, "#text": "\n            ", "@id": "H0EE11859D9CF48E59280C368293FD6F4"}, "#text": "\n          ", "@id": "H529E92E2C13D4AAC95DE9E7683B85120"}, {"#tail": "\n      ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " is amended by inserting after the item relating to ", "@entity-type": "act", "#text": "part III of subchapter B of ", "external-xref": {"#tail": " of the Internal Revenue Code of 1986", "@parsable-cite": "usc-chapter/26/1", "#text": "chapter 1", "@legal-doc": "usc-chapter"}, "@value": "Internal Revenue Code of 1986/ch:1/sch:B/pt:III"}, {"#tail": " the following new item:", "@entity-type": "act", "#text": "section 139C", "@value": "Internal Revenue Code of 1986/s:139C"}], "#text": "The table of sections for "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Clerical amendment"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "#tail": "\n        ", "@display-inline": "no-display-inline", "toc": {"#tail": "\n            ", "#text": "\n              ", "toc-entry": {"#tail": "\n            ", "@level": "section", "#text": "Sec. 139E. Certain income received from tax exempt organizations.", "@bol