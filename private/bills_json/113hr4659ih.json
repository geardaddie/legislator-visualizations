[{"#tail": " and inserting ", "#text": "subsections (a) and (b) of section 203"}, {"#tail": ".", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "act", "#text": "subsection (a) of section 203", "@value": "Immigration and Nationality Act/s:203/ss:a"}, "#text": "\n          "}], "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended by striking ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 202(a)(2) of the Immigration and Nationality Act", "@value": "Immigration and Nationality Act/s:202/ss:a/p:2"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "uscode", "#text": "8 U.S.C. 1152", "@value": "usc/8/1152"}, "@parsable-cite": "usc/8/1152", "#text": "\n          ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Removal of per country numerical caps on employment visas"}, "#text": "\n      ", "@id": "H73C7C5EAD093422F8AF18BFAE6715421"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended by adding\n\t\t\t at the end the following:", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 203(b)(5) of the Immigration and Nationality Act", "@value": "Immigration and Nationality Act/s:203/ss:b/p:5"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "uscode", "#text": "8 U.S.C. 1153(b)(5)", "@value": "usc/8/1153/b/5"}, "@parsable-cite": "usc/8/1153", "#text": "\n          ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "Regional center program made permanent"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n      ", "#text": "."}, "#tail": "\n\t\t", "@id": "HCA3E801FDBE04E32B84DE95CB5A00C6B", "@display-inline": "no-display-inline", "#text": "\n\t\t\t\t", "subparagraph": {"#tail": "\n        ", "clause": [{"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "#text": "Not less than 3,000 of the visas made available under this paragraph in each fiscal year shall be\n\t\t\t reserved for qualified immigrants for a program to implement the\n\t\t\t provisions of this paragraph.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(i)"}, "header": {"#tail": "\n            ", "#text": "Set aside"}, "#text": "\n            ", "@id": "H5C74050B5E7445CEA4B3C4DB52D0C81A"}, {"#tail": "\n          ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall involve a regional center in the United States,\n\t\t\t designated by the ", "@entity-type": "act", "#text": "clause (i)", "@value": "Immigration and Nationality Act/s:203/ss:b/p:5/sp:E/cl:i", "@proposed": "true"}, {"#tail": " on the basis of a general\n\t\t\t proposal, for the promotion of economic growth, including increased export\n\t\t\t sales, improved regional productivity, job creation, or increased domestic\n\t\t\t capital investment. A regional center shall have jurisdiction over a\n\t\t\t limited geographic area, which shall be described in the proposal and\n\t\t\t consistent with the purpose of concentrating pooled investment in defined\n\t\t\t economic zones. The establishment of a regional center may be based on\n\t\t\t general predictions, contained in the proposal, concerning the kinds of\n\t\t\t commercial enterprises that will receive capital from aliens, the jobs\n\t\t\t that will be created directly or indirectly as a result of such capital\n\t\t\t investments, and the other positive economic effects such capital\n\t\t\t investments will have.", "@entity-type": "federal-body", "#text": "Secretary of Homeland Security", "@entity-id": "7000"}], "#text": "The program referred to in ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(ii)"}, "header": {"#tail": "\n            ", "#text": "In general"}, "#text": "\n            ", "@id": "H9BF1E6C7BC924AB68C284F981EB86093"}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", and notwithstanding the\n\t\t\t requirements of 8 CFR 204.6, the ", "@entity-type": "act", "#text": "subparagraph (A)(ii)", "@value": "Immigration and Nationality Act/s:203/ss:b/p:5/sp:A/cl:ii"}, {"#tail": " shall\n\t\t\t permit aliens admitted under the program described in this subparagraph to\n\t\t\t establish reasonable methodologies for determining the number of jobs\n\t\t\t created by the program, including such jobs which are estimated to have\n\t\t\t been created indirectly through revenues generated from increased exports,\n\t\t\t improved regional productivity, job creation, or increased domestic\n\t\t\t capital investment resulting from the program.", "@entity-type": "federal-body", "#text": "Secretary of Homeland Security", "@entity-id": "7000"}], "#text": "For purposes of determining compliance with ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(iii)"}, "header": {"#tail": "\n            ", "#text": "Rule for determining compliance with job creation requirements"}, "#text": "\n            ", "@id": "H7BF389141CA74F33AFB0886E172C3C42"}], "enum": {"#tail": "\n          ", "#text": "(E)"}, "header": {"#tail": "\n\t\t\t\t\t", "#text": "Regional center program"}, "#text": "\n          ", "@id": "H77D90C1CFF2F4F2EB8C24779A08A7BA8"}}, "#text": "\n      ", "@id": "HE51B4DD4A84B447F88C998B7CEF5E8C0"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"#tail": ".", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may give\n\t\t\t priority to petitions filed by aliens seeking admission under the program\n\t\t\t described in ", "@entity-type": "federal-body", "#text": "Secretary of Homeland Security", "@entity-id": "7000"}, {"#tail": ". Notwithstanding ", "@entity-type": "act", "#text": "section 203(b)(5)(E)", "@value": "Immigration and Nationality Act/s:203/ss:b/p:5/sp:E", "@proposed": "true"}, {"#tail": ",\n\t\t\t immigrant visas made available under ", "@entity-type": "act", "#text": "section 203(e)", "@value": "Immigration and Nationality Act/s:203/ss:e"}, {"#tail": " may be issued to\n\t\t\t such aliens in an order that takes into account any priority accorded\n\t\t\t under the preceding sentence.", "@entity-type": "act", "#text": "section 203(b)(5)", "@value": "Immigration and Nationality Act/s:203/ss:b/p:5"}], "#text": "In processing petitions under this subparagraph, the "}, "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended by\n\t\t\t adding at the end the following: ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 204(a)(1)(H) of the Immigration and Nationality Act", "@value": "Immigration and Nationality Act/s:204/ss:a/p:1/sp:H"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "uscode", "#text": "8 U.S.C. 1154(a)(1)(H)", "@value": "usc/8/1154/a/1/H"}, "@parsable-cite": "usc/8/1154", "#text": "\n          ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "4."}, "header": {"#tail": "\n      ", "#text": "Priority consideration for regional center applicants"}, "#text": "\n      ", "@id": "H6C6DF21906B7456EB1EE453A58F260BC"}, {"#tail": "\n  ", "text": {"#tail": "\n\t\t", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is hereby repealed.", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "\n          ", "@entity-type": "public-law", "#text": "Section 610 of ", "external-xref": {"#tail": " (", "@parsable-cite": "pl/102/395", "#text": "\n          Public Law 102\u2013395\n        ", "@legal-doc": "public-law"}, "@value": "public-law/102/395/s:610"}, {"#tail": ")", "@entity-type": "uscode", "#text": "\n            ", "external-xref": {"#tail": " note", "@parsable-cite": "usc/8/1153", "#text": "\n          8 U.S.C. 1153\n        ", "@legal-doc": "usc"}, "@value": "usc/8/1153/note"}], "@entity-type": "law-citation", "#text": "\n          "}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "5."}, "header": {"#tail": "\n      ", "#text": "Conforming amendment"}, "#text": "\n      ", "@id": "HE583232EF05F4DF0BC2C939C3279522E"}], "#text": "\n\t\t", "@id": "H17AB1B1EBCA54C53B84D9FB283BAC213"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-05-13"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 4659 IH: EB-5 Regional Center Extension Act of 2014"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}