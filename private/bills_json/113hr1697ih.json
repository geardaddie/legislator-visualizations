"subsection": [{"#tail": "\n", "text": {"#tail": "\n", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " (relating to fuels not used for taxable purposes) is amended by inserting after ", "@entity-type": "act", "#text": "\n            ", "external-xref": {"#tail": " of the Internal Revenue Code of 1986", "@parsable-cite": "usc/26/6427", "#text": "Section 6427", "@legal-doc": "usc"}, "@value": "Internal Revenue Code of 1986/s:6427"}, {"#tail": " the following new subsection:", "@entity-type": "act", "#text": "subsection (f)", "@value": "Internal Revenue Code of 1986/s:6427/ss:f"}], "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "Refunds"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "subsection": {"#tail": "\n          ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", if any fuel on which tax was imposed by ", "@entity-type": "act", "#text": "subsection (k)", "@value": "Internal Revenue Code of 1986/s:6427/ss:k"}, {"#tail": " or ", "@entity-type": "act", "#text": "section 4041", "@value": "Internal Revenue Code of 1986/s:4041"}, {"#tail": " is used in any highway vehicle designed exclusively to provide mobile mammography services to patients within such vehicle, the Secretary shall pay (without interest) to the ultimate purchaser of such fuel an amount equal to the aggregate amount of the tax imposed on such fuel.", "@entity-type": "act", "#text": "4081", "@value": "Internal Revenue Code of 1986/s:4081"}], "#text": "Except as provided in ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(g)"}, "header": {"#tail": "\n            ", "#text": "Fuels used in mobile mammography vehicles"}, "#text": "\n            ", "@id": "HEC7DA04E15D845E3A576B08AA9AD45D3"}, "@display-inline": "no-display-inline", "#text": "\n", "@id": "H893F3