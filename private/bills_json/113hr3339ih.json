{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "H. R. 3339"}, "official-title": {"#tail": "\n  ", "#text": "To prohibit the use of funds by the Department of Education relating to multistate academic content standards."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "I"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20131024", "#text": "October 24, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "sponsor": {"#tail": " introduced the following bill; which was referred to the ", "#text": "Mr. Kingston", "@name-id": "K000220"}, "committee-name": {"#tail": "\n      ", "@committee-id": "HED00", "#text": "Committee on Education and the Workforce"}}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-House", "@bill-type": "olc", "@dms-id": "H8A319CBB0E094F68A1B4CC16CEAB7C23", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "One Size Does Not Fit All in Education Act"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H1ED9028FA7B84D4CABF6F575EE5261FB"}, {"@section-type": "subsequent-section", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Prohibition on use of funds for certain purposes"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n       