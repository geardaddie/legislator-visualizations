ttp://namespaces.cato.org/catoxml}entity": {"#tail": ").", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "section 1002 of the Consumer Financial Protection Act of 2010", "@value": "Consumer Financial Protection Act of 2010/s:1002"}, "@entity-type": "law-citation", "#text": "\n                  ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                  ", "@entity-type": "uscode", "#text": "12 U.S.C. 5481", "@value": "usc/12/5481"}, "@parsable-cite": "usc/12/5481", "#text": "\n                    ", "@legal-doc": "usc"}}}, "enum": {"#tail": "\n              ", "#text": "(D)"}, "header": {"#tail": "\n              ", "#text": "Savings promotion raffle"}, "#text": "\n              ", "@id": "HD8A989AF09E44125940F67A95B37E897"}}, "#text": "\n          ", "@id": "H6B334CF9E9894042B6C118D772F886AA"}], "#text": "\n        ", "@id": "H5311B4411F334A9A80423913847A58D5"}], "#text": "\n      ", "@id": "H957D172A70A94E2EADA7FC92CFF11977"}, {"#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "4."}, "header": {"#tail": "\n      ", "#text": "Criminal provisions"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended by adding at the end the following:", "@entity-type": "uscode", "#text": "\n            ", "external-xref": {"#tail": " of title 18, United States Code", "@parsable-cite": "usc-chapter/18/61", "#text": "Chapter 61", "@legal-doc": "usc-chapter"}, "@value": "usc-chapter/18/61"}, "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "quoted-block": {"@style": "USC", "after-quoted-block": {"#tail": "\n        ", "#text": "."}, "#tail": "\n      ", "section": {"#tail": "\n          ", "enum": {"#tail": "\n            ", "#text": "1308."}, "header": {"#tail": "\n            ", "#text": "Limitati