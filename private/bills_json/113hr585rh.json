{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "@display": "yes", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n", "congress": {"#tail": "\n    ", "@display": "yes", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "H. R. 585"}, "official-title": {"#tail": "\n  ", "#text": "To provide for the unencumbering of title to non-Federal land owned by the city of Anchorage, Alaska, for purposes of economic development by conveyance of the Federal reversion interest to the City.", "@display": "yes", "pagebreak": {"#tail": "\n    "}}, "session": {"#tail": "\n    ", "@display": "yes", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "IB"}, "action": [{"action-date": {"#tail": "\n      ", "@date": "20130206", "#text": "February 6, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "sponsor": {"#tail": " introduced the following bill; which was referred to the ", "#text": "Mr. Young of Alaska", "@name-id": "Y000033"}, "committee-name": {"#tail": "\n      ", "@committee-id": "HII00", "#text": "Committee on Natural Resources"}}, "#text": "\n      "}, {"action-date": {"#tail": "\n      ", "#text": "December 16, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "Committed to the Committee of the Whole House on the State of the Union and ordered to be printed", "pagebreak": {"#tail": "\n      "}}, "#text": "\n      "}], "associated-doc": {"#tail": "\n    ", "@display": "yes", "#text": "[Report No. 113\u2013298]", "@role": "report"}, "calendar": {"#tail": "\n    ", "@display": "yes", "#text": "Union Calendar No. 216"}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Reported-in-House", "@bill-type": "olc", "endorsement": {"action-date": {"#tail": "\n    ", "#text": "December 16, 2013"}, "#tail": "\n", "@display": "yes", "action-desc": {"#tail": "\n  ", "#text": "Committed to the Committee of the Whole House on the State of the Union and ordered to be printed"}, "#text": "\n    "}, "@dms-id": "H549453E1428D4FD59D8DDE6EFC5F40B8", "@public-private": "public", "legis-body": {"#tail": "\n  ", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "Anchorage Land Conveyance Act of 2013", "@value": "Anchorage Land Conveyance Act of 2013", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H479BA50B926A4CA6BCE1CFB4D2B6C1D2"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "#text": "In this Act:", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "term": {"#tail": " means the city of Anchorage, Alaska.", "#text": "City"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "header": {"#tail": "\n        ", "#text": "City"}, "#text": "\n        ", "@id": "H7330216980794E2F9DD44F4461C7393C"}, {"#tail": "\n      ", "@id": "H42F0C1FCB9CE4702AF7A7FAD2D998262", "enum": {"#tail": "\n        ", "#text": "(2)"}, "header": {"#tail": "\n        ", "#text": "Non-federal land"}, "text": {"#tail": "\n        ", "term": {"#tail": " means certain parcels of land located in the City and owned by the City, which are more particularly described as follows:", "#text": "non-Federal land"}, "#text": "The term "}, "#text": "\n        ", "subparagraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "Block 42, Original Townsite of Anchorage, Anchorage Recording District, Third Judicial District, State of Alaska, consisting of approximately 1.93 acres, commonly known as the Egan Center, Petrovich Park, and Old City Hall."}, "enum": {"#tail": "\n          ", "#text": "(A)"}, "@id": "HDCE2D9F4C022406DA2C4E1300E778CC6", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "Lots 9, 10, and 11, Block 66, Original Townsite of Anchorage, Anchorage Recording District, Third Judicial District, State of Alaska, consisting of approximately 0.48 acres, commonly known as the parking lot at 7th Avenue and I Street."}, "enum": {"#tail": "\n          ", "#text": "(B)"}, "@id": "HC9E81C37640C4431AD5F32BF784C5EA2", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n        ", "#text": "Lot 13, Block 15, Original Townsite of Anchorage, Anchorage Recording District, Third Judicial District, State of Alaska, consisting of approximately 0.24 acres, an unimproved vacant lot located at H Street and Christensen Drive."}, "enum": {"#tail": "\n          ", "#text": "(C)"}, "@id": "H9FD18D7FA09D4BFCBCCACDA98F9AC7B0", "#text": "\n          "}]}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "federal-body", "#text": "Secretary of the Interior", "@entity-id": "1400"}, "term": {"#tail": " means the ", "#text": "Secretary"}, "#text": "The term "}, "enum": {"#tail": "\n        ", "#text": "(3)"}, "header": {"#tail": "\n        ", "#text": "Secretary"}, "#text": "\n        ", "@id": "H0B4599175ABD4B56A6D1E918CFA8237C"}], "#text": "\n      ", "@id": "HB1D74E4B05684C08941CC317121EA857"}, {"@section-type": "subsequent-section", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "3."}, "header": {"#tail": "\n      ", "#text": "Conveyance of reversionary interests, Anchorage, Alaska"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall convey to the City, without consideration, the reversionary interests of the United States in and to the non-Federal land for the purpose of unencumbering the title to the non-Federal land to enable economic development of the non-Federal land.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, "#text": "Notwithstanding any other provision of law, the "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "#text": "\n        ", "@id": "HFBDF22B9E2B84C2EB386D84245FF5740"}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, "#text": "As soon as practicable after the date of enactment of this Act, the exact legal descriptions of the non-Federal land shall be determined in a manner satisfactory to the "}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Legal descriptions"}, "#text": "\n        ", "@id": "HFE094E9376FF452B8477141BC602CF9A"}, {"#tail": "\n      ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may require such additional terms and conditions to the conveyance under ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}, {"#tail": " as the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Anchorage Land Conveyance Act of 2013/s:3/ss:a", "@proposed": "true"}, {"#tail": " considers appropriate to protect the interests of the United States.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "1400"}], "#text": "The "}, "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "Additional terms and conditions"}, "#text": "\n        ", "@id": "HF3EF54AC2E434347A9C5E479DE6F0EE0"}, {"#tail": "\n    ", "text": {"#tail": "\n      ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", including the costs of any surveys, recording costs, and other reasonable costs.", "@entity-type": "act", "#text": "subsection (a)", "@value": "Anchorage Land Conveyance Act of 2013/s:3/ss:a", "@proposed": "true"}, "#text": "The City shall pay all costs associated with the conveyance under "}, "enum": {"#tail": "\n        ", "#text": "(d)"}, "header": {"#tail": "\n        ", "#text": "Costs"}, "#text": "\n        ", "@id": "HA2EF5E68B7C24D3391B74D5CC7761436"}], "#text": "\n      ", "@id": "HC516DFDEBE8D48638654472051520F59"}], "#text": "\n    ", "@id": "H9A7517D1957F43F6A6F3E2C5648174F6"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-02-06"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 585 RH: Anchorage Land Conveyance Act of 2013"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}