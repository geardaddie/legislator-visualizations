"HD1928D3E29164ED8B2854FBA7FB2656B"}, {"#tail": " \n", "text": {"#tail": " \n", "#text": "\n        ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": " is amended by adding at the end the following:", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "Section 214 of the Immigration and Nationality Act", "@value": "Immigration and Nationality Act/s:214"}, "@entity-type": "law-citation", "#text": "\n          ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n        ", "@entity-type": "uscode", "#text": "8 U.S.C. 1184", "@value": "usc/8/1184"}, "@parsable-cite": "usc/8/1184", "#text": "\n          ", "@legal-doc": "usc"}}, "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Encouraging Canadian tourism to the United States"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n      ", "#text": "."}, "#tail": "\n    ", "subsection": {"#tail": "\n        ", "enum": {"#tail": "\n          ", "#text": "(s)"}, "header": {"#tail": " \n", "#text": "Canadian retirees"}, "paragraph": [{"#tail": " \n", "@id": "H58752BEB7E6449018F889575543EB6EE", "enum": {"#tail": "\n            ", "#text": "(1)"}, "header": {"#tail": "\n            ", "#text": "In general"}, "text": {"#tail": " \n", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may admit as a visitor for pleasure as described in ", "@entity-type": "federal-body", "#text": "Secretary of Homeland Security", "@entity-id": "7000"}, {"#tail": " any alien for a period not to exceed 240 days, if the alien demonstrates, to the satisfaction of the ", "@entity-type": "act", "#text": "section 101(a)(15)(B)", "@value": "Immigration and Nationality Act/s:101/ss:a/p:15/sp:B"}, {"#tail": ", that the alien\u2014", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "7000"}], "#text": "The "}, "#text": "\n            ", "subparagraph": [{"#tail": " \n", "text": {"#tail": "\n            ", "#text": "is a citizen of Canada;"}, "enum": {"#tail": "\n              ", "#text": "(A)"}, "@id": "H739F0D94FC1C4FBE9B747C21CEE461E4", "#text": "\n              "}, {"#tail": " \n", "text": {"#tail": "\n            ", "#text": "is at least 55 years of age;"}, "enum": {"#tail": "\n              ", "#text": "(B)"}, "@id": "HF4E61F8DD8EC452492B961A597A27AF8", "#text": "\n              "}, {"#tail": " \n", "text": {"#tail": "\n            ", "#text": "maintains a residence in Canada;"}, "enum": {"#tail": "\n              ", "#text": "(C)"}, "@id": "HB5B40CA00AD449BFA17C93ECA02245E5", "#text": "\n              "}, {"#tail": " \n", "text": {"#tail": "\n            ", "#text": "owns a residence in the United States or has signed a rental agreement for accommodations in the United States for the duration of the alien\u2019s stay in the United States;"}, "enum": {"#tail": "\n              ", "#text": "(D)"}, "@id": "HF6CD00021E944B45A9FB91DA83A621A2", "#text": "\n              "}, {"#tail": " \n", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ";", "@entity-type": "act", "#text": "section 212", "@value": "Immigration and Nationality Act/s:212"}, "#text": "is not inadmissible under "}, "enum": {"#tail": "\n              ", "#text": "(E)"}, "@id": "H7228305D07734FAAA1931FE62B22A251", "#text": "\n              "}, {"#tail": " \n", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ";", "@entity-type": "act", "#text": "section 237", "@value": "Immigration and Nationality Act/s:237"}, "#text": "is not described in any ground of deportability under "}, "enum": {"#tail": "\n              ", "#text": "(F)"}, "@id": "H12E123F0B2E4442AB4159F33EFBCD8F6", "#text": "\n              "}, {"#tail": " \n", "text": {"#tail": "\n            ", "#text": "will not engage in employment or labor for hire in the United States; and"}, "enum": {"#tail": "\n              ", "#text": "(G)"}, "@id": "HCDF98914FEAA4731B964F1C70B762E09", "#text": "\n              "}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "#text": "will not seek any form of assistance or benefit described in ", "{http://namespaces.cato.org/catoxml}entity": {"#tail": ".", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (", "@entity-type": "act", "#text": "section 403(a) of the Personal Responsibility and Work Opportunity Reconciliation Act of 1996", "@value": "Personal Responsibility and Work Opportunity Reconciliation Act of 1996/s:403/ss:a"}, "@entity-type": "law-citation", "#text": "\n                  ", "external-xref": {"#tail": ")", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                ", "@entity-type": "uscode", "#text": "8 U.S.C. 1613(a)", "@value": "usc/8/1613/a"}, "@parsable-cite": "usc/8/1613", "#text": "\n                  ", "@legal-doc": "usc"}}}, "enum": {"#tail": "\n              ", "#text": "(H)"}, "@id": "H0C26C38F2CF54C3F812064A64ABD3F48", "#text": "\n              "}]}, {"#tail": " \n", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " may be admitted under the same terms as the principal alien if the spouse satisfies the requirements of ", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Immigration and Nationality Act/s:214/ss:s/p:1", "@proposed": "true"}, {"#tail": ", other than ", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Immigration and Nationality Act/s:214/ss:s/p:1", "@proposed": "true"}, {"#tail": " and ", "@entity-type": "act", "#text": "subparagraphs (B)", "@value": "Immigration and Nationality Act/s:214/ss:s/p:1/sp:B", "@proposed": "true"}, {"#tail": ".", "@entity-type": "act", "#text": "(D)", "@value": "Immigration and Nationality Act/s:214/ss:s/p:1/sp:D", "@proposed": "true"}], "#text": "The spouse of an alien described in "}, "enum": {"#tail": "\n            ", "#text": "(2)"}, "header": {"#tail": "\n            ", "#text": "Spouse"}, "#text": "\n            ", "@id": "H3B15551B991F4C119388722EC343EAE4"}, {"#tail": " \n", "text": {"#tail": "\n          ", "#text": "In determining eligibility for admission under this subsection, maintenance of a residence in the United States shall not be considered evidence of intent by the alien to abandon the alien\u2019s residence in Canada."}, "enum": {"#tail": "\n            ", "#text": "(3)"}, "header": {"#tail": "\n            ", "#text": "Immigrant intent"}, "#text": "\n            ", "@id": "H84CFA4AA18F342018227C5F2099675D4"}, {"#tail": " \n", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " pursuant to this subsection for a period not to exceed 240 days, beginning on the date of admission. Periods of time spent outside the United States during such 240-day period shall not toll the expiration of such 240-day period.", "@entity-type": "act", "#text": "section 101(a)(15)(B)", "@value": "Immigration and Nationality Act/s:101/ss:a/p:15/sp:B"}, "#text": "During any single 365-day period, an alien may be admitted as described in "}, "enum": {"#tail": "\n            ", "#text": "(4)"}, "header": {"#tail": "\n            ", "#text": "Period of admission"}, "#text": "\n            ", "@id": "HD1FD43C146EF457FB7BCB56F74DAC4D2"}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " (other than subtitle B thereof). ", "@entity-type": "act", "#text": "Internal Revenue Code of 1986", "@value": "Internal Revenue Code of 1986"}, "#text": "An alien admitted pursuant to this subsection shall be treated as a nonresident alien for purposes the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n            ", "#text": "(5)"}, "header": {"#tail": "\n            ", "#text": "Non-resident alien tax status"}, "#text": "\n            ", "@id": "H457E1A1A9DF4436F9EAF9EEBD8AD5831"