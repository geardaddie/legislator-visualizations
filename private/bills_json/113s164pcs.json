{"bill": {"@star-print": "no-star-print", "#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "@display": "yes", "#text": "IN THE SENATE OF THE UNITED\n\t\t  STATES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "@display": "yes", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "@display": "yes", "#text": "S. 164"}, "official-title": {"#tail": "\n\t", "@display": "yes", "#text": "To prohibit the United States from\n\t\t  providing financial assistance to Pakistan until Dr. Shakil Afridi is\n\t\t  freed."}, "@display": "yes", "session": {"#tail": "\n\t\t", "@display": "yes", "#text": "1st Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "II"}, "action": [{"action-date": {"#tail": "\n\t\t\t", "@date": "20130128", "#text": "January 28, 2013"}, "#tail": "\n\t\t", "@display": "yes", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "sponsor": {"#tail": " introduced the\n\t\t\t following bill; which was read the first time", "#text": "Mr. Paul", "@name-id": "S348"}}, "#text": "\n\t\t\t"}, {"action-date": {"#tail": "\n\t\t\t", "@date": "20130129", "#text": "January 29, 2013"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "Read the second time and placed on the calendar\n\t\t\t "}, "#text": "\n\t\t\t"}], "calendar": {"#tail": "\n\t\t", "#text": "Calendar No. 8"}, "legis-type": {"#tail": "\n\t\t", "@display": "yes", "#text": "A BILL"}}, "@stage-count": "1", "@bill-stage": "Placed-on-Calendar-Senate", "@bill-type": "olc", "endorsement": {"action-date": {"#tail": "\n\t\t", "@date": "20130129", "#text": "January 29, 2013"}, "#tail": "\n", "action-desc": {"#tail": "\n\t", "#text": "Read the second time and placed on the calendar\n\t\t  "}, "#text": "\n\t\t"}, "@public-private": "public", "legis-body": {"#tail": "\n\t", "@style": "OLC", "section": {"@section-type": "section-one", "#tail": "\n  ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " certifies to ", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": " that\u2014", "@entity-type": "federal-body", "#text": "Congress", "@entity-id": "0001"}], "#text": "The United States may\n\t\t\t not obligate or expend any direct financial assistance to the Government of\n\t\t\t Pakistan unless the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Limitation on foreign assistance to\n\t\t\t Pakistan", "@display-inline": "yes-display-inline"}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "#text": "Dr. Shakil Afridi has been released from\n\t\t\t prison in Pakistan;", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@display-inline": "no-display-inline", "#text": "\n        ", "@id": "idc9f486f3441548438c80b06a52cd2ebb", "@commented": "no"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "#text": "any criminal charges brought against Dr.\n\t\t\t Afridi, including treason, have been dropped; and", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "@display-inline": "no-display-inline", "#text": "\n        ", "@id": "id99B8FF1F60044968A4C393DFEDD33F47", "@commented": "no"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "#text": "Dr. Afridi has been allowed to leave\n\t\t\t Pakistan, if such departure is necessary to ensure his freedom.", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(3)"}, "@display-inline": "no-display-inline", "#text": "\n        ", "@id": "id49F3F253C8974403817DB71401B114F0", "@commented": "no"}], "@display-inline": "no-display-inline", "#text": "\n      ", "@id": "S1", "@commented": "no"}, "#text": "\n\t\t", "@display-enacting-clause": "yes-display-enacting-clause"}, "@public-print": "no", "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2013-01-29"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 S164 PCS: To prohibit the United States from providing financial assistance to Pakistan until Dr. Shakil Afridi is freed."}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. Senate"}, "#text": "\n"}, "#text": "\n"}}}