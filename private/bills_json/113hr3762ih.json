{"bill": {"#text": "\n  ", "form": {"current-chamber": {"#tail": "\n    ", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n", "congress": {"#tail": "\n    ", "#text": "113th CONGRESS"}, "#tail": "\n  ", "legis-num": {"#tail": "\n    ", "#text": "H. R. 3762"}, "official-title": {"#tail": "\n  ", "#text": "To impose penalties for the unauthorized disclosure of personal tax information by Federal employees."}, "session": {"#tail": "\n    ", "#text": "1st Session"}, "distribution-code": {"#tail": "\n    ", "@display": "yes", "#text": "I"}, "action": {"action-date": {"#tail": "\n      ", "@date": "20131212", "#text": "December 12, 2013"}, "#tail": "\n    ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "sponsor": {"#tail": " introduced the following bill; which was referred to the ", "#text": "Mr. Posey", "@name-id": "P000599"}, "committee-name": [{"#tail": ", and in addition to the Committee on the ", "@committee-id": "HGO00", "#text": "Committee on Oversight and Government Reform"}, {"#tail": ", for a period to be subsequently determined by the Speaker, in each case for consideration of such provisions as fall within the jurisdiction of the committee concerned", "@committee-id": "HJU00", "#text": "Judiciary"}]}, "#text": "\n      "}, "legis-type": {"#tail": "\n    ", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-House", "@bill-type": "olc", "@dms-id": "HE76FCE0BDF7549519535144FB6D17FAF", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n    ", "quote": {"short-title": {"#tail": "\n        ", "#text": "Personal Tax Information Protection Act"}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "HFB9F776C69204890B20C6C5AEF467A51"}, {"#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n      ", "#text": "Unauthorized disclosure of personally identifiable covered information"}, "subsection": [{"#tail": "\n      ", "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "Liability for Certain Acts"}, "paragraph": [{"#tail": "\n        ", "@id": "H6EDF1FED85D944FB8DC6AB47EA80CCC8", "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "text": {"#tail": "\n          ", "#text": "It shall be unlawful for any officer or employee of the United States\u2014"}, "#text": "\n          ", "subparagraph": [{"#tail": "\n          ", "text": {"#tail": "\n          ", "#text": "willfully to make an unauthorized disclosure of personally identifiable covered information; or"}, "enum": {"#tail": "\n            ", "#text": "(A)"}, "@id": "H59B01B702F6C4D67B530707B73C25FB8", "#text": "\n            "}, {"#tail": "\n        ", "text": {"#tail": "\n          ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "subparagraph (A)", "@value": "Personal Tax Information Protection Act/s:2/ss:a/p:1/sp:A", "@proposed": "true"}, "#text": "to conspire to commit a violation of "}, "enum": {"#tail": "\n            ", "#text": "(B)"}, "@id": "H7728058130F846B6B233AC89637B22F6", "#text": "\n            "}]}, {"#tail": "\n      ", "subparagraph": [{"#tail": "\n          ", "clause": [{"#tail": "\n            ", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", or", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Personal Tax Information Protection Act/s:2/ss:a/p:1", "@proposed": "true"}, "#text": "the penalty specified in the law setting forth the offense which covers a violation of "}, "enum": {"#tail": "\n              ", "#text": "(i)"}, "@id": "H2FC90A9D3C484EF7B4E1B69E38BD201B", "#text": "\n              "}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "act", "#text": "subparagraph (B)", "@value": "Personal Tax Information Protection Act/s:2/ss:a/p:2/sp:B", "@proposed": "true"}, "#text": "the penalty set forth in "}, "enum": {"#tail": "\n              ", "#text": "(ii)"}, "@id": "H3C04DF8662984D6697BB08B4A38B7B63", "#text": "\n              "}], "enum": {"#tail": "\n            ", "#text": "(A)"}, "header": {"#tail": "\n            ", "#text": "In general"}, "text": {"#tail": "\n            ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall be subject to a penalty of not more than the greatest of\u2014", "@entity-type": "act", "#text": "paragraph (1)", "@value": "Personal Tax Information Protection Act/s:2/ss:a/p:1", "@proposed": "true"}, "#text": "Any violation of "}, "#text": "\n            ", "@id": "H200B30A320014144ACD62C52B565A554"}, {"#tail": "\n          ", "clause": [{"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "a fine in any amount not exceeding $100,000 for each such violation and imprisonment of not more than 7 years,", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n              ", "#text": "(i)"}, "@id": "H20FD9B209C2F4455A2B18D177DEEAED4", "#text": "\n              "}, {"#tail": "\n            ", "text": {"#tail": "\n            ", "#text": "the costs of prosecution, and"}, "enum": {"#tail": "\n              ", "#text": "(ii)"}, "@id": "H62BE564F57834DD798F1467D345F5D4D", "#text": "\n              "}, {"#tail": "\n          ", "text": {"#tail": "\n            ", "#text": "dismissal from office or discharge from employment."}, "enum": {"#tail": "\n              ", "#text": "(iii)"}, "@id": "H885471D234C940EEA230F955F698E76C", "#text": "\n              "}], "enum": {"#tail": "\n            ", "#text": "(B)"}, "header": {"#tail": "\n            ", "#text": "Uniform penalty"}, "text": {"#t