{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "@display": "yes", "#text": "IN THE HOUSE OF REPRESENTATIVES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "@display": "yes", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "H. R. 5683"}, "official-title": {"#tail": "\n\t", "#text": "To ensure appropriate judicial review of Federal Government actions by amending the prohibition on\n\t\t\t the exercise of jurisdiction by the United States Court of Federal Claims\n\t\t\t of certain claims pending in other courts.", "@display": "yes", "pagebreak": {"#tail": "\n    "}}, "session": {"#tail": "\n\t\t", "@display": "yes", "#text": "2d Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "IB"}, "action": [{"action-date": {"#tail": "\n\t\t\t", "@date": "20141112", "#text": "November 12, 2014"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "committee-name": {"#tail": "\n      ", "@committee-id": "HJU00", "#text": "Committee on the Judiciary"}, "sponsor": {"#tail": " (for himself and ", "#text": "Mr. DeSantis", "@name-id": "D000621"}, "cosponsor": {"#tail": ") introduced the following bill; which was referred to the ", "#text": "Mr. Cicilline", "@name-id": "C001084"}}, "#text": "\n\t\t\t"}, {"action-date": {"#tail": "\n\t\t\t", "#text": "December 8, 2014"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "Committed to the Committee of the Whole House on the State of the Union and ordered to be printed"}, "#text": "\n\t\t\t"}, {"#tail": "\n    ", "#text": "\n      ", "action-desc": {"#tail": "\n    ", "#text": "\n        ", "pagebreak": {"#tail": "\n      "}}}], "associated-doc": {"#tail": "\n\t\t", "@display": "yes", "#text": "[Report No. 113\u2013650]", "@role": "report"}, "calendar": {"#tail": "\n\t\t", "@display": "yes", "#text": "Union Calendar No. 488"}, "legis-type": {"#tail": "\n\t\t", "#text": "A BILL"}}, "@bill-stage": "Reported-in-House", "@bill-type": "olc", "endorsement": {"action-date": {"#tail": "\n\t\t", "#text": "December 8, 2014"}, "#tail": "\n", "@display": "yes", "action-desc": {"#tail": "\n  ", "#text": "Committed to the Committee of the Whole House on the State of the Union and ordered to be printed"}, "#text": "\n\t\t"}, "@dms-id": "H416110EBB3854FA2980E64166E27DE92", "@public-private": "public", "legis-body": {"#tail": "\n\t", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "Ensuring Access to Justice for Claims Against the United States Act", "@value": "Ensuring Access to Justice for Claims Against the United States Act", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H7A564ECA22A84B639B9131798121614F"}, {"@section-type": "subsequent-section", "#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n\t\t\t", "#text": "Jurisdiction of United States Court of Federal Claims"}, "subsection": [{"#tail": "\n      ", "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n\t\t\t\t", "#text": "Amendments to title 28, United States Code"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended to read as follows:", "@entity-type": "uscode", "#text": "\n              ", "external-xref": {"#tail": " of title 28, United States Code", "@parsable-cite": "usc/28/1500", "#text": "Section 1500", "@legal-doc": "usc"}, "@value": "usc/28/1500"}, "#text": "\n            "}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "quoted-block": {"@style": "USC", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "#tail": "\n\t\t\t\t", "section": {"#tail": "\n            ", "enum": {"#tail": "\n              ", "#text": "1500."}, "header": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "Presumption of stay"}, "subsection": [{"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "In this section\u2014"}, "enum": {"#tail": "\n                ", "#text": "(a)"}, "header": {"#tail": "\n                ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "term": {"#tail": " means any of the courts described in section 610; and", "#text": "court"}, "#text": "the term "}, "enum": {"#tail": "\n                  ", "#text": "(1)"}, "@id": "H8845772EA6DF4029BF5B4F2FA2054410", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ".", "@entity-type": "federal-body", "#text": "United States Court of Federal Claims", "@entity-id": "1005"}, "term": {"#tail": " means the ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n                    ", "@entity-type": "federal-body", "#text": "Court of Federal Claims", "@entity-id": "1005"}, "#text": "\n                      "}, "#text": "the term "}, "enum": {"#tail": "\n                  ", "#text": "(2)"}, "@id": "HC125A0F4B30D47CE9E4A20FD95AD9BD7", "#text": "\n                  "}], "#text": "\n                ", "@id": "H658C0F79BF0F4080B7FF68E6125F1F03"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", and the plaintiff or assignee of the plaintiff also has\n\t\t\t pending in any other court a civil action that includes a claim against\n\t\t\t the United States or an agency or officer of the United States arising\n\t\t\t from substantially the same set of operative facts, the court presiding\n\t\t\t over the action that was filed later shall stay the action, in whole or in\n\t\t\t part, until the action that was filed first is no longer pending.", "@entity-type": "federal-body", "#text": "Court\n\t\t\t of Federal Claims", "@entity-id": "1005"}, "#text": "Except as provided in subsection (d), if a civil action is pending in, or on appeal from, the "}, "enum": {"#tail": "\n                ", "#text": "(b)"}, "header": {"#tail": "\n                ", "#text": "Presumption of stay"}, "#text": "\n                ", "@id": "HA6257DA984204F60ADFA1B61DD4F8C42"}, {"#tail": "\n              ", "enum": {"#tail": "\n                ", "#text": "(c)"}, "header": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "Determining first-Filed action"}, "paragraph": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " is the date on which the action was filed\n\t\t\t in the ", "@entity-type": "federal-body", "#text": "Court of Federal Claims", "@entity-id": "1005"}, {"#tail": ".", "@entity-type": "federal-body", "#text": "Court of Federal Claims", "@entity-id": "1005"}], "#text": "For purposes of subsection (b), the date of filing for a civil action that is pending on appeal\n\t\t\t from the "}, "enum": {"#tail": "\n                  ", "#text": "(1)"}, "header": {"#tail": "\n                  ", "#text": "Appeals"}, "#text": "\n                  ", "@id": "H15CC2DA8348242EEB55FD9693D4565B2"}, {"#tail": "\n              ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall be treated as having\n\t\t\t been filed first.", "@entity-type": "federal-body", "#text": "Court of Federal Claims", "@entity-id": "1005"}, "#text": "For purposes of subsection (b), if the actions described in subsection (b) were filed on the same\n\t\t\t day, without regard to the time of day, the action that is pending in, or\n\t\t\t on appeal from, the "}, "enum": {"#tail": "\n                  ", "#text": "(2)"}, "header": {"#tail": "\n                  ", "#text": "Actions or appeals filed on same day"}, "#text": "\n                  ", "@id": "HFD776C84F1404EC1889358F164E61F56"}], "#text": "\n                ", "@id": "HBEC190A175B44CD293F5C6828E09F196"}, {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "The requirement to stay an action under subsection (b) shall not apply if\u2014"}, "enum": {"#tail": "\n                ", "#text": "(d)"}, "header": {"#tail": "\n                ", "#text": "Exceptions"}, "paragraph": [{"#tail": "\n                ", "text": {"#tail": "\n\t\t\t\t\t\t\t\t", "#text": "the parties in each of the actions that include a claim based on substantially the same set of\n\t\t\t operative facts otherwise agree; or"}, "enum": {"#tail": "\n                  ", "#text": "(1)"}, "@id": "HEE8C6EA52306498E933BFA4C485C8A2B", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "#text": "the required stay is not, or ceases to be, in the interests of justice."}, "enum": {"#tail": "\n                  ", "#text": "(2)"}, "@id": "HD6FA0A92D0094125AF36278555828851", "#text": "\n                  "}], "#text": "\n                ", "@id": "H6A00F14EA9974916A0DBD41EA95DD6CF"}], "#text": "\n              ", "@id": "H54F9B98793184B188928496A1013A0F0"}, "@display-inline": "no-display-inline", "#text": "\n\t\t\t\t\t\t", "@id": "H2E07FFC84A4047BABE47D9419CF58B09"}, "#text": "\n          ", "@id": "H91E245348D544AB1881E09BB396FCB78"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", is amended by striking the\n\t\t\t item relating to ", "@entity-type": "uscode", "#text": "\n              ", "external-xref": {"#tail": " of title 28, United States Code", "@parsable-cite": "usc-chapter/28/91", "#text": "chapter 91", "@legal-doc": "usc-chapter"}, "@value": "usc-chapter/28/91"}, {"#tail": " and inserting the following:", "@entity-type": "uscode", "#text": "section 1500", "@value": "usc/28/1500", "@proposed": "true"}], "#text": "The table of sections for "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Technical and conforming amendment"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "#tail": "\n\t\t\t\t", "@display-inline": "no-display-inline", "toc": {"#tail": "\n            ", "#text": "\n\t\t\t\t\t\t\t", "toc-entry": {"#tail": "\n            ", "@level": "section", "#text": "1500. Presumption of stay.", "@bold": "off"}}, "#text": "\n\t\t\t\t\t\t", "@id": "H21C6AF0AEFBB4549972D07F42D2CC710"}, "#text": "\n          ", "@id": "HAFF84E47CB7B402F90993BCDF217317B"}], "#text": "\n        ", "@id": "H3EDF15C903AA46509D6F00FA31666D77"}, {"#tail": "\n    ", "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n\t\t\t\t", "#text": "Applicability"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"@proposed": "true", "#tail": ", as amended by subsection (a), shall apply to any\n\t\t\t action pending on, or filed on or after, the date of the enactment of this\n\t\t\t Act, but does not apply in a case in which the action filed later (as\n\t\t\t determined in accordance with such section, as so amended) is pending (on\n\t\t\t such date of enactment) in a court of appeals of the United States or the\n\t\t\t ", "@entity-type": "uscode", "@value": "usc/28/1500", "external-xref": {"#tail": " of title 28, United States Code", "@parsable-cite": "usc/28/1500", "#text": "Section 1500", "@legal-doc": "usc"}, "#text": "\n              "}, {"#tail": ", or in a case in which a judgment has\n\t\t\t been entered as of such date of enactment but for which the time to file\n\t\t\t an appeal has not expired.", "@entity-type": "federal-body", "#text": "Supreme Court of the United States", "@entity-id": "1001"}], "#text": "\n            ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "header": {"#tail": "\n          ", "#text": "In general"}, "#text": "\n          ", "@id": "HCA90833861C1415C912DA202129BC274"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": ", or\n\t\t\t for which the time to file an appeal has not expired, shall not be subject\n\t\t\t to the jurisdictional bar under ", "@entity-type": "federal-body", "#text": "Supreme Court of the United States", "@entity-id": "1001"}, {"@proposed": "true", "#tail": ", as in effect on the day before the date of the enactment of this\n\t\t\t Act.", "@entity-type": "uscode", "@value": "usc/28/1500", "external-xref": {"#tail": " of title 28, United States\n\t\t\t Code", "@parsable-cite": "usc/28/1500", "#text": "section 1500", "@legal-doc": "usc"}, "#text": "\n              "}], "#text": "Any claim in an action pending on the date of the enactment of this Act either in a court of\n\t\t\t appeals of the United States or the ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "header": {"#tail": "\n          ", "#text": "Previous jurisdictional bar"}, "#text": "\n          ", "@id": "H65493E54467C464482E7A91AD2F2D874"}], "#text": "\n        ", "@id": "H50F507AD1F0A4B41A0CCE14AD6CF8EEF"}], "#text": "\n      ", "@id": "H7085732A804B40AF9BC0C6B3D8BACF44"}], "#text": "\n\t\t", "@id": "H9DCCE2CDA33C4C1D9BF7BFE3CCA2B4BB"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-11-12"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 5683 RH: Ensuring Access to Justice for Claims Against the United States Act"}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. House of Representatives"}, "#text": "\n"}, "#text": "\n"}}}