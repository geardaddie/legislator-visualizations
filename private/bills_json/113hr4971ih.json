"@bill-type": "olc", "@dms-id": "H9BE595C7F6D44333B63111927B9CCB8B", "@public-private": "public", "legis-body": {"#tail": "\n", "@style": "OLC", "section": [{"@section-type": "section-one", "#tail": "\n    ", "text": {"#tail": "\n\t\t", "quote": {"short-title": {"#tail": "\n        ", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\n          ", "@entity-type": "act", "#text": "Ask Veterans Act", "@value": "Ask Veterans Act", "@proposed": "true"}, "#text": "\n            "}, "#tail": ".", "#text": "\n          "}, "#text": "This Act may be cited as the ", "@display-inline": "no-display-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "H2868ECC7B9534A6F9AD130960272971F"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n\t\t\t", "#text": "Survey of veteran experiences with Department of Veterans Affairs medical care"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall seek to enter into a contract with a non-government entity\n\t\t\t with significant experience conducting scientifically verifiable surveys\n\t\t\t and research to conduct an annual survey of a statistically significant\n\t\t\t sample of veterans who reside in the geographic area served by each of the\n\t\t\t medical facilities of the ", "@entity-type": "federal-body", "#text": "Secretary of Veterans Affairs", "@entity-id": "3600"}, {"#tail": " to determine the\n\t\t\t nature of the experiences of such veterans in obtaining hospital care and\n\t\t\t medical services furnished by the ", "@entity-type": "federal-body", "#text": "Department of Veterans Affairs", "@entity-id": "3600"}, {"#tail": " at each such medical facility.\n\t\t\t Each such survey shall be conducted using scientific and verifiable\n\t\t\t methods. Such contract shall provide that the non-government entity shall\n\t\t\t conduct such annual surveys during the five-year period beginning on the\n\t\t\t date on which the ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "3600"}, {"#tail": " enters into the contract with the\n\t\t\t non-government entity.", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "3600"}], "#text": "The ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In general"}, "#text": "\n        ", "@id": "HEA983A9151A2418687F2A38FC74A00BF"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall provide that each survey conducted pursuant to\n\t\t\t the contract shall be specific to a medical facility of the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Ask Veterans Act/s:2/ss:a", "@proposed": "true"}, {"#tail": " and\n\t\t\t shall include questions relating to the experiences of veterans in\n\t\t\t requesting and receiving appointments for hospital care and medical\n\t\t\t services furnished by the ", "@entity-type": "federal-body", "#text": "Department", "@entity-id": "3600"}, {"#tail": " at that medical facility, including\n\t\t\t questions relating to each of the following:", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "3600"}], "#text": "The contract entered into under "}, "enum": {"#tail": "\n        ", "#text": "(b)"}, "header": {"#tail": "\n        ", "#text": "Contents"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "#text": "The veteran\u2019s ability to obtain hospital care and medical services at the facility in a timely\n\t\t\t manner."}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "H005A4DFF97F14621B55D7072A636593C", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "#text": "The period of time between the date on which the veteran requests an appointment at the facility\n\t\t\t and the date on which the appointment is scheduled."}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@id": "H191D951199964862902091AD0C636C68", "#text": "\n          "}, {"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "#text": "The frequency with which scheduled appointments are cancelled by the facility."}, "enum": {"#tail": "\n          ", "#text": "(3)"}, "@id": "H27503109254146DEB88775C29684C2D6", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "#text": "The quality of hospital care or medical services the veteran has received at the facility."}, "enum": {"#tail": "\n          ", "#text": "(4)"}, "@id": "HCD22EDCAEE6C4D24969C4166734FE4DE", "#text": "\n          "}], "#text": "\n        ", "@id": "HAE70B3342484413AB7CB0793D6ABC6FD"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall provide that in designing and conducting the\n\t\t\t surveys for each medical facility of the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Ask Veterans Act/s:2/ss:a", "@proposed": "true"}, {"#tail": " pursuant to such\n\t\t\t contract, the non-government entity shall consult with veterans service\n\t\t\t organizations.", "@entity-type": "federal-body", "#text": "Department", "@entity-id": "3600"}], "#text": "The contract entered into under "}, "enum": {"#tail": "\n        ", "#text": "(c)"}, "header": {"#tail": "\n        ", "#text": "Consultation"}, "#text": "\n        ", "@id": "HEF70B08A3EBE47CAA5792E71361DA3B8"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall provide that\u2014", "@entity-type": "act", "#text": "subsection (a)", "@value": "Ask Veterans Act/s:2/ss:a", "@proposed": "true"}, "#text": "The contract entered into under "}, "enum": {"#tail": "\n        ", "#text": "(d)"}, "header": {"#tail": "\n        ", "#text": "Certification"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " who shall assess whether the\n\t\t\t survey is scientifically valid and whether the proposed sample size of\n\t\t\t veterans to be surveyed is statistically significant; and", "@entity-type": "federal-body", "#text": "Comptroller General", "@entity-id": "0500"}, "#text": "before conducting a survey pursuant to the contract, the non-government entity shall submit the\n\t\t\t proposed survey to the "}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "H35E903CAF1894741A226C5953C9A608B", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " provides such\n\t\t\t a certification for the survey.", "@entity-type": "federal-body", "#text": "Comptroller General", "@entity-id": "0500"}, "#text": "the non-government entity may not conduct such a survey until the "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "@id": "H952CA197C51F4A8A937D040C42FCE829", "#text": "\n          "}], "#text": "\n        ", "@id": "H4AB75BBD547642ED9445FE20F9CC9950"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " for a year, the ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Ask Veterans Act/s:2/ss:a", "@proposed": "true"}, {"#tail": " shall make the results\n\t\t\t of the surveys publicly available on the Internet website of the\n\t\t\t ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "3600"}, {"#tail": ".", "@entity-type": "federal-body", "#text": "Department", "@entity-id": "3600"}], "#text": "Not later than 30 days after the completion of the surveys conducted pursuant to a contract entered\n\t\t\t into under "}, "enum": {"#tail": "\n        ", "#text": "(e)"}, "header": {"#tail": "\n        ", "#text": "Submittal of results and public availability of information"}, "#text": "\n        ", "@id": "H96117A204FF849F4B38E62B9457D80C2"}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " shall not apply to this section.", "@entity-type": "uscode", "#text": "Subchapter I of ", "external-xref": {"#tail": " of title 44, United States Code", "@parsable-cite": "usc-chapter/44/35", "#text": "chapter 35", "@legal-doc": "usc-chapter"}, "@value": "usc-chapter/44/35/I"}, "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(f)"}, "header": {"#tail": "\n        ", "#text": "Paperwork reduction"}, "#text": "\n        ", "@id": "H0D3ACA6CDD7F4E11A029CEE0FE6CE228"}, {"#tail": "\n    ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " shall enter into a contract under ", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "3600"}, {"#tail": " for each medical facility of the\n\t\t\t ", "@entity-type": "act", "#text": "subsection (a)", "@value": "Ask Veterans Act/s:2/ss:a", "@proposed": "true"}, {"#tail": " by not later than 180 days after the date of the enactment of\n\t\t\t this Act.", "@entity-type": "federal-body", "#text": "Department", "@entity-id": "3600"}], "#text": "The "}, "enum": {"#tail": "\n        ", "#text": "(g)"}, "header": {"#tail": "\n        ", "#text": "Deadline for implementation"}, "#text": "\n        ", "@id": "H1B88865A4304468BA503EC809B9FD635"}], "#text": "\n      ", "@id": "HACEE9F9681AD4BAEBAD94EB1FC0E6107"}], "#text": "\n\t\t", "@id": "HC0C666E20C2641A88092B49890FA6935"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-06-25"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 HR 4971 IH: Ask Veterans Act"}, 