": {"#tail": "\n                ", "#text": "(2)"}, "header": {"#tail": "\n                ", "#text": "Volunteer\n\t\t\t\tProtection Act"}, "#text": "\n                ", "@id": "HF26A5EEC5A63428EB884364D6B072522", "@commented": "no"}], "#text": "\n              ", "@id": "H6227FC49D8D6437A900DC16F18F3E81A", "@commented": "no"}, {"#tail": "\n          ", "text": {"#tail": "\n              ", "#text": "In\n\t\t\t\tthis section:"}, "enum": {"#tail": "\n              ", "#text": "(e)"}, "header": {"#tail": "\n              ", "#text": "Definitions"}, "paragraph": [{"#tail": "\n              ", "text": {"#tail": "\n                ", "term": {"#tail": "means\u2014", "#text": "disaster"}, "#text": "The term"}, "enum": {"#tail": "\n                ", "#text": "(1)"}, "#text": "\n                ", "@id": "HE6C42746DB5D47FB846D0DCF288ACEA6", "subparagraph": [{"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "under the", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": ";", "@entity-type": "act", "#text": "National Emergencies Act", "@value": "National Emergencies Act"}], "#text": "a national\n\t\t\t\temergency declared by the"}, "enum": {"#tail": "\n                  ", "#text": "(A)"}, "@id": "H26D8EECA4BC74280BABD571D7BF49D36", "#text": "\n                  "}, {"#tail": "\n                ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "under the", "@entity-type": "federal-body", "#text": "President", "@entity-id": "1100"}, {"#tail": "; or", "@entity-type": "act", "#text": "Robert T. Stafford Disaster\n\t\t\t\tRelief and Emergency Assistance Act", "@value": "Robert T. Stafford Disaster Relief and Emergency Assistance Act"}], "#text": "an emergency or\n\t\t\t\tmajor disaster declared by the"}, "enum": {"#tail": "\n                  ", "#text": "(B)"}, "@id": "H025189D9B15645F0876397736CA14AB1", "#text": "\n                  "}, {"#tail": "\n              ", "text": {"#tail": "\n                ", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": "under", "@entity-type": "federal-body", "#text": "Secretary", "@entity-id": "7022"}, {"#tail": ".", "@entity-type": "act", "#text": "section 319 of this Act", "@value": "Public Health Service Act/s:319"}], "#text": "a public health\n\t\t\t\temergency determined by the"}, "enum": {"#tail": "\n                  ", "#text": "(C)"}, "@id": "H0DA0F4491EB945019E1BF189FCA2CB02", "#text": "\n                  "}]}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": "includes physical, nonphysical, economic, and noneconomic\n\t\t\t\tlosses.", "#text": "harm"}, "#text": "The term"}, "enum": {"#tail": "\n                ", "#text": "(2)"}, "@id": "H6C4054EA8E9E4AE685926073925A7672", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": "means an individual who is licensed,\n\t\t\t\tcertified, or authorized in one or more States to practice a health care\n\t\t\t\tprofession.", "#text": "health care professional"}, "#text": "The term"}, "enum": {"#tail": "\n                ", "#text": "(3)"}, "@id": "H1F6CC7BA04234B41B8AA46FFE1BBE588", "#text": "\n                "}, {"#tail": "\n              ", "text": {"#tail": "\n              ", "term": {"#tail": "includes each of the several States, the District of\n\t\t\t\tColumbia, the Commonwealth of Puerto Rico, the Virgin Islands, Guam, American\n\t\t\t\tSamoa, the Northern Mariana Islands, and any other territory or possession of\n\t\t\t\tthe United States.", "#text": "State"}, "#text": "The term"}, "enum": {"#tail": "\n                ", "#text": "(4)"}, "@id": "H61937EE911C440029DD13E04F1C83F0B", "#text": "\n                "}, {"#tail": "\n            ", "subparagraph": [{"@display-inline": "yes-display-inline", "#tail": "\n                ", "text": {"#tail": "\n        