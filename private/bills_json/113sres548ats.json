n-type": "senate-resolution", "resolution-body": {"#tail": "\n", "section": {"@section-type": "undesignated-section", "#tail": "\n  ", "text": {"#tail": "\n\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": "\u2014", "@entity-type": "federal-body", "#text": "Senate", "@entity-id": "0010"}, "#text": "That the "}, "enum": {"#tail": "\n      "}, "paragraph": [{"#tail": "\n      ", "text": {"#tail": "\n      ", "quote": {"#tail": "; and", "#text": "Small Business Saturday"}, "#text": "designates November 29, 2014, as "}, "enum": {"#tail": "\n        ", "#text": "(1)"}, "@id": "id4b22ea57dbd4406fa18a9d99d7d1cce7", "#text": "\n        "}, {"#tail": "\n    ", "text": {"#tail": "\n        ", "#text": "supports efforts\u2014"}, "enum": {"#tail": "\n        ", "#text": "(2)"}, "#text": "\n        ", "@id": "id1a56201bcb134f2c8b649cf99797ed85", "subparagraph": [{"#tail": "\n        ", "text": {"#tail": "\n        ", "#text": "to encourage consumers to shop locally; and"}, "enum": {"#tail": "\n          ", "#text": "(A)"}, "@id": "id33ece34d5a9441eab582d6bb9765d47e", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "#text": "to increase awareness of the value of locally owned small businesses and the impact of locally\n\t\t\t owned small businesses on the economy of the United States."}, "enum": {"#tail": "\n          ", "#text": "(B)"}, "@id": "ideb6ad1ebf5b24732a1c80d15c0737ba3", "#text": "\n          "}]}], "@display-inline": "yes-display-inline", "#text": "\n      ", "@id": "S1"}, "#text": "\n\t\t"}, "@public-private": "public", "#text": "\n\t", "preamble": {"#tail": "\n\t", "whereas": [{"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas there are 28,200,000 small businesses in the United States;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas small businesses represent 99.7 percent of all businesses with employees in the United\n\t\t\t States;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas small businesses employ over 48 percent of the employees in the private sector in the\n\t\t\t United States;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas small businesses pay over 42 percent of the total payroll of the employees in the private\n\t\t\t sector in the United States;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas small businesses constitute 98 percent of firms exporting goods;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas small businesses are responsible for more than 46 percent of private sector output;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas small businesses generated 63 percent of net new jobs created from 1993 through 2013;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas 87 percent of consumers in the United States agree that the success of small businesses is\n\t\t\t critical to the overall economic health of the United States;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas 89 percent of consumers in the United States agree that small businesses contribute\n\t\t\t positively to local communities by supplying jobs and generating tax\n\t\t\t revenue;"}, "#text": "\n      "}, {"#tail": "\n    ", "text": {"#tail": "\n    ", "#text": "Whereas 93 percent of consumers in the United States agree that it is important to support the\n\t\t\t small businesses in their communities; and"}, "#text": "\n      "}, {"#tail": "\n  ", "text": {"#tail": "\n\t\t", "quote": {"#tail": ": Now, therefore, be it", "#text": "Small Business Saturday"}, "#text": "Whereas November 29, 2014,  is an appropriate day to designate as "}, "#text": "\n      "}], "#text": "\n\t\t"}, "metadata": {"#tail": "\n", "dublinCore": {"{http://purl.org/dc/elements/1.1/}date": {"#tail": "\n", "#text": "2014-09-16"}, "{http://purl.org/dc/elements/1.1/}format": {"#tail": "\n", "#text": "text/xml"}, "{http://purl.org/dc/elements/1.1/}language": {"#tail": "\n", "#text": "EN"}, "#tail": "\n", "{http://purl.org/dc/elements/1.1/}title": {"#tail": "\n", "#text": "113 SRES 548 ATS: Designating November 29, 2014, as \u201cSmall Business Saturday\u201d and supporting efforts to increase awareness of the value of locally owned small businesses."}, "{http://purl.org/dc/elements/1.1/}rights": {"#tail": "\n", "#text": "Pursuant to Title 17 Section 105 of the United States Code, this file is not subject to copyright protection and is in the public domain."}, "{http://purl.org/dc/elements/1.1/}publisher": {"#tail": "\n", "#text": "U.S. Senate"}, "#text": "\n"}, "#text": "\n"}}}