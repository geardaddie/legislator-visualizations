play-inline"}, "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n      ", "#text": "Short title"}, "#text": "\n      ", "@id": "id8100E76F004844D593BF3D628E3CB11C"}, {"#tail": "\n  ", "enum": {"#tail": "\n      ", "#text": "2."}, "header": {"#tail": "\n\t\t\t", "#text": "Prohibited\n\t\t\t activity"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", is amended\u2014", "@entity-type": "uscode", "#text": "\n            ", "external-xref": {"#tail": " of title 18, United States Code", "@parsable-cite": "usc/18/1030", "#text": "Section 1030(a)", "@legal-doc": "usc"}, "@value": "usc/18/1030/a"}, "#text": "\n          ", "@display-inline": "yes-display-inline"}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header": {"#tail": "\n        ", "#text": "In\n\t\t\t general"}, "paragraph": [{"#tail": "\n        ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": ", by inserting ", "@entity-type": "uscode", "#text": "paragraph (7)(C)", "@value": "usc/18/1030/a/7/C"}, "#text": "in ", "quote": {"#tail": " after the semicolon; and", "#text": "or"}}, "enum": {"#tail": "\n          ", "#text": "(1)"}, "@id": "id77066F11415B4370B6AB61F2116542E7", "#text": "\n          "}, {"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": {"#tail": " the following:", "@entity-type": "uscode", "#text": "paragraph (7)(C)", "@value": "usc/18/1030/a/7/C"}, "#text": "by inserting after "}, "enum": {"#tail": "\n          ", "#text": "(2)"}, "quoted-block": {"@style": "OLC", "after-quoted-block": {"#tail": "\n          ", "#text": "."}, "#tail": "\n\t\t\t\t", "paragraph": {"#tail": "\n            ", "text": {"#tail": "\n\t\t\t\t\t\t\t", "#text": "acting as an\n\t\t\t\temployer, knowingly and intentionally\u2014"}, "enum": {"#tail": "\n              ", "#text": "(8)"}, "#text": "\n              ", "@id": "id0