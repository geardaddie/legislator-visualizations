{"bill": {"#text": "\n\t", "form": {"current-chamber": {"#tail": "\n\t\t", "#text": "IN THE SENATE OF THE UNITED STATES"}, "#text": "\n\t\t", "congress": {"#tail": "\n\t\t", "#text": "113th CONGRESS"}, "#tail": "\n\t", "legis-num": {"#tail": "\n\t\t", "#text": "S. 1450"}, "official-title": {"#tail": "\n\t", "#text": "To amend the Internal Revenue Code of 1986 to impose an\n\t\t  ad valorem excise tax on certain passenger cruise voyages, and for other\n\t\t  purposes."}, "session": {"#tail": "\n\t\t", "#text": "1st Session"}, "distribution-code": {"#tail": "\n\t\t", "@display": "yes", "#text": "II"}, "action": {"action-date": {"#tail": "\n\t\t\t", "@date": "20130801", "#text": "August 1, 2013"}, "#tail": "\n\t\t", "action-desc": {"#tail": "\n\t\t", "#text": "\n        ", "sponsor": {"#tail": "\n\t\t\t introduced the following bill; which was read twice and referred to the\n\t\t\t ", "#text": "Mr. Rockefeller", "@name-id": "S176"}, "committee-name": {"#tail": "\n      ", "@committee-id": "SSFI00", "#text": "Committee on\n\t\t\t Finance"}}, "#text": "\n\t\t\t"}, "legis-type": {"#tail": "\n\t\t", "#text": "A BILL"}}, "@bill-stage": "Introduced-in-Senate", "@dms-id": "A1", "@public-private": "public", "legis-body": {"#tail": "\n", "section": [{"@section-type": "section-one", "#tail": "\n    ", "enum": {"#tail": "\n      ", "#text": "1."}, "header": {"#tail": "\n\t\t\t", "#text": "Excise tax on gross receipts\n\t\t\t derived from cruises"}, "subsection": [{"#tail": "\n      ", "text": {"#tail": "\n\t\t\t\t", "{http://namespaces.cato.org/catoxml}entity-ref": [{"#tail": " is amended by inserting after ", "@entity-type": "act", "#text": "Subchapter B of chapter 36 of the Internal Revenue Code\n\t\t\t of 1986", "@value": "Internal Revenue Code of 1986/ch:36/sch:B"}, {"#tail": " the following:", "@entity-type": "act", "#text": "section 4472", "@value": "Internal Revenue Code of 1986/s:4472"}], "#text": "\n          "}, "enum": {"#tail": "\n        ", "#text": "(a)"}, "header":